SELECT pl_barcode.codice, pl_barcode.data, pl_barcode.data_sosp, pl_barcode.data_chiuso, sr_utenti.nome, pl_barcode.note_1, pl_barcode.note_2, pl_barcode.path_file_pilota, pl_barcode.x_datins, pl_barcode.x_utente ,pl_barcode.stato ,pl_barcode.priorita ,pl_barcode.prima_del_barcode ,"Apri" as k_path_file_pilota,pilota_cmd.tipo ,pl_barcode.pilota_cmd_num_richFROM pl_barcode left outer join sr_utenti onpl_barcode.upd_utente_chiuso = sr_utenti.idleft outer join pilota_cmd on pl_barcode.pilota_cmd_num_rich = pilota_cmd.num_richWHERE pl_barcode.data >= '21-05-2018' AND ( 'tutti' = 'tutti' or (pl_barcode.stato = 'A' AND 'tutti' = 'aperti' ) OR ( pl_barcode.stato = 'C' AND 'tutti' = 'chiusi' ) OR ( pl_barcode.stato = 'S' AND 'tutti' = 'sospesi' ) )
