rag_soc_10 A,id_cliente A,data_scad D,cod_sl_pt A
0000000017
SELECT distinctsl_pt.cod_sl_pt as cod_sl_pt,COALESCE(clienti.rag_soc_10, '') as rag_soc_10,COALESCE(contratti.codice, 0) as contratti_codice, sl_pt.descr, sl_pt.fila_1 + '+' + sl_pt.fila_1p as fila_1, sl_pt.fila_2 + '+' + sl_pt.fila_2p as fila_2, sl_pt.densita, sl_pt.dose_min, sl_pt.dose_max, sl_pt.composizione, sl_pt.peso, sl_pt.routine, sl_pt.dosimetrie_spec,sl_pt.magazzino,sl_pt.dose,sl_pt.mis_x, sl_pt.mis_y, sl_pt.mis_z, COALESCE(contratti.mc_co, '') as mc_co,COALESCE(contratti.sc_cf, '') as sc_cf, contratti.data_scad,listino.attivo, case when trim(sl_pt.proposta_tipo_cicli) > ' ' then 1else 0 end as proposta,COALESCE(clienti.codice, 0) as id_cliente,COALESCE(sl_pt.id_cliente, 0) as id_cliente_sl_pt FROM (sl_pt left outer join contratti on sl_pt.cod_sl_pt = contratti.sl_pt) left outer join clienti on contratti.cod_cli = clienti.codiceleft outer join listino on contratti.codice = listino.contrattowhere sl_pt.cod_sl_pt > ' ' and (sl_pt.cod_sl_pt like '%'or 0 > 0 and (clienti.codice = 0or sl_pt.id_cliente = 0)or clienti.rag_soc_10 like '%')
