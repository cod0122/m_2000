SELECT trim(clienti.rag_soc_10) as rag_soc_10, COALESCE(listino.cod_cli, 0) as cod_cli, listino.cod_art, listino.id,trim(prodotti.des_mkt) as prodotti_des, COALESCE(prodotti.gruppo, 0) as prodotti_gruppo,COALESCE(listino.contratto, 0) as contratto, COALESCE(contratti.sl_pt, '') as sl_pt, COALESCE(listino.dose,0.00) as listino_dose, COALESCE(listino.magazzino, 0) as listino_magazzino, COALESCE(listino.mis_x, 0) as listino_mis_x, COALESCE(listino.mis_y, 0) as listino_mis_y, COALESCE(listino.mis_z, 0) as listino_mis_z, COALESCE(listino.occup_ped, 0) as listino_occup_ped, listino.peso_kg, COALESCE(listino.campione, '') as listino_campione, COALESCE(listino.tipo, '') as listino_tipo, COALESCE(listino.prezzo, 0) as prezzo, COALESCE(listino.attivo, 'N') as listino_attivo,COALESCE(contratti.mc_co, '') as mc_co,COALESCE(contratti.sc_cf, '') as sc_cf,contratti.data_scad,0 as k_pwd,listino.x_datins,gru.des,COALESCE(listino.prezzo_2, 0.00) as prezzo_2, COALESCE(listino.prezzo_3, 0.00) as prezzo_3 ,COALESCE(listino.attiva_listino_pregruppi, '') as attiva_listino_pregruppi,COALESCE(listino.e1litm, '') as e1litmFROM (((listino inner JOIN clienti ON listino.cod_cli = clienti.codice) LEFT OUTER JOIN contratti ON listino.contratto = contratti.codice) INNER JOIN prodotti ON listino.cod_art = prodotti.codice) inner join gru on prodotti.gruppo = gru.codicewhere (0 = 0 or listino.cod_cli = 0) and(0 = 0 or listino.contratto = 0) and (contratti.data_scad >= '04-06-2018' or contratti.data_scad is null) order by clienti.rag_soc_10, listino.attivo desc, listino.cod_art
