SELECT clienti.rag_soc_10,contratti.codice,contratti.tipo,contratti.mc_co, contratti.data, contratti.data_scad, contratti.cod_cli, contratti.sc_cf, contratti.sl_pt, contratti.descr, sl_pt.cert_st_dose_min, sl_pt.cert_st_dose_max, sl_pt.cert_st_data_ini, sl_pt.cert_st_data_fin, 0 as anno_contratto ,contratti.x_datins, trim(COALESCE(et_bcode_note,' ')) as et_bcode_note, COALESCE(contratti.id_meca_causale, 0) as id_meca_causalefrom contratti left outer join clienti on contratti.cod_cli = clienti.codice left outer join sl_pt oncontratti.sl_pt = sl_pt.cod_sl_ptwhere (37 = 0 or contratti.cod_cli = 37) and ('*' = '*' or contratti.mc_co like '*')and contratti.data_scad > '01-01-1900'and (0 = 0 or contratti.codice = 0)order by anno_contratto desc, clienti.rag_soc_10, contratti.mc_co
