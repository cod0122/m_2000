SELECT distinct sv_eventi_sked.id, COALESCE(trim(sv_eventi_sked.id_menu_window), '') + COALESCE(trim(sv_eventi_sked.cmd_dos),'') + COALESCE('  (' + trim(menu_window_oggetti.nome_oggetto) + ') ','') as id_menu_window, menu_window.descr as menu_window_titolo, sv_eventi_sked.run_giorno, sv_eventi_sked.run_ora, sv_eventi_sked.stato, sv_eventi_sked.esito, sv_eventi_sked.id_sv_skedula ,sv_eventi_sked.run_giorno_start ,sv_eventi_sked.run_giorno_stop ,sv_eventi_sked.x_datins ,sv_eventi_sked.x_utente FROM sv_eventi_sked LEFT OUTER JOIN sv_skedula ON sv_eventi_sked.id_sv_skedula = sv_skedula.id LEFT OUTER JOIN menu_window ON sv_eventi_sked.id_menu_window = menu_window.id LEFT OUTER JOIN menu_window_oggetti ON sv_eventi_sked.id_menu_window = menu_window_oggetti.id_menu_window ORDER BY sv_eventi_sked.run_giorno ASC, sv_eventi_sked.run_ora ASC
