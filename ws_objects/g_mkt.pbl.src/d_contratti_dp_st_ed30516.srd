 retrieve=" SELECT contratti_dp.id_contratto_dp,   
         contratti_dp.anno,   
         contratti_dp.offerta_data,   
         contratti_dp.offerta_validita,
		contratti_dp.stampa_tradotta,   
         contratti_dp.oggetto,   
         contratti_dp.id_cliente,   
         trim(clienti.rag_soc_10) as rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         clienti.id_nazione_1,   
         trim(contratti_dp.id_clie_settore) as id_clie_settore ,   
         trim(trim(clie_settori.descr) + ' - ' + trim(gru.des)) as gru_des,   
         contratti_dp.gruppo,   
         contratti_dp.art,
         trim(prodotti.des_mkt) as prodotti_des_mkt,   
         trim(contratti_dp.nome_contatto) as nome_contatto,   
         trim(contratti_dp.note) as note,   
         contratti_dp.mis_x_1,   
         contratti_dp.mis_y_1,   
         contratti_dp.mis_z_1,   
         contratti_dp.peso_max_kg,   
    contratti_dp.qta_anno,   
    contratti_dp.previsione_area,
    contratti_dp.previsione_qta,
    contratti_dp.sovrapponibili,
      contratti_dp.unita_misura,
	contratti_dp.consegna_des,
         contratti_dp.iva,   
         iva.aliq,   
         iva.des,   
         contratti_dp.cod_pag,   
         pagam.des,   
         trim(contratti_dp.banca) as banca,   
         contratti_dp.abi,   
         contratti_dp.cab,   
         trim(contratti_dp.altre_condizioni) as altre_condizioni,   
         contratti_dp.data_inizio,   
         contratti_dp.data_fine,   
         contratti_dp.fattura_da,   
         contratti_dp.x_datins,   
         contratti_dp.x_utente,   
         contratti_dp.stato,   
         contratti_dp.data_stampa,
        '- D010108.H12:00.U0011' as k_marca_stampa  
         , contratti_dp.descr_1 + ': '       as v1_descr_xctr 
         , contratti_dp.descr_1 + ': ' as v1_descr_xctr_eng
         , contratti_dp.descr_2 + ': '        as v2_descr_xctr 
         , contratti_dp.descr_2 + ': ' as v2_descr_xctr_eng
         , contratti_dp.descr_3 + ': '        as v3_descr_xctr 
         , contratti_dp.descr_3 + ': ' as v3_descr_xctr_eng
         , contratti_dp.descr_4 + ': '        as v4_descr_xctr 
         , contratti_dp.descr_4 + ': ' as v4_descr_xctr_eng
         , contratti_dp.descr_5 + ': '        as v5_descr_xctr 
         , contratti_dp.descr_5 + ': ' as v5_descr_xctr_eng
         , contratti_dp.descr_6 + ': '        as v6_descr_xctr 
         , contratti_dp.descr_6 + ': ' as v6_descr_xctr_eng
         , contratti_dp.descr_7 + ': '        as v7_descr_xctr 
         , contratti_dp.descr_7 + ': ' as v7_descr_xctr_eng
         , contratti_dp.descr_8 + ': '        as v8_descr_xctr 
         , contratti_dp.descr_8 + ': ' as v8_descr_xctr_eng
		, contratti_dp.voce_prezzo_1 
		, contratti_dp.voce_prezzo_2 
		, contratti_dp.voce_prezzo_3 
		, contratti_dp.voce_prezzo_4 
		, contratti_dp.voce_prezzo_5
		, contratti_dp.voce_prezzo_6 
		, contratti_dp.voce_prezzo_7 
		, contratti_dp.voce_prezzo_8 
        , contratti_dp.qta_max_disponibile   
        ,'1' as k_ragsoc_base_1
        ,'2' as k_ragsoc_base_2
    FROM contratti_dp LEFT OUTER JOIN clie_settori ON contratti_dp.id_clie_settore = clie_settori.id_clie_settore 
                               LEFT OUTER JOIN clienti ON contratti_dp.id_cliente = clienti.codice 
                               LEFT OUTER JOIN gru ON contratti_dp.gruppo = gru.codice 
						    LEFT OUTER JOIN prodotti on contratti_dp.art = prodotti.codice 
                               LEFT OUTER JOIN pagam ON contratti_dp.cod_pag = pagam.codice 
                               LEFT OUTER JOIN iva ON contratti_dp.iva = iva.codice  
--                               LEFT OUTER JOIN listino_voci as v1 ON contratti_dp.id_listino_voce_1 = v1.id_listino_voce
--                               LEFT OUTER JOIN listino_voci as v2 ON contratti_dp.id_listino_voce_2 = v2.id_listino_voce
--                               LEFT OUTER JOIN listino_voci as v3 ON contratti_dp.id_listino_voce_3 = v3.id_listino_voce
--                               LEFT OUTER JOIN listino_voci as v4 ON contratti_dp.id_listino_voce_4 = v4.id_listino_voce
--                               LEFT OUTER JOIN listino_voci as v5 ON contratti_dp.id_listino_voce_5 = v5.id_listino_voce
--                               LEFT OUTER JOIN listino_voci as v6 ON contratti_dp.id_listino_voce_6 = v6.id_listino_voce
--                               LEFT OUTER JOIN listino_voci as v7 ON contratti_dp.id_listino_voce_7 = v7.id_listino_voce
--                               LEFT OUTER JOIN listino_voci as v8 ON contratti_dp.id_listino_voce_8 = v8.id_listino_voce
   WHERE contratti_dp.id_contratto_dp = :k_id_contratto_dp    
" arguments=(("k_id_contratto_dp", number)) )
