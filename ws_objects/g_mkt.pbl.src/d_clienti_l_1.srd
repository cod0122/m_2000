 retrieve="  SELECT DISTINCT clienti.rag_soc_10 as rag_soc_1,   
         clienti.codice as id_cliente,   
         clienti.loc_1 as localita,   
         clienti.prov_1 as prov,   
         clienti.indi_1 as indirizzo,   
         clienti.cap_1 as cap,   
         clienti.fono as tel_num,   
         clienti.fax as fax_num,   
         clienti.p_iva,   
         clienti.banca  
         ,clienti.iva
         ,clienti.iva_valida_dal
         ,clienti.iva_valida_al
         ,clienti.cod_pag  
         ,clienti.x_datins  
         ,clienti.stato  
         ,COALESCE(clienti.id_meca_causale,0) as id_meca_causale
         ,  clienti.rag_soc_11
         ,  pagam.des  as pagam_des 
         ,  clienti.rag_soc_20 as rag_soc_2 
         ,  clienti.rag_soc_21
         ,  COALESCE(clienti.cf, ' ') as cf
         ,  clienti.abi  as abi 
         ,  clienti.cab  as cab 
         ,  clienti.id_nazione_1  as id_nazione 
         ,  iva.des  as iva_des
         ,  meca_causali.descrizione
		 , CASE
 			  	WHEN clienti_memo.id_cliente > 0  THEN 1
   				ELSE 0
   			END as allegato
        FROM clienti  
               inner join vx_MAST2_clienti_l as clienti_l on 
                 clienti.codice = clienti_l.id_cliente
              LEFT OUTER JOIN pagam  ON 
				     clienti.cod_pag = pagam.codice
				  LEFT OUTER JOIN iva ON 
                    clienti.iva = iva.codice
				  LEFT OUTER JOIN meca_causali ON 
			         clienti.id_meca_causale = meca_causali.id_meca_causale 
         		  LEFT OUTER JOIN clienti_memo 
             ON clienti.codice = clienti_memo.id_cliente 
        ORDER BY rag_soc_1          ASC  "  )
