 retrieve=" SELECT ctr.id_contratto_doc as id_contratto_doc,   
         ctr.anno as anno,   
		ctr.stampa_tradotta as stampa_tradotta,   
         ctr.offerta_data as offerta_data,   
         ctr.offerta_validita as offerta_validita,   
         ctr.oggetto as oggetto,   
         ctr.id_cliente,   
         (trim(clienti.rag_soc_10) + ' ' + (case when trim(clienti.rag_soc_11) > ' ' then trim(clienti.rag_soc_11) else '' end)) as rag_soc_10,   
         trim(clienti.indi_1) + ' - ' + trim(clienti.cap_1) + ' ' + trim(clienti.loc_1)  + ' - ' +  clienti.prov_1 + ' - ' + clienti.id_nazione_1 indi_1,   
         trim(ctr.nome_contatto) as nome_contatto,   
         trim(COALESCE(ctr.note,'')) as note,   
         trim(ctr.note_audit) as note_audit,   
		 trim(ctr.note_fasi_operative) as note_fasi_operative,
         ctr.cod_pag,   
         pagam.des as pagam_des,   
         trim(ctr.altre_condizioni) as altre_condizioni,   
         ctr.data_inizio as data_inizio,   
         ctr.data_fine as data_fine, 
         ctr.fattura_da,   
         ctr.x_datins,   
         ctr.x_utente,   
         ctr.stato,   
         ctr.data_stampa as data_stampa, 
        '- D010108.H12:00.U0011' as k_marca_stampa  
         , COALESCE(ctr.totale_contratto,0) as totale_contratto
        ,'1' as k_ragsoc_base_1
        ,'2' as k_ragsoc_base_2
        , ctr.quotazione_cod
	    , ctr.quotazione_tipo
		, ctr.cliente_desprod
		, ctr.cliente_desprod_rid
         ,concat(cast(format(COALESCE(ctr.prezzo_1, 0.00), 'N2') as varchar), ' €/', trim(COALESCE(ctr.unita_misura, '')))  prezzo_irr
		FROM v_contratti_doc ctr 
		LEFT OUTER JOIN clienti ON ctr.id_cliente = clienti.codice
		LEFT OUTER JOIN pagam ON ctr.cod_pag = pagam.codice
   WHERE ctr.id_contratto_doc = :k_id_contratto_doc    
" arguments=(("k_id_contratto_doc", number)) )
compute(band=detail alignment="0" expression="if ( quotazione_tipo = 'VAL' ,
