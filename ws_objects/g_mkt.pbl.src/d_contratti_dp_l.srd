 retrieve="  SELECT contratti_dp.id_contratto_dp,   
         contratti_dp.anno,   
         contratti_dp.offerta_data,   
         contratti_dp.id_docprod,   
         contratti_dp.id_cliente,   
         clienti.rag_soc_10,   
         clienti.id_clie_classe,   
         contratti_dp.stato,   
         contratti_dp.data_stampa,   
         contratti_dp.offerta_validita,   
         contratti_dp.oggetto,   
         contratti_dp.data_inizio,   
         contratti_dp.data_fine,   
         contratti_dp.id_clie_settore,   
         clie_settori.descr,   
         contratti_dp.gruppo,   
         gru.des,   
         contratti_dp.nome_contatto,   
         contratti_dp.x_datins,   
         contratti_dp.x_utente,  
         contratti_dp.mis_x_1,   
         contratti_dp.mis_y_1,    
         contratti_dp.mis_z_1,
         contratti_dp.attivo,
         0 as knr_contratti
        ,max(clienti_memo.id_cliente_memo) as id_cliente_memo
       , ' ' p_memo_x
    FROM contratti_dp LEFT OUTER JOIN clie_settori ON contratti_dp.id_clie_settore = clie_settori.id_clie_settore 
							LEFT OUTER JOIN clienti ON contratti_dp.id_cliente = clienti.codice 
							LEFT OUTER JOIN gru ON contratti_dp.gruppo = gru.codice 
                               left outer join clienti_memo on clienti.codice = clienti_memo.id_cliente and clienti_memo.tipo_sv = ~"CDP~" 
  WHERE (:k_id_cliente = 0 or contratti_dp.id_cliente = :k_id_cliente)
         	and data_inizio between :k_data_ini and :k_data_fin    
              and (:k_attivo = '*' or attivo = :k_attivo)
group by contratti_dp.id_contratto_dp,
         contratti_dp.anno,   
         contratti_dp.offerta_data,   
         contratti_dp.id_docprod,   
         contratti_dp.id_cliente,   
         clienti.rag_soc_10,   
         clienti.id_clie_classe,   
         contratti_dp.stato,   
         contratti_dp.data_stampa,   
         contratti_dp.offerta_validita,   
         contratti_dp.oggetto,   
         contratti_dp.data_inizio,   
         contratti_dp.data_fine,   
         contratti_dp.id_clie_settore,   
         clie_settori.descr,   
         contratti_dp.gruppo,   
         gru.des,   
         contratti_dp.nome_contatto,   
         contratti_dp.x_datins,   
         contratti_dp.x_utente,  
         contratti_dp.mis_x_1,   
         contratti_dp.mis_y_1,    
         contratti_dp.mis_z_1,
         contratti_dp.attivo" arguments=(("k_data_ini", date),("k_data_fin", date),("k_id_cliente", number),("k_attivo", string))  sort="offerta_data D id_contratto_dp D " )
