 retrieve="  SELECT   clienti.codice as id_cliente ,
			 clienti.tipo as tipo,
         	case    
				when clienti.tipo = 'C' then 'Contatto'
				when clienti.tipo = 'M' then 'Movimenta magazzino'
				when clienti.tipo = 'A' then 'altro'
				else  ' '
			end  as tipo_descr,
			clienti.stato,
         	case    
				when clienti.stato = '2' then 'da Contattare'
				when clienti.stato = '3' then 'in Contatto'
				when clienti.stato = '5' then 'Attivo Parziale'
				when clienti.stato = '6' then 'Attivo'
				when clienti.stato = '7' then 'Sospeso'
				when clienti.stato = '8' then 'Estinto'
				else  '*non definito* '
			end  as stato_descr,
			COALESCE(clienti.data_attivazione, convert(date,'01.01.2000')) as data_attivazione,
			clienti.id_clie_settore as id_clie_settore,
		clie_settori.descr as settore_descrizione,
			clienti.id_clie_classe as id_clie_classe,
		clie_classi.descr as classe_descr,
             trim(clienti.rag_soc_10) + ' ' + COALESCE(trim(clienti.rag_soc_11), ~"~") as  sede_rag_soc,
             trim(clienti.indi_1) as sede_indi,
           clienti.cap_1 as sede_cap,
             trim(clienti.loc_1) as sede_loc,
           clienti.prov_1 as sede_prov,
           clienti.zona as sede_zona,
           clienti.id_nazione_1 as sede_nazione,
           nazioni.nome as nazione_nome,
           nazioni.area as nazione_area,
           clienti.p_iva as partitaiva,
           clienti.cf as codicefiscale,
             clienti.fono as  telefono,
             clienti.fax as    fax, 
           clienti.cod_pag as codice_pagamento, 
			pagam.des as pagamento_descr,
           clienti.banca as banca, 
           clienti.abi as abi, 
           clienti.cab as cab, 
           clienti.iva as esente_iva , 
			iva.des as esente_iva_descr,
           clienti.iva_valida_dal as esente_dal, 
           clienti.iva_valida_al as esente_al, 
           clienti.iva_esente_imp_max as esente_fino_importo, 
           clienti.iva_esente_imp_min_x_fatt as esente_importo_minimo, 
           clienti.mese_es_1 as escludi_scadenza_mese_1, 
           clienti.mese_es_2  as escludi_scadenza_mese_2, 
           trim(clienti.cadenza_fattura) as cadenza_fatturazione,
             trim(clienti.rag_soc_20) + ' ' + COALESCE(trim(clienti.rag_soc_21), ~"~") as  ddt_rag_soc,
            trim(clienti.indi_2) as       ddt_indi,
           clienti.cap_2  as ddt_cap,
             trim(clienti.loc_2) as ddt_loc,
           clienti.prov_2 as ddt_prov,
           clienti.id_nazione_2 as ddt_nazione,
           nazioni1.nome as ddt_nazione_nome,
           nazioni1.area as ddt_nazione_area,
             trim(ind_comm.rag_soc_1_c) + ' ' + COALESCE(trim(ind_comm.rag_soc_2_c), ~"~") as  fatt_rag_soc,
             trim(ind_comm.indi_c) as       fatt_indi,
            ind_comm.cap_c as fatt_cap,
             trim(ind_comm.loc_c) as  fatt_loc,
            ind_comm.prov_c as fatt_prov,
           ind_comm.id_nazione_c as fatt_nazione,
           nazioni2.nome as fatt_nazione_nome,
           nazioni2.area as fatt_nazione_area
           ,clienti_fatt.fattura_da
         	,case    
				when clienti_fatt.fattura_da = 'B' then 'fattura da DDT di spedizione'
				when clienti_fatt.fattura_da = 'C' then 'fattura da LOTTO con Attestato emesso'
				else  ' '
			end  as fattura_da_descr
           ,clienti_fatt.note_1 + ' ' + COALESCE(clienti_fatt.note_2, ~"~") as fatt_note
           ,clienti_fatt.modo_stampa as fatt_modo_stampa
         	,case    
				when clienti_fatt.modo_stampa = 'S' then 'solo stampa Cartacea'
				when clienti_fatt.modo_stampa = 'D' then 'emissione solo Digitale (pdf)'
				when clienti_fatt.modo_stampa = 'E' then 'stampa Cartacea e Digitale'
				else  ' '
			end  as fatt_modo_stampa_descr
           ,clienti_fatt.modo_email as fatt_modo_email
         	,case    
				when clienti_fatt.modo_email = 'A' then 'invia Solo Avviso'
				when clienti_fatt.modo_email = 'F' then 'invia con allegato Fattura'
				when clienti_fatt.modo_email = 'N' then 'nessun invio'
				else  ' '
			end  as fatt_modo_email_descr
           ,clienti_fatt.email_invio as fatt_email_invio
         	,case    
				when clienti_fatt.email_invio = '1' then 'invia al I indirizzo email'
				when clienti_fatt.email_invio = '2' then 'invia al II indirizzo email'
				when clienti_fatt.email_invio = '3' then 'invia al III indirizzo email'
				else  ' '
			end  as fatt_email_invio_descr
           ,clienti.id_meca_causale as causale_entrata_merce
		,meca_causali.descrizione as causale_descrizione_entrata_merce  
         ,clienti_mkt.id_cliente_link as id_cliente_link
		,clienti_mkt.qualifica as qualifica,   
         clienti_mkt.tipo_rapporto ,   
         clienti_mkt.altra_sede,   
         clienti_mkt.cod_atecori,   
         clienti_mkt.note_attivita,   
         clienti_mkt.note_prodotti,   
         clienti_mkt.gruppo as cod_gruppo_prevalente,   
         clienti_web.email as email_1,   
         clienti_web.email1 as email_2,     
         clienti_web.email2  as email_3,     
         clienti_web.note as note_web,   
         clienti_web.sito_web as sito_web_1,   
         clienti_web.sito_web as sito_web_2,     
         clienti_web.blog_web as sito_blog_1,      
         clienti_web.blog_web1 as sito_blog_2,    
         COALESCE(clienti_fatt.codice_ipa, '') as codice_ipa,   
         COALESCE(clienti_fatt.impon_minimo , 0) as impon_minimo,
         COALESCE(clienti.e1an , 0) as e1an,
         COALESCE(clienti.e1ancodrs , '') as e1ancodrs
        FROM clienti  
							  inner join #vx_MAST2_clienti_l as clienti_l on clienti.codice = clienti_l.id_cliente
							  left outer join  ind_comm on clienti.codice = ind_comm.clie_c
							  left outer join nazioni on   clienti.id_nazione_1 = nazioni.id_nazione     
							  left outer join nazioni as nazioni1 on   clienti.id_nazione_2 = nazioni1.id_nazione 
							  left outer join nazioni as nazioni2 on   ind_comm.id_nazione_c = nazioni2.id_nazione    
							  left outer join pagam on   clienti.cod_pag = pagam.codice 
							  left outer join iva on   clienti.iva = iva.codice 
							  left outer join clienti_fatt on  clienti.codice = clienti_fatt.id_cliente 
							  left outer join clie_settori on   clienti.id_clie_settore = clie_settori.id_clie_settore 
							  left outer join clie_classi on   clienti.id_clie_classe = clie_classi.id_clie_classe 
         				  left outer join meca_causali on clienti.id_meca_causale = meca_causali.id_meca_causale
         				  left outer join clienti_mkt on clienti.codice = clienti_mkt.id_cliente
         				  left outer join clienti_web on clienti.codice = clienti_web.id_cliente
         order by 1 " )
