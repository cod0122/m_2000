 retrieve="  SELECT 
         COALESCE(trim(clienti.rag_soc_10),'') as rag_soc_1,   
		  clienti.codice as id_cliente ,
           COALESCE(trim(clienti.loc_1), '') as localita ,
           COALESCE(clienti.prov_1,'') as prov ,
           COALESCE(clienti.indi_1,'') as indirizzo ,
           clienti.cap_1 as cap ,
           COALESCE(clienti.id_nazione_1,'')  as id_nazione_1,
           COALESCE(clienti.fono,'') as tel_num , 
           COALESCE(clienti.fax,'') as fax_num ,
           clienti.banca
           ,clienti.stato
		  ,COALESCE(clienti.e1an, 0) as e1an
		  ,COALESCE(trim(clienti.e1ancodrs), '') as e1ancodrs
        FROM clienti      
        WHERE 	(:k_rag_soc = '%' or clienti.rag_soc_10 like :k_rag_soc )  
        ORDER BY 1       ASC " retrieve.asneeded=yes arguments=(("k_rag_soc", string)) )
