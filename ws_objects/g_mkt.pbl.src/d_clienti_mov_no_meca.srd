 retrieve="  SELECT v_arfa.tipo_doc,   
         v_arfa.data_fatt,   
         v_arfa.num_fatt,   
         v_arfa.art,   
         v_arfa.colli_out,   
         v_arfa.prezzo_u,   
         v_arfa.prezzo_t,   
         v_arfa.prodotti_des,   
         v_arfa.gruppo,   
         v_arfa.campione  
    FROM v_arfa   
  where clie_3 = :k_cliente
         		and v_arfa.data_fatt between :k_data_ini and :k_data_fin
				and id_armo = 0
  order by 
         v_arfa.data_fatt,   
         v_arfa.num_fatt
" arguments=(("k_cliente", number),("k_data_ini", date),("k_data_fin", date)) )
