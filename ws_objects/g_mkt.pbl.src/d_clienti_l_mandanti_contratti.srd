 retrieve="  SELECT distinct
           clienti.rag_soc_10 as rag_soc_1 ,
			  clienti.codice as id_cliente ,
           clienti.loc_1 as localita ,
           clienti.prov_1 as prov ,
           clienti.indi_1 as indirizzo ,
           clienti.cap_1 as cap ,
           clienti.fono as tel_num , 
           clienti.fax as fax_num 
        FROM clienti 
          where  codice in (
              select distinct  m_r_f.clie_1 
                   from m_r_f inner join listino on m_r_f.clie_3 = listino.cod_cli and listino.attivo = 'S'
                            inner join contratti on listino.contratto = contratti.codice and contratti.data_scad > :k_data_scad
                               )
        		and	(:k_rag_soc = '%' or clienti.rag_soc_10 like :k_rag_soc ) 
        ORDER BY clienti.rag_soc_10       ASC  " retrieve.asneeded=yes arguments=(("k_rag_soc", string),("k_data_scad", date)) )
