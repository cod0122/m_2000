 retrieve="  SELECT  clienti.codice as id_cliente,   
         clienti.rag_soc_10 as rag_soc_10,   
         clienti.loc_1 as localita,   
         clienti.prov_1 as prov
         ,COALESCE(clienti.id_nazione_1,'') as id_nazione_1
         ,trim(COALESCE(clienti.id_clie_classe,'')) as id_clie_classe
         ,COALESCE(nazioni.nome,'') as nazioni_nome
		,COALESCE(fono,'') as fono
		,COALESCE(fax,'') as fax
        ,COALESCE((select max(clienti_memo.id_cliente_memo) from clienti_memo where clienti.codice = clienti_memo.id_cliente),0) as id_cliente_memo
		,COALESCE(clienti_mkt.gruppo,0) as gruppo 
         ,COALESCE(gru.des,'') as gruppo_descrizione
		,COALESCE(province.regione,'') as regione
        ,COALESCE(clienti_web.email,'') as web_email
		,clienti.data_attivazione
      ,clienti.x_utente
      ,clienti.x_datins
      ,COALESCE(clienti.e1an,0) e1an
      ,COALESCE(clienti.e1ancodrs,'') e1ancodrs
    FROM clienti    
               left outer join nazioni on clienti.id_nazione_1 = nazioni.id_nazione
               left outer join clienti_mkt on clienti.codice = clienti_mkt.id_cliente 
               left outer join province on clienti.prov_1 = province.sigla
               left outer join gru on clienti_mkt.gruppo = gru.codice
               left outer join clienti_web  on clienti_web.id_cliente = clienti.codice
   WHERE (:k_tipo = '%' or clienti.tipo = :k_tipo)
              and (:k_stato = '%' or (clienti.stato = :k_stato and clienti.tipo <> 'C'))
ORDER BY clienti.rag_soc_10 ASC    " arguments=(("k_tipo", string),("k_stato", string)) )
