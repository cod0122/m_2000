 column=(type=char(30) updatewhereclause=yes name=localita dbname="clienti.localita" dbalias=".localita" )
 column=(type=char(2) updatewhereclause=yes name=prov dbname="clienti.prov" dbalias=".prov" )
 column=(type=char(3) updatewhereclause=yes name=id_nazione_1 dbname="id_nazione_1" )
 column=(type=char(8) updatewhereclause=yes name=id_clie_classe dbname="id_clie_classe" )
