 retrieve="  SELECT v_contratti_doc.id_contratto_doc,   
         v_contratti_doc.anno,   
         v_contratti_doc.offerta_data,   
         v_contratti_doc.id_cliente,   
         trim(clienti.rag_soc_10) rag_soc_10,   
         v_contratti_doc.stato,   
         v_contratti_doc.data_stampa,   
         v_contratti_doc.offerta_validita,   
         v_contratti_doc.oggetto,   
         v_contratti_doc.data_inizio,   
         v_contratti_doc.data_fine,   
         v_contratti_doc.id_clie_settore,   
          trim(clie_settori.descr) clie_settori_descr,   
         0 as sel
		,v_contratti_doc.quotazione_tipo
		,v_contratti_doc.quotazione_cod
		 , v_contratti_doc.cliente_desprod as cliente_desprod
		 , v_contratti_doc.cliente_desprod_rid as cliente_desprod_rid
    FROM v_contratti_doc LEFT OUTER JOIN clie_settori ON v_contratti_doc.id_clie_settore = clie_settori.id_clie_settore 
							LEFT OUTER JOIN clienti ON v_contratti_doc.id_cliente = clienti.codice 
  WHERE (:k_id_cliente = 0 or v_contratti_doc.id_cliente = :k_id_cliente)
			and v_contratti_doc.offerta_data >= :k_data 
              and v_contratti_doc.stato = '5'
" arguments=(("k_data", date),("k_id_cliente", number))  sort="offerta_data D id_contratto_doc D " )
