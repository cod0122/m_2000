 retrieve=" SELECT ctr.id_contratto_doc,   
         ctr.anno,   
		ctr.stampa_tradotta,   
         ctr.offerta_data,   
         ctr.offerta_validita,   
         ctr.id_cliente,   
         trim(clienti.rag_soc_10) as rag_soc_10,   
         trim(clienti.indi_1) + ' - ' + trim(clienti.cap_1) + ' ' + trim(clienti.loc_1)  + ' - ' +  clienti.prov_1 + ' - ' + clienti.id_nazione_1 indi_1,   
         ctr.data_stampa , 
         ctr.quotazione_cod
	    , ctr.quotazione_tipo
		, ctr.cliente_desprod
		, ctr.cliente_desprod_rid
		, (case when trim(ctr.venditore_nome) <> '.' then trim(ctr.venditore_nome) else ' ' end) venditore_nome
		, trim(ctr.venditore_ruolo) venditore_ruolo
		FROM v_contratti_doc ctr
		LEFT OUTER JOIN clienti ON ctr.id_cliente = clienti.codice
   WHERE ctr.id_contratto_doc = :k_id_contratto_doc    
" arguments=(("k_id_contratto_doc", number)) )
