 retrieve=" SELECT ctr.id_contratto_doc,   
         ctr.anno,   
         ctr.magazzino,   
         ctr.offerta_data,   
         ctr.offerta_validita,   
         ctr.oggetto,   
         ctr.id_cliente,   
         rtrim(clienti.rag_soc_10) as rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         clienti.id_nazione_1,   
         ctr.id_clie_settore,   
         clie_settori.descr,   
         ctr.gruppo,   
         gru.des,   
         (ctr.nome_contatto) as nome_contatto,   
         (ctr.note) as note,   
         (ctr.note_audit) as note_audit,   
         (ctr.note_fasi_operative) as note_fasi_operative,
         ctr.iva,   
         iva.aliq,   
         iva.des,   
         ctr.cod_pag,   
         pagam.des,   
         rtrim(ctr.banca) as banca,   
         ctr.abi,   
         ctr.cab,   
         (ctr.altre_condizioni) as altre_condizioni,   
         ctr.data_inizio,   
         ctr.data_fine,   
         ctr.fattura_da,   
         ctr.x_datins,   
         ctr.x_utente,   
         ctr.stato,   
         ctr.data_stampa,  
         ctr.stampa_tradotta,  
         ctr.art,
         prodotti.des_mkt,
         ctr.esito_operazioni_ts_operazione
	, flg_fatt_dopo_valid as flg_fatt_dopo_valid
	, ctr.id_meca_causale as id_meca_causale
	, meca_causali.descrizione as meca_causali_descrizione 
	, ctr.acconto_perc as acconto_perc
	, ctr.acconto_imp as acconto_imp
	, ctr.acconto_cod_pag as acconto_cod_pag
	, acc_pagam.des as acconto_pagam_des
	, ctr.stato as stato_1  
    , ctr.quotazione_tipo as quotazione_tipo
    , ctr.quotazione_cod as quotazione_cod
    , ctr.cliente_desprod as cliente_desprod
    , ctr.cliente_desprod_rid as cliente_desprod_rid
	, ctr.stato as stato_2  
	, ctr.stato as stato_3  
	, ctr.stato as stato_4  
	, ctr.stato as stato_5  
	, ctr.note_interne
	, ctr.gest_doc_prezzo   
	, ctr.dir_tecnico_prezzo   
	, ctr.analisi_lab_prezzo   
	, ctr.dosim_agg_prezzo   
	, ctr.stoccaggio_prezzo   
	, ctr.logistica_prezzo   
	, ctr.altro_prezzo
	, trim(ctr.gest_doc_des) as gest_doc_des
	, trim(ctr.dir_tecnico_des) as dir_tecnico_des
	, trim(ctr.analisi_lab_des) as analisi_lab_des   
	, trim(ctr.dosim_agg_des) as dosim_agg_des
	, trim(ctr.stoccaggio_des) as stoccaggio_des
	, trim(ctr.logistica_des) as logistica_des  
	, trim(ctr.altro_des) as altro_des
  , (select  max(coalesce(clienti_memo.id_cliente,0)) from  clienti_memo where ctr.id_cliente = clienti_memo.id_cliente) as id_memo
FROM v_contratti_doc as ctr LEFT OUTER JOIN clie_settori ON ctr.id_clie_settore = clie_settori.id_clie_settore 
                   LEFT OUTER JOIN clienti ON ctr.id_cliente = clienti.codice
                   LEFT OUTER JOIN gru ON ctr.gruppo = gru.codice 
                   LEFT OUTER JOIN pagam ON ctr.cod_pag = pagam.codice
                   LEFT OUTER JOIN pagam as acc_pagam ON ctr.acconto_cod_pag = acc_pagam.codice
                   LEFT OUTER JOIN iva ON ctr.iva = iva.codice  
                   LEFT OUTER JOIN prodotti ON ctr.art = prodotti.codice  
				 LEFT OUTER JOIN meca_causali on  ctr.id_meca_causale = meca_causali.id_meca_causale
   WHERE ctr.id_contratto_doc = :k_id_contratto_doc" arguments=(("k_id_contratto_doc", number)) )
