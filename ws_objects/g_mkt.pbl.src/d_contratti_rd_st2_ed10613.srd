 retrieve=" SELECT contratti_rd.id_contratto_rd,   
         contratti_rd.anno,   
		contratti_rd.stampa_tradotta,   
         contratti_rd.offerta_data,   
         contratti_rd.offerta_validita,   
         contratti_rd.oggetto,   
         contratti_rd.id_cliente,   
         trim(clienti.rag_soc_10) as rag_soc_10,   
         trim(clienti.indi_1) as indi_1,   
         clienti.loc_1,   
         clienti.prov_1,   
         clienti.id_nazione_1,   
         contratti_rd.id_clie_settore,   
         clie_settori.descr,   
         clie_settori.descr_eng,   
         contratti_rd.gruppo,   
         gru.des,   
         gru.des_eng,   
         trim(contratti_rd.nome_contatto) as nome_contatto,   
         COALESCE(trim(contratti_rd.note),'') + ' ' + COALESCE(trim(contratti_rd.note_audit), '') as note,   
         trim(contratti_rd.note_audit) as note_audit,   
		trim(contratti_rd.note_fasi_operative) as note_fasi_operative,
         contratti_rd.voce_prezzo_1 as fase_b1,   
         contratti_rd.voce_prezzo_2 as fase_b2,   
         contratti_rd.voce_prezzo_3 as fase_b3,   
         contratti_rd.voce_prezzo_4 as fase_b4,   
         contratti_rd.voce_prezzo_5 as fase_b5,   
         contratti_rd.voce_prezzo_6 as fase_b6,   
         contratti_rd.voce_prezzo_7 as fase_d1,    
         contratti_rd.voce_prezzo_8 as fase_d2,   
         contratti_rd.voce_prezzo_9 as fase_d3,   
         contratti_rd.voce_prezzo_10 as fase_d4,   
         contratti_rd.iva,   
         iva.aliq,   
         iva.des,   
         contratti_rd.cod_pag,   
         pagam.des,   
         trim(contratti_rd.banca) as banca,   
         contratti_rd.abi,   
         contratti_rd.cab,   
         trim(contratti_rd.altre_condizioni) as altre_condizioni,   
         contratti_rd.data_inizio,   
         contratti_rd.data_fine,   
         contratti_rd.fattura_da,   
         contratti_rd.x_datins,   
         contratti_rd.x_utente,   
         contratti_rd.stato,   
         contratti_rd.data_stampa , 
        ~"- D010108.H12:00.U0011~" as k_marca_stampa  
         , trim(contratti_rd.descr_1) + ': '       as v1_descr_xctr 
         , trim(contratti_rd.descr_1) + ': ' as v1_descr_xctr_eng
         , trim(contratti_rd.descr_2) + ': '        as v2_descr_xctr 
         , trim(contratti_rd.descr_2) + ': ' as v2_descr_xctr_eng
         , trim(contratti_rd.descr_3) + ': '        as v3_descr_xctr 
         , trim(contratti_rd.descr_3) + ': ' as v3_descr_xctr_eng
         , trim(contratti_rd.descr_4) + ': '        as v4_descr_xctr 
         , trim(contratti_rd.descr_4) + ': ' as v4_descr_xctr_eng
         , trim(contratti_rd.descr_5) + ': '        as v5_descr_xctr 
         , trim(contratti_rd.descr_5) + ': ' as v5_descr_xctr_eng
         , trim(contratti_rd.descr_6) + ': '        as v6_descr_xctr 
         , trim(contratti_rd.descr_6) + ': ' as v6_descr_xctr_eng
         , trim(contratti_rd.descr_7) + ': '        as v7_descr_xctr 
         , trim(contratti_rd.descr_7) + ': ' as v7_descr_xctr_eng
         , trim(contratti_rd.descr_8) + ': '        as v8_descr_xctr 
         , trim(contratti_rd.descr_8) + ': ' as v8_descr_xctr_eng
         , trim(contratti_rd.descr_9) + ': '        as v9_descr_xctr 
         , trim(contratti_rd.descr_9) + ': ' as v9_descr_xctr_eng
         , trim(contratti_rd.descr_10) + ': '        as v10_descr_xctr 
         , trim(contratti_rd.descr_10) + ': ' as v10_descr_xctr_eng
         , COALESCE(contratti_rd.totale_contratto,0) as totale_contratto
		FROM contratti_rd LEFT OUTER JOIN clie_settori ON contratti_rd.id_clie_settore = clie_settori.id_clie_settore
		LEFT OUTER JOIN clienti ON contratti_rd.id_cliente = clienti.codice
		LEFT OUTER JOIN gru ON contratti_rd.gruppo = gru.codice
		LEFT OUTER JOIN pagam ON contratti_rd.cod_pag = pagam.codice
		LEFT OUTER JOIN iva ON contratti_rd.iva = iva.codice  
//                               LEFT OUTER JOIN listino_voci as v1 ON contratti_rd.id_listino_voce_1 = v1.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v2 ON contratti_rd.id_listino_voce_2 = v2.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v3 ON contratti_rd.id_listino_voce_3 = v3.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v4 ON contratti_rd.id_listino_voce_4 = v4.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v5 ON contratti_rd.id_listino_voce_5 = v5.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v6 ON contratti_rd.id_listino_voce_6 = v6.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v7 ON contratti_rd.id_listino_voce_7 = v7.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v8 ON contratti_rd.id_listino_voce_8 = v8.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v9 ON contratti_rd.id_listino_voce_9 = v9.id_listino_voce
//                               LEFT OUTER JOIN listino_voci as v10 ON contratti_rd.id_listino_voce_10 = v10.id_listino_voce
   WHERE contratti_rd.id_contratto_rd = :k_id_contratto_rd    
