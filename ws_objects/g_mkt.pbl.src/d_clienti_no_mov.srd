 retrieve="  SELECT distinct
         s_armo.id_meca,   
         s_armo.num_int,   
         s_armo.data_int,   
         s_armo.magazzino,   
         s_armo.dose,   
           (s_armo.colli_2) as colli_2,   
         s_armo.pedane as pedane,   
         s_armo.m_cubi,   
         s_arsp.num_bolla_out,   
         s_arsp.data_bolla_out,   
         COALESCE(s_arsp.colli,0) as colli,   
         v_arfa.tipo_doc,   
         v_arfa.id_fattura,   
         s_arfa.num_fatt,   
         s_arfa.data_fatt,   
         COALESCE(s_arfa.colli,0) as colli_fatt,   
         s_arsp.cub_tot as m_cubi_arsp ,
         s_arfa.prezzo_t,   
         c1.rag_soc_10,   
         c2.rag_soc_10,   
         c3.rag_soc_10,
			s_armo.clie_1,
			s_arsp.clie_2,
			s_armo.clie_3
        ,certif.num_certif
		,s_arsp.id_sped  
        ,meca.contratto
         ,v_arfa.art   
         ,v_arfa.prezzo_u
         ,v_arfa.prodotti_des   
         ,v_arfa.gruppo   
         ,v_arfa.campione  
    FROM (((((
             s_armo    
              inner join meca on s_armo.id_meca = meca.id 
	          left outer join s_arfa on
					s_armo.id_meca = s_arfa.id_meca 
               and s_armo.dose = s_arfa.dose)
	          LEFT OUTER join v_arfa on
					s_arfa.num_fatt = v_arfa.num_fatt
					and s_arfa.data_fatt = v_arfa.data_fatt
	          left outer join s_arsp on
					s_armo.id_meca = s_arsp.id_meca 
               and s_armo.dose = s_arsp.dose)
	          left outer join clienti c1 on
					s_armo.clie_1 = c1.codice)
	          left outer join clienti c2 on
					s_arsp.clie_2 = c2.codice)
	          left outer join clienti c3 on
					s_armo.clie_3 = c3.codice)
	          left outer join certif on
					s_armo.id_meca = certif.id_meca
   WHERE 
         s_armo.data_int between :k_data_ini and :k_data_fin and
         ( ( s_armo.clie_1 = :k_id_cliente ) OR  
           ( s_armo.clie_2 = :k_id_cliente ) OR  
           ( s_armo.clie_3 = :k_id_cliente ) )   
		and (s_armo.magazzino < 2 or s_armo.magazzino > 8 or s_armo.magazzino is null) 

union all
  SELECT 
         0 as id_meca,   
         0 as num_int,   
         convert(date, '01.01.1899') as data_int,   
         0,   
         0,
         0 as colli_2,
         0 as pedane,   
         0,   
         0 as num_bolla_out,   
         convert(date, '01.01.1899') as data_bolla_out,   
         0 as colli,   
         v_arfa.tipo_doc,   
         v_arfa.id_fattura,   
         v_arfa.num_fatt,   
         v_arfa.data_fatt,   
         COALESCE(v_arfa.colli_out,0) as colli_fatt,   
         0 as m_cubi_arsp ,
         v_arfa.prezzo_t,   
         '',   
         '',   
         c3.rag_soc_10,
		0,
		0,
		v_arfa.clie_3
        ,0
		,0  
        ,0
         ,v_arfa.art   
         ,v_arfa.prezzo_u
         ,v_arfa.prodotti_des   
         ,v_arfa.gruppo   
         ,v_arfa.campione  
    FROM v_arfa   
	          left outer join clienti c3 on
					v_arfa.clie_3 = c3.codice
  where v_arfa.clie_3 = :k_id_cliente
         		and v_arfa.data_fatt between :k_data_ini and :k_data_fin
				and v_arfa.id_armo = 0
ORDER BY data_int desc,   
         num_int desc,  
         data_bolla_out desc, 
         num_bolla_out desc,   
         data_fatt desc,   
         num_fatt
" arguments=(("k_id_cliente", number),("k_data_ini", date),("k_data_fin", date)) )
