 column=(type=date updatewhereclause=yes name=offerta_data dbname="contratti_dp.offerta_data" )
 column=(type=long updatewhereclause=yes name=id_cliente dbname="contratti_dp.id_cliente" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_10 dbname="clienti.rag_soc_10" )
 column=(type=char(1) updatewhereclause=yes name=stato dbname="contratti_dp.stato" values=" Nuovo	1/ Riaperto	2/ Stampato	3/ Accettato	5/" )
 column=(type=date updatewhereclause=yes name=data_stampa dbname="contratti_dp.data_stampa" )
 column=(type=char(16) updatewhereclause=yes name=contratti_dp_offerta_validita dbname="contratti_dp.offerta_validita" )
 column=(type=char(80) updatewhereclause=yes name=oggetto dbname="contratti_dp.oggetto" )
 column=(type=date updatewhereclause=yes name=data_inizio dbname="contratti_dp.data_inizio" )
 column=(type=date updatewhereclause=yes name=data_fine dbname="contratti_dp.data_fine" )
 column=(type=char(8) updatewhereclause=yes name=id_clie_settore dbname="contratti_dp.id_clie_settore" )
 column=(type=char(80) updatewhereclause=yes name=clie_settori_descr dbname="clie_settori.descr" )
 column=(type=char(1) updatewhereclause=yes name=contratti_dp_attivo dbname="contratti_dp.attivo" )
 column=(type=long updatewhereclause=yes name=sel dbname="sel" values="	1/	0" )
 retrieve="  SELECT contratti_dp.id_contratto_dp,   
         contratti_dp.anno,   
         contratti_dp.offerta_data,   
         contratti_dp.id_cliente,   
         clienti.rag_soc_10,   
         contratti_dp.stato,   
         contratti_dp.data_stampa,   
         contratti_dp.offerta_validita,   
         contratti_dp.oggetto,   
         contratti_dp.data_inizio,   
         contratti_dp.data_fine,   
         contratti_dp.id_clie_settore,   
         clie_settori.descr,   
         contratti_dp.attivo,
         0 as sel
    FROM contratti_dp LEFT OUTER JOIN clie_settori ON contratti_dp.id_clie_settore = clie_settori.id_clie_settore 
							LEFT OUTER JOIN clienti ON contratti_dp.id_cliente = clienti.codice 
  WHERE (:k_id_cliente = 0 or contratti_dp.id_cliente = :k_id_cliente)
			and contratti_dp.offerta_data >= :k_data 
              and contratti_dp.attivo = 'S'
              and contratti_dp.stato = '5'
" arguments=(("k_data", date),("k_id_cliente", number))  sort="offerta_data D id_contratto_dp D " )
