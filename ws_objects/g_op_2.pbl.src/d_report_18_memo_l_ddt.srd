table(column=(type=long update=yes updatewhereclause=yes identity=yes name=id_memo dbname="memo.id_memo" )
 column=(type=datetime update=yes updatewhereclause=yes name=dataora_ins dbname="memo.dataora_ins" )
 column=(type=char(12) update=yes updatewhereclause=yes name=utente_ins dbname="utente_ins" )
 column=(type=char(40) update=yes updatewhereclause=yes name=titolo dbname="memo.titolo" )
 column=(type=char(120) update=yes updatewhereclause=yes name=note dbname="memo.note" )
 column=(type=char(5) updatewhereclause=yes name=tipo_memo dbname="tipo_memo" )
 column=(type=long updatewhereclause=yes name=id_cliente dbname="meca.id_cliente" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="meca_memo.id_meca" )
 column=(type=long updatewhereclause=yes name=num_int dbname="meca.num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="meca.data_int" )
 column=(type=char(61) updatewhereclause=yes name=rag_soc_10 dbname="rag_soc_10" )
 column=(type=long updatewhereclause=yes name=num_bolla_out dbname="sped.num_bolla_out" )
 column=(type=date updatewhereclause=yes name=data_bolla_out dbname="sped.data_bolla_out" )
 column=(type=long updatewhereclause=yes identity=yes name=id_sped dbname="sped.id_sped" )
 retrieve="  SELECT distinct memo.id_memo,   
         memo.dataora_ins,   
         trim(memo.utente_ins) as utente_ins,   
         memo.titolo,   
         memo.note,   
         trim(memo.tipo_memo) as tipo_memo,   
		meca.clie_3 as id_cliente,
		meca_memo.id_meca ,
		meca.num_int,
		meca.data_int,
         COALESCE(clienti.rag_soc_10,'') + ' ' + COALESCE(clienti.rag_soc_11, '') as rag_soc_10, 
		sped.num_bolla_out,
		sped.data_bolla_out,
		sped.id_sped
    FROM meca_memo inner join meca on  meca_memo.id_meca = meca.id
					  inner join memo on meca_memo.id_memo = memo.id_memo
					  inner join armo on meca.id = armo.id_meca
					  inner join arsp on armo.id_armo = arsp.id_armo and arsp.id_sped = :k_id_sped
					  inner join sped on arsp.id_sped = sped.id_sped
					  inner join clienti on sped.clie_2 = clienti.codice
   WHERE meca_memo.allarme = :k_allarme 
union 
  SELECT distinct memo.id_memo,   
         memo.dataora_ins,   
         trim(memo.utente_ins) as utente_ins,   
         memo.titolo,   
         memo.note,   
         trim(memo.tipo_memo) as tipo_memo,   
		clienti_memo.id_cliente,
		meca.id as id_meca ,
		meca.num_int,
		meca.data_int,
         COALESCE(clienti.rag_soc_10,'') + ' ' + COALESCE(clienti.rag_soc_11, '') as rag_soc_10, 
		sped.num_bolla_out,
		sped.data_bolla_out,
		sped.id_sped
    FROM clienti_memo inner join memo on  clienti_memo.id_memo = memo.id_memo
					 inner join clienti on clienti_memo.id_cliente = clienti.codice 
					 inner join sped on clienti_memo.id_cliente = sped.clie_2  and sped.id_sped = :k_id_sped
					 inner join arsp on sped.id_sped = arsp.id_sped 
					 inner join armo on arsp.id_armo = armo.id_armo 
					 inner join meca on armo.id_meca = meca.id
   WHERE clienti_memo.allarme = :k_allarme and clienti_memo.id_cliente = :k_clie_2
                   
ORDER BY memo.dataora_ins DESC,   
         memo.titolo ASC   
" arguments=(("k_allarme", string),("k_id_sped", number),("k_clie_2", number)) )
