 retrieve="  SELECT help.id_meca,   
         help.num_int,   
         convert(date,help.data_ent) data_ent,   
         help.data_int,   
         data_bolla_out,   
         COALESCE(num_bolla_out, 0) num_bolla_out,   
         COALESCE(colli_trattati,0) colli_trattati,   
         (convert(datetime, CONVERT(CHAR(8), help.consegna_data, 112) + ' ' + CONVERT(CHAR(8), coalesce(help.consegna_ora, '00:00:00'), 108))) consegna_data,   
         (help.consegna_ora) consegna_ora, 
         COALESCE(gruppo,0) gruppo,
         (help.id_sped) id_sped, 
         help.data_fin,   
         (convert(datetime, CONVERT(CHAR(8), data_rit, 112) + ' ' + CONVERT(CHAR(8), ora_rit, 108))) data_rit,   
         ora_rit ora_rit ,
         month(help.data_ent) month_data_ent, 
         (convert(datetime, CONVERT(CHAR(8), data_certif, 112) + ' ' + CONVERT(CHAR(8), ora_certif, 108))) data_certif,   
         ora_certif ora_certif 
	   ,clienti.codice
	   ,trim(clienti.rag_soc_10) rag_soc
    FROM vx_mast2_report_23_idx_consegne_help as help inner join meca on help.id_meca = meca.id
	                inner join clienti on meca.clie_3 = clienti.codice
   where month(help.data_ent) = month(:k_data)
        and year(help.data_ent) = year(:k_data) 
" arguments=(("k_data", date))  sort="data_ent A gruppo A num_int A " )
