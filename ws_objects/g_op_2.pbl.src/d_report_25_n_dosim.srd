table(column=(type=date updatewhereclause=yes name=data_int dbname="meca.data_int" )
 column=(type=long updatewhereclause=yes name=num_int dbname="meca.num_int" )
 column=(type=date updatewhereclause=yes name=data_bolla_in dbname="meca.data_bolla_in" )
 column=(type=char(24) updatewhereclause=yes name=num_bolla_in dbname="meca.num_bolla_in" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_2 dbname="colli_2" )
 column=(type=long updatewhereclause=yes name=clie_1 dbname="meca.clie_1" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="meca.clie_3" )
 column=(type=char(30) updatewhereclause=yes name=fatturato dbname="clienti.fatturato" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="meca.id_meca" )
 column=(type=datetime updatewhereclause=yes name=data_ent dbname="meca.data_ent" )
 column=(type=long updatewhereclause=yes name=e1doco dbname="e1doco" )
 column=(type=long updatewhereclause=yes name=e1rorn dbname="e1rorn" )
 column=(type=char(4) updatewhereclause=yes name=e1srst dbname="e1srst" )
 column=(type=long updatewhereclause=yes name=dosimprev dbname="dosimprev" )
 column=(type=long updatewhereclause=yes name=k_n_meca_dosim dbname="k_n_meca_dosim" )
 column=(type=decimal(0) updatewhereclause=yes name=num_certif dbname="num_certif" )
 column=(type=date updatewhereclause=yes name=certif_data_stampa dbname="certif.data_stampa" )
 retrieve="  SELECT meca.data_int,   
         meca.num_int,   
         meca.data_bolla_in,   
         meca.num_bolla_in,   
         (select sum(armo.colli_2) from armo where armo.id_meca = meca.id) colli_2 ,   
         meca.clie_1,
         meca.clie_3,
         clienti_c.rag_soc_10 as fatturato,   
         meca.id id_meca 
       ,meca.data_ent
       ,coalesce(meca.e1doco, 0) e1doco
       ,coalesce(meca.e1rorn, 0) e1rorn
       ,coalesce(meca.e1srst, 'NC') e1srst
       ,coalesce(meca.dosimprev, 0) dosimprev
       ,coalesce(count(meca_dosim.id_meca), 0) k_n_meca_dosim
       ,coalesce(certif.num_certif, 0) num_certif
       ,certif.data_stampa
    FROM meca inner JOIN meca_dosim  ON meca.id = meca_dosim.id_meca 
   LEFT OUTER JOIN clienti clienti_c ON meca.clie_3 = clienti_c.codice 
   LEFT OUTER JOIN certif ON meca.id = certif.id_meca 
   WHERE 
         ( meca.id between :id_meca_da and :id_meca_a )   
        and  ( meca.data_int between :data_da and :data_a )   
		and (meca.aperto is null or meca.aperto NOT IN ('A'))
  group by 
        meca.data_int,   
         meca.num_int,   
         meca.data_bolla_in,   
         meca.num_bolla_in,   
         meca.clie_1,
         meca.clie_3,
         clienti_c.rag_soc_10,   
         meca.id  
       ,meca.data_ent
      ,meca.e1doco  
     ,meca.e1rorn
      ,meca.e1srst
     ,meca.dosimprev
       ,certif.num_certif
       ,certif.data_stampa
	order by meca.data_int desc
" arguments=(("id_meca_da", number),("id_meca_a", number),("data_da", date),("data_a", date)) )
