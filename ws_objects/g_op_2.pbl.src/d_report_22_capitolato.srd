 column=(type=decimal(2) updatewhereclause=yes name=pedane dbname="pedane" )
 column=(type=long updatewhereclause=yes name=clie_1 dbname="meca.clie_1" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_1 dbname="clienti.rag_soc_1" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="meca.clie_2" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_2 dbname="clienti.rag_soc_2" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="meca.clie_3" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_3 dbname="clienti.rag_soc_3" )
 column=(type=decimal(0) updatewhereclause=yes name=num_certif dbname="certif.num_certif" )
 column=(type=date updatewhereclause=yes name=certif_data dbname="certif.data" )
 retrieve=" SELECT  
		  '?' as b_barcode
        ,contratti.codice as contratti_codice
        ,contratti.sc_cf as sc_cf
        ,meca.id as id_meca   
        ,meca.num_int   
        ,meca.data_int   
        ,meca.data_ent  
        ,meca.e1doco
        ,meca.e1rorn
        ,max(armo.dose) as dose
        ,(COALESCE(min(meca_dosim_1.dosim_dose), 0.00) ) as dose_min
        ,(COALESCE(max(meca_dosim_2.dosim_dose), 0.00) ) as dose_max
        ,sum(armo.colli_2) as colli_2
	   ,sum(armo.pedane) as pedane
        ,meca.clie_1
        ,clienti_a.rag_soc_10 as rag_soc_1
        ,meca.clie_2
        ,clienti_b.rag_soc_10 as rag_soc_2
        ,meca.clie_3
        ,clienti_c.rag_soc_10 as rag_soc_3
        ,certif.num_certif
        ,certif.data
    FROM 
         vx_MAST2_report_22 as vx_report_22 inner join armo 
         ON vx_report_22.id_armo = armo.id_armo
					 INNER JOIN prodotti 
         ON armo.art = prodotti.codice
               INNER JOIN meca
         ON armo.id_meca = meca.id
					 INNER JOIN contratti 
			ON meca.contratto = contratti.codice
               INNER JOIN clienti as clienti_a 
         ON meca.clie_1 = clienti_a.codice
               INNER JOIN clienti as clienti_b 
         ON meca.clie_2 = clienti_b.codice
               INNER JOIN clienti as clienti_c 
         ON meca.clie_3 = clienti_c.codice
               LEFT OUTER JOIN certif 
         ON meca.id = certif.id_meca
			 LEFT OUTER JOIN meca_dosim as meca_dosim_1
		ON meca_dosim_1.id_meca = meca.id and meca_dosim_1.dosim_flg_tipo_dose = 'M'
			 LEFT OUTER JOIN meca_dosim as meca_dosim_2
		ON meca_dosim_2.id_meca = meca.id and meca_dosim_2.dosim_flg_tipo_dose = 'X'
where :k_sc_cf = '' or contratti.sc_cf = :k_sc_cf
group by 
        contratti.codice
        ,contratti.sc_cf
        ,meca.id
        ,meca.num_int   
        ,meca.data_int   
        ,meca.data_ent  
        ,meca.e1doco
        ,meca.e1rorn
        ,meca.clie_1
        ,clienti_a.rag_soc_10
        ,meca.clie_2
        ,clienti_b.rag_soc_10
        ,meca.clie_3
        ,clienti_c.rag_soc_10
        ,certif.num_certif
        ,certif.data
" arguments=(("k_sc_cf", string)) )
