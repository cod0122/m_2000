 retrieve=" SELECT distinct   
  sped.id_sped,     
  sped.num_bolla_out,  
  sped.data_bolla_out, 
  sped.stampa,       
  sped.colli,   
  sped.data_rit,     
  sped.data_uscita,   
  sped.ora_rit,   
  sped.clie_2, 	
  c2.rag_soc_10 as rag_soc_2,
  sped.clie_3, 			
  c3.rag_soc_10 as rag_soc_3,   
  trim(sped.causale) as sped_causale,    
  trim(sped.aspetto) as sped_aspetto
  FROM 
      sped inner join clienti c2 ON 		
                  sped.clie_2 = c2.codice
            LEFT OUTER JOIN clienti c3 ON 	
                 sped.clie_3 = c3.codice 
   where (:k_clie_2 = 0 or sped.clie_2 = :k_clie_2) 
       and sped.data_bolla_out between :k_data_da and :k_data_a
		and sped.id_sped in 
   ( select arsp.id_sped
          from arsp
            inner join armo on
                 arsp.id_armo = armo.id_armo 	
            inner join meca on
                 armo.id_meca = meca.id  
             and (meca.aperto is null or meca.aperto <> 'N' )
          where arsp.stampa = 'S' 
    )
 order by sped.data_bolla_out desc, sped.num_bolla_out desc 
" arguments=(("k_clie_2", number),("k_data_da", date),("k_data_a", date)) )
