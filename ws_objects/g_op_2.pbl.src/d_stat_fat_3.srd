 column=(type=long updatewhereclause=yes name=mese dbname="mese" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="s_arfa.clie_3" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_10 dbname="clienti.rag_soc_10" )
 column=(type=char(30) updatewhereclause=yes name=loc_1 dbname="clienti.loc_1" )
 column=(type=char(2) updatewhereclause=yes name=prov_1 dbname="clienti.prov_1" )
 column=(type=char(2) updatewhereclause=yes name=tipo_doc dbname="s_arfa.tipo_doc" )
 column=(type=decimal(2) updatewhereclause=yes name=k_fatt dbname="k_fatt" )
 column=(type=decimal(0) updatewhereclause=yes name=k_colli dbname="k_colli" )
 column=(type=decimal(2) updatewhereclause=yes name=k_cub_tot1 dbname="k_cub_tot" )
 column=(type=char(32) updatewhereclause=yes name=regione dbname="province.regione" )
 retrieve="  SELECT 
			year(s_arfa.data_fatt) as anno,   
			month(s_arfa.data_fatt) as mese,   
			s_arfa.clie_3,   
         clienti.rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         s_arfa.tipo_doc,   
         sum(s_arfa.prezzo_t) as k_fatt,   
         sum(s_arfa.colli) as k_colli,   
         sum(s_arfa.cub_tot) as k_cub_tot, 
         province.regione  
    FROM s_arfa  
			LEFT OUTER JOIN clienti  ON s_arfa.clie_3 = clienti.codice
			left outer join clie_classi on clienti.id_clie_classe = clie_classi.id_clie_classe
			left outer join province on clienti.prov_1 = province.sigla
             left outer join gru on s_arfa.gruppo = gru.codice 
	where
			(:k_id_cliente = 0 or s_arfa.clie_3 = :k_id_cliente) and   
			( (:k_no_gruppo = 'S' and (s_arfa.gruppo is null or s_arfa.gruppo = 0) ) 
                or (:k_no_gruppo = 'N' and s_arfa.gruppo > 0 and ((:k_id_gruppo = 0 and gru.escludi_da_stat_glob = 'N') or s_arfa.gruppo = :k_id_gruppo ))) and     
			( (:k_no_dose = 'S' and s_arfa.dose = 0)  
			  or (:k_no_dose = 'N' and (:k_dose = 0 or s_arfa.dose = :k_dose))
         ) and 
			(:k_magazzino = 9 or s_arfa.magazzino = :k_magazzino ) and 
			(s_arfa.data_fatt between :k_data_da and :k_data_a) 
			and (:k_id_clie_classe = '' or clienti.id_clie_classe = :k_id_clie_classe ) 
			and (:k_classe_punteggio = '' or clie_classi.punteggio = :k_classe_punteggio ) 
		group by 
		year(s_arfa.data_fatt),   
		month(s_arfa.data_fatt),   
		s_arfa.clie_3,   
         clienti.rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         s_arfa.tipo_doc,   
         province.regione  
		ORDER BY 
					1 desc, 2 desc,
				   4 ASC   

" arguments=(("k_id_gruppo", number),("k_id_cliente", number),("k_dose", number),("k_data_da", date),("k_data_a", date),("k_no_dose", string),("k_no_gruppo", string),("k_magazzino", number),("k_id_clie_classe", string),("k_classe_punteggio", string))  sort="anno D mese D k_fatt D rag_soc_10 A " )
