 retrieve="  SELECT i.clie_3 as clie_3,   
         clienti.rag_soc_10, 
         (COALESCE( i.colli_trattati,0)) as colli_2,   
         (COALESCE(s_artr.m_cubi,0.0000))      as m_cubi,   
         (COALESCE(s_artr.giri_f1_pl,0))  as giri_f1_pl,   
         (COALESCE(s_artr.giri_f1_lav,0)) as giri_f1_lav,   
         (COALESCE(s_artr.giri_f2_pl,0))  as giri_f2_pl,   
         (COALESCE(s_artr.giri_f2_lav,0)) as giri_f2_lav,   
         (COALESCE(s_artr.giri_f1_pl_gp,0))  as giri_f1_pl_gp,   
         (COALESCE(s_artr.giri_f1_lav_gp,0)) as giri_f1_lav_gp,   
         (COALESCE(s_artr.giri_f2_pl_gp,0))  as giri_f2_pl_gp,   
         (COALESCE(s_artr.giri_f2_lav_gp,0)) as giri_f2_lav_gp,   
         (COALESCE(s_artr.id_armo_contati,0)) as id_armo_contati,   
         (COALESCE(s_artr.pedane_trattate,0.00))  as pedane_trattate,
         (i.pedane )  as pedane,
         (i.imp_trattati )  as imp_periodo,
         (i.imp_trattati - i.imp_armo_fatt)        as imp_da_fatt, 
         (i.colli_fatt)       as colli_fatt,
         (i.colli_da_fatt)  as colli_da_fatt
       , (i.imp_x_pedana)  as imp_x_pedana
       , i.imp_fatturati_fila1      as imp_fatturati_fila1
       , i.imp_fatturati_fila2      as imp_fatturati_fila2 
	   ,s_artr.somma_giri     as somma_giri
	   ,0 as dettaglio
	  ,i.imp_fatturati as imp_fatt 
      ,CASE WHEN s_artr.somma_giri_F1 > 0 then (i.imp_fatturati_fila1 / (s_artr.somma_giri_F1 / 2)) 
         ELSE 0.00 END as imp_x_GiriF1 
      ,CASE WHEN s_artr.somma_giri_F2 > 0 then (i.imp_fatturati_fila2 / (s_artr.somma_giri_F2 / 2)) 
         ELSE 0.00 END as imp_x_GiriF2 
      ,i.colli_f1_lav  
      ,i.colli_f2_lav  
      ,s_artr.somma_giri_F1 , s_artr.somma_giri_F2
	,i.id_meca_contati id_meca_contati
	, ((COALESCE(s_artr.pedane_trattate,0.00)) / i.id_meca_contati)  pedane_in_xlotto 
			,i.n_dosimetri n_dosimetri
			,imp_fatt_dosimetri imp_fatt_dosimetri
    FROM #vx_MAST3_statp_artr_x_cli as s_artr 
                INNER JOIN #vx_MAST3_statp_imp2 as i ON 
         s_artr.clie_3 = i.clie_3   
                LEFT OUTER JOIN clienti ON  
         s_artr.clie_3 = clienti.codice    
"  sort="rag_soc_10 A " )
