table(column=(type=long updatewhereclause=yes identity=yes name=id_meca dbname="meca.id" dbalias=".id" )
 column=(type=long updatewhereclause=yes name=num_int dbname="meca.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="meca.data_int" dbalias=".data_int" )
 column=(type=long updatewhereclause=yes name=clie_1 dbname="meca.clie_1" dbalias=".clie_1" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="meca.clie_2" dbalias=".clie_2" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="meca.clie_3" dbalias=".clie_3" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10 dbname="clienti.rag_soc_10" dbalias="clienti_a.rag_soc_10" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10_1 dbname="clienti.rag_soc_10" dbalias="clienti_b.rag_soc_10" )
 column=(type=date updatewhereclause=yes name=meca_consegna_data dbname="meca.consegna_data" dbalias=".consegna_data" )
 column=(type=char(12) updatewhereclause=yes name=area_mag dbname="meca.area_mag" dbalias=".area_mag" )
 column=(type=decimal(0) updatewhereclause=yes name=nr_barcode dbname="nr_barcode" )
 column=(type=long updatewhereclause=yes name=k_flg_area_dawmf dbname="k_flg_area_dawmf" )
 retrieve="  SELECT meca.id,   
         meca.num_int,   
         meca.data_int,   
         meca.clie_1,   
         meca.clie_2,   
         meca.clie_3,   
         clienti_a.rag_soc_10,   
         clienti_b.rag_soc_10,   
         meca.consegna_data,   
         meca.area_mag,
         count(*) as nr_barcode,  
         0 as k_flg_area_dawmf
    FROM meca LEFT OUTER JOIN barcode bc      ON meca.id = bc.id_meca
                        LEFT OUTER JOIN clienti clienti_a ON meca.clie_1 = clienti_a.codice 
                        LEFT OUTER JOIN clienti clienti_b ON meca.clie_2 = clienti_b.codice   
    where year(meca.data_int) = :k_anno and meca.num_int between  :k_num_int_da and :k_num_int_a and (:k_clie_1=0 or meca.clie_1 = :k_clie_1) 
	group by  meca.id,   
         meca.num_int,   
         meca.data_int,   
         meca.clie_1,   
         meca.clie_2,   
         meca.clie_3,   
         clienti_a.rag_soc_10,   
         clienti_b.rag_soc_10,   
         meca.consegna_data,   
         meca.area_mag,
		k_flg_area_dawmf
   order by num_int
" arguments=(("k_anno", number),("k_num_int_da", number),("k_num_int_a", number),("k_clie_1", number)) )
