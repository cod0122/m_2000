 column=(type=datetime updatewhereclause=yes name=meca_data_ent dbname="meca.data_ent" )
 retrieve=" SELECT distinct 
         armo.magazzino,   
         armo.num_int,   
         armo.data_int,   
         meca.clie_1,   
         clienti_a.rag_soc_10 as rag_soc_1,   
         meca.clie_2,   
         clienti_b.rag_soc_10 as rag_soc_2,   
         meca.clie_3,   
         clienti_c.rag_soc_10 as rag_soc_3,   
         armo.art,   
         prodotti.des,
         armo.dose,
         armo.colli_2,
		armo.pedane,
         armo.m_cubi,
         armo.peso_kg,
         meca.consegna_data,
         meca_dosim.dosim_data,
         meca.num_bolla_in,
         meca.data_bolla_in,
         armo.id_armo,
	    armo.id_meca,
		barcode.barcode 
		,barcode.data_lav_fin
		,barcode.tipo_cicli
        ,case 
                when sl_pt.fila_pref = 2 then  0 
                else COALESCE((barcode.fila_1 + barcode.fila_1p) / 2,0) 
         end  sl_pt_fila_1
        ,case 
                when sl_pt.fila_pref = 1 then  0 
                else COALESCE((barcode.fila_2 + barcode.fila_2p) / 2,0) 
         end  sl_pt_fila_2

		,COALESCE((barcode.lav_fila_1 + barcode.lav_fila_1p) / 2,0) as lav_fila_1
		,COALESCE((barcode.lav_fila_2 + barcode.lav_fila_2p) / 2,0) as lav_fila_2
      ,meca.e1doco
      ,meca.data_ent
    FROM 
         vx_MAST2_report_7 as vx_report_7 inner join armo 
         ON vx_report_7.id_armo = armo.id_armo
					 INNER JOIN barcode 
         ON vx_report_7.id_armo = barcode.id_armo
					 LEFT OUTER JOIN prodotti 
         ON armo.art = prodotti.codice
               LEFT OUTER JOIN sl_pt  
         ON armo.cod_sl_pt = sl_pt.cod_sl_pt
               left OUTER JOIN meca
         ON armo.id_meca = meca.id
               LEFT OUTER JOIN clienti as clienti_a 
         ON meca.clie_1 = clienti_a.codice
               LEFT OUTER JOIN clienti as clienti_b 
         ON meca.clie_2 = clienti_b.codice
               LEFT OUTER JOIN clienti as clienti_c 
         ON meca.clie_3 = clienti_c.codice
               LEFT OUTER JOIN meca_dosim 
         ON meca.id = meca_dosim.id_meca


"  sort="data_int D num_int D id_armo A " )
