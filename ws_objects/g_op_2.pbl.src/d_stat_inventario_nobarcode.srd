table(column=(type=long updatewhereclause=yes name=num_int dbname="meca.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="meca.data_int" dbalias=".data_int" )
 column=(type=char(1) updatewhereclause=yes name=meca_aperto dbname="meca.aperto" dbalias=".aperto" )
 column=(type=char(255) updatewhereclause=yes name=meca_blk_descrizione dbname="meca_blk.descrizione" dbalias=".descrizione" )
 column=(type=char(24) updatewhereclause=yes name=num_bolla_in dbname="meca.num_bolla_in" dbalias=".num_bolla_in" )
 column=(type=date updatewhereclause=yes name=data_bolla_in dbname="meca.data_bolla_in" dbalias=".data_bolla_in" )
 column=(type=long updatewhereclause=yes name=clie_1 dbname="meca.clie_1" dbalias=".clie_1" )
 column=(type=char(30) updatewhereclause=yes name=mandante dbname="clienti.rag_soc_10" dbalias=".rag_soc_10" )
 column=(type=long updatewhereclause=yes identity=yes name=id_meca dbname="meca.id" dbalias=".id" )
 column=(type=long updatewhereclause=yes name=meca_stato dbname="meca.stato" dbalias=".stato" )
 column=(type=char(40) updatewhereclause=yes name=stato_descrizione dbname="meca_stato.descrizione" dbalias=".descrizione" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_2 dbname="colli_2" )
 retrieve="  SELECT meca.num_int,   
         meca.data_int,   
         meca.aperto,   
         meca_blk.descrizione,   
         meca.num_bolla_in,   
         meca.data_bolla_in,   
         meca.clie_1,   
         clienti.rag_soc_10,   
         meca.id,   
         meca.stato,   
		meca_stato.descrizione,
         sum(armo.colli_2) as colli_2  
    FROM meca LEFT OUTER JOIN meca_blk ON meca.id = meca_blk.id_meca 
LEFT OUTER JOIN clienti ON meca.clie_1 = clienti.codice 
LEFT OUTER JOIN meca_stato ON meca.stato = meca_stato.codice 
LEFT OUTER JOIN armo ON meca.id = armo.id_meca  
where  meca.data_int between :k_data_da and :k_data_a
             and meca.stato > 0
             and (:k_flag_lotto_chiuso = 'N' or (meca.aperto is not null and meca.aperto <> 'N' and meca.aperto <> 'A'))
group by 
         meca.num_int,   
         meca.data_int,   
         meca.aperto,   
         meca_blk.descrizione,   
         meca.num_bolla_in,   
         meca.data_bolla_in,   
         meca.clie_1,   
         clienti.rag_soc_10,   
         meca.stato,   
		meca_stato.descrizione,
         meca.id
ORDER BY meca.data_int ASC,   
         meca.num_int ASC   
" arguments=(("k_data_da", date),("k_data_a", date),("k_flag_lotto_chiuso", string)) )
