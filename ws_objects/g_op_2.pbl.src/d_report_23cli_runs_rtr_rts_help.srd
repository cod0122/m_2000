 retrieve="  SELECT help.id_meca,   
         help.num_int,   
         convert(date,help.data_ent) data_ent,   
         help.data,   
         help.data_bolla_out,   
         COALESCE(help.num_bolla_out, 0) num_bolla_out,   
         COALESCE(help.colli_trattati,0) colli_trattati,   
         COALESCE(help.rtr,0) rtr,   
         COALESCE(help.rts,0) rts, 
         COALESCE(help.gruppo,0) gruppo,
         COALESCE(help.etr,0) etr, 
         help.data_fin   
		,trim(clienti.rag_soc_10) rag_soc_10
    FROM vx_mast2_report_23CLI_runsrtrrts_help  as help inner join clienti on 
	           help.clie_3 = clienti.codice
   where month(help.data) = month(:k_data)
        and year(help.data) = year(:k_data) 
        and help.clie_3 = :k_clie 
" arguments=(("k_clie", number),("k_data", date))  sort="gruppo A data_ent A num_int A " )
