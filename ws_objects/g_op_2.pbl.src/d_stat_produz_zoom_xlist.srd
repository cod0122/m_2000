table(column=(type=long updatewhereclause=yes name=id_meca dbname="s_armo.id_meca" )
 column=(type=long updatewhereclause=yes name=num_int dbname="s_armo.num_int" )
 column=(type=date updatewhereclause=yes name=data_ent dbname="s_armo.data_ent" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_entrati dbname="colli_entrati" )
 column=(type=char(71) updatewhereclause=yes name=mandante dbname="mandante" )
 column=(type=char(71) updatewhereclause=yes name=ricevente dbname="ricevente" )
 column=(type=char(71) updatewhereclause=yes name=fatturato dbname="fatturato" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_trattati dbname="colli_trattati" )
 column=(type=long updatewhereclause=yes name=gruppo dbname="vx_mast5_statp_meca.gruppo" )
 column=(type=long updatewhereclause=yes name=vx_mast5_statp_meca_clie_3 dbname="vx_mast5_statp_meca.clie_3" )
 column=(type=decimal(4) updatewhereclause=yes name=imp_x_collo dbname="imp_x_collo" )
 column=(type=long updatewhereclause=yes name=id_listino dbname="vx_mast5_statp_meca.id_listino" )
 column=(type=char(25) updatewhereclause=yes name=e1litm dbname="e1litm" )
 retrieve="  SELECT  
 		s_armo.id_meca  id_meca,
          s_armo.num_int  num_int,   
          s_armo.data_ent  data_ent,   
          sum(dati.colli_entrati)  colli_entrati,
         concat(s_armo.clie_1, ' ', clienti_a.rag_soc_10)  mandante,
         concat(s_armo.clie_2, ' ', clienti_b.rag_soc_10)  ricevente,
         concat(s_armo.clie_3 , ' ', clienti_c.rag_soc_10)  fatturato
 	    ,sum(dati.colli_trattati)  colli_trattati
 		,r.gruppo  gruppo
 		,r.clie_3  clie_3
         ,sum(dati.imp_x_collo)  imp_x_collo
         ,r.id_listino  id_listino
         ,COALESCE(listino.e1litm, '')  e1litm 
    from  vx_MAST5_statp_meca as r inner join vx_MAST5_statp_colli as dati  ON 
          r.id_armo = dati.id_armo  
 			  INNER JOIN s_armo on
          r.id_armo = s_armo.id_armo  
 			  INNER JOIN listino on
          r.id_listino = listino.id 
 			  LEFT OUTER JOIN clienti as clienti_a ON 
          s_armo.clie_1 = clienti_a.codice   
                 LEFT OUTER JOIN clienti as clienti_b ON 
          s_armo.clie_2 = clienti_b.codice   
                 LEFT OUTER JOIN clienti as clienti_c ON 
          s_armo.clie_3 = clienti_c.codice   
      group by 
 		s_armo.id_meca,
          s_armo.num_int,   
          s_armo.data_ent,   
         concat(s_armo.clie_1, ' ', clienti_a.rag_soc_10),
         concat(s_armo.clie_2, ' ', clienti_b.rag_soc_10),
         concat(s_armo.clie_3 , ' ', clienti_c.rag_soc_10)
 		,r.gruppo 
 		,r.clie_3 
         ,r.id_listino 
         ,listino.e1litm

" )
