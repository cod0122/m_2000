table(column=(type=long updatewhereclause=yes name=num_int dbname="barcode.num_int" )
 column=(type=char(13) updatewhereclause=yes name=barcode_lav dbname="barcode.barcode" )
 column=(type=long updatewhereclause=yes name=barcode_num_int_1 dbname="barcode.num_int" )
 column=(type=char(13) updatewhereclause=yes name=barcode dbname="barcode.barcode" )
 column=(type=char(1) updatewhereclause=yes name=de dbname="de" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="barcode.id_meca" )
 column=(type=long updatewhereclause=yes name=clie_1 dbname="meca.clie_1" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10_padre dbname="clienti.rag_soc_10" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="meca.clie_1" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10_figlio dbname="clienti.rag_soc_10" )
 column=(type=date updatewhereclause=yes name=pl_barcode_data_chiuso_1 dbname="pl_barcode.data_chiuso" )
 column=(type=long updatewhereclause=yes name=clie_2_padre dbname="meca.clie_2_padre" )
 column=(type=char(63) updatewhereclause=yes name=padre_ric_rag_soc_10 dbname="padre_ric_rag_soc_10" )
 column=(type=long updatewhereclause=yes name=clie_2_figlio dbname="meca.clie_2_figlio" )
 column=(type=char(63) updatewhereclause=yes name=figlio_ric_rag_soc_10 dbname="figlio_ric_rag_soc_10" )
 retrieve="  SELECT 
         padre.num_int,   
         padre.barcode,   
         figlio.num_int,   
         figlio.barcode,   
         ' ' as de,   
         padre.id_meca
        ,meca_padre.clie_1
        ,clie_padre.rag_soc_10
        ,meca_figlio.clie_1 
        ,clie_figlio.rag_soc_10
        ,pl_barcode.data_chiuso
        ,meca_padre.clie_2 as clie_2_padre
        ,concat((select trim(rag_soc_10) from clienti where codice = meca_padre.clie_2), ~" (~", convert(varchar, meca_padre.clie_2), ~")~") as padre_ric_rag_soc_10
        ,meca_figlio.clie_2 as clie_2_figlio
        ,concat((select trim(rag_soc_10) from clienti where codice = meca_figlio.clie_2), ~" (~", convert(varchar, meca_figlio.clie_2), ~")~") as figlio_ric_rag_soc_10
    FROM pl_barcode inner join barcode as padre on  pl_barcode.codice = padre.pl_barcode 
                              LEFT OUTER JOIN barcode as figlio ON padre.barcode = figlio.barcode_lav
                              INNER JOIN meca as meca_padre ON padre.id_meca = meca_padre.id
                              INNER JOIN clienti as clie_padre ON meca_padre.clie_1 = clie_padre.codice
                              INNER JOIN meca as meca_figlio ON figlio.id_meca = meca_figlio.id
                              INNER JOIN clienti as clie_figlio ON meca_figlio.clie_1 = clie_figlio.codice
   WHERE  
          pl_barcode.data_chiuso = :data_chiuso AND  
          padre.groupage = 'S'  AND  
         (padre.barcode_lav = '' OR padre.barcode_lav is null) 
         and (meca_padre.aperto is null or meca_padre.aperto <> 'N')
ORDER BY padre.num_int ASC,   
         padre.barcode ASC   
" arguments=(("data_chiuso", date)) )
