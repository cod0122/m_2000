 column=(type=long updatewhereclause=yes name=clie_2 dbname="clie_2" )
 column=(type=char(30) updatewhereclause=yes name=c2_rag_soc_10 dbname="c2_rag_soc_10" )
 column=(type=long updatewhereclause=yes name=clie_1 dbname="clie_1" )
 column=(type=long updatewhereclause=yes name=id_fattura dbname="id_fattura" )
 column=(type=long updatewhereclause=yes name=arfa_num_fatt dbname="num_fatt" )
 column=(type=date updatewhereclause=yes name=arfa_data_fatt dbname="data_fatt" )
 retrieve=" 	SELECT distinct 
	      certif.id, 
	      certif.data_stampa, 
	      certif.num_certif, 
	      certif.data, 
	      certif.id_meca, 
		meca.num_int, 
		meca.data_int, 
		COALESCE(arsp.id_sped,0) as id_sped, 
		COALESCE(arsp.num_bolla_out, 0) as arsp_num_bolla_out, 
		arsp.data_bolla_out,
		meca.contratto, 
		contratti.mc_co, 
		contratti.sc_cf, 
	      certif.lav_data_fin, 
	      certif.colli, 
	      certif.dose, 
         meca.clie_3,  
--  (select c3.rag_soc_10 from clienti as c3 where meca.clie_3 = c3.codice) as c3_rag_soc_10, 
         c3.rag_soc_10 c3_rag_soc_10, 
         certif.clie_2,  
--  (select c2.rag_soc_10 from clienti as c2 where certif.clie_2 = c2.codice) as c2_rag_soc_10,
         c2.rag_soc_10 c2_rag_soc_10, 
         meca.clie_1, 
		COALESCE(arfa.id_fattura, 0) as id_fattura, 
		COALESCE(arfa.num_fatt, 0) as num_fatt, 
		COALESCE(arfa.data_fatt, convert(date,'01.01.1899')) as data_fatt
   FROM  certif 
	        INNER JOIN meca ON  
	     certif.id_meca = meca.id  
	        inner JOIN contratti ON  
	     meca.contratto = contratti.codice 
	        inner JOIN armo ON  
	     meca.id = armo.id_meca 
	        inner JOIN clienti as c3 ON  
	     meca.clie_3 = c3.codice 
	        inner JOIN clienti as c2 ON  
	    certif.clie_2 = c2.codice 
	        inner JOIN arsp ON  
	     armo.id_armo = arsp.id_armo 
	        left outer JOIN arfa ON  
	     armo.id_armo = arfa.id_armo 
  where  certif.data_stampa between :k_stampa_ini and :k_stampa_fin
       and contratti.mc_co like :k_mc_co
       and (:k_clie_3 = 0 or meca.clie_3 = :k_clie_3 or arfa.clie_3 = :k_clie_3)

" arguments=(("k_stampa_ini", date),("k_stampa_fin", date),("k_mc_co", string),("k_clie_3", number)) )
