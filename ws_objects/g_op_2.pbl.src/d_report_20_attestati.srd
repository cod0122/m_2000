 column=(type=long updatewhereclause=yes name=arsp_num_bolla_out dbname="arsp_num_bolla_out" )
 column=(type=date updatewhereclause=yes name=arsp_data_bolla_out dbname="data_bolla_out" )
 column=(type=long updatewhereclause=yes name=meca_contratto dbname="contratto" )
 column=(type=char(12) updatewhereclause=yes name=contratti_mc_co dbname="mc_co" )
 column=(type=char(12) updatewhereclause=yes name=contratti_sc_cf dbname="sc_cf" )
