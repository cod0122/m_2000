table(column=(type=long updatewhereclause=yes name=id_meca dbname="s_armo.id_meca" )
 column=(type=long updatewhereclause=yes name=num_int dbname="s_armo.num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="s_armo.data_int" )
 column=(type=decimal(0) updatewhereclause=yes name=colli dbname="colli" )
 column=(type=decimal(2) updatewhereclause=yes name=pedane dbname="pedane" )
 column=(type=char(71) updatewhereclause=yes name=mandante dbname="mandante" )
 column=(type=char(71) updatewhereclause=yes name=ricevente dbname="ricevente" )
 column=(type=char(71) updatewhereclause=yes name=fatturato dbname="fatturato" )
 retrieve="SELECT  
			s_armo.id_meca as id_meca,
         s_armo.num_int as num_int,   
         s_armo.data_int as data_int,   
         sum(s_armo.colli_2) as colli,
			sum(s_armo.pedane) as pedane,
         concat(s_armo.clie_1, ' ', clienti_a.rag_soc_10) as mandante,
         concat(s_armo.clie_2, ' ', clienti_b.rag_soc_10) as ricevente,
         concat(s_armo.clie_3 , ' ', clienti_c.rag_soc_10) as fatturato
   from  s_armo LEFT OUTER JOIN clienti as clienti_a ON 
         s_armo.clie_1 = clienti_a.codice   
                inner JOIN vx_info_stat_inv2 as r ON 
         s_armo.id_meca = r.id_meca   
                LEFT OUTER JOIN clienti as clienti_b ON 
         s_armo.clie_2 = clienti_b.codice   
                LEFT OUTER JOIN clienti as clienti_c ON 
         s_armo.clie_3 = clienti_c.codice   
    where 
				(:k_clie_3 = 0 or (:k_clie_3 > 0 and s_armo.clie_3 = :k_clie_3)) 
     group by 
			s_armo.id_meca,
         s_armo.num_int,   
         s_armo.data_int,   
         sum(s_armo.colli_2),
         concat(s_armo.clie_1, ' ', clienti_a.rag_soc_10),
         concat(s_armo.clie_2, ' ', clienti_b.rag_soc_10),
         concat(s_armo.clie_3 , ' ', clienti_c.rag_soc_10)

" arguments=(("k_clie_3", number)) )
