 retrieve="  SELECT distinct  
        case
			 WHEN  pallet.F2AVP > '00' or pallet.F2APP > '00' THEN '2'
			 WHEN  pallet.F1AVP > '00' or pallet.F1APP > '00' THEN '1'
			ELSE '0'
        end as fila
       ,(data_entrata - INTERVAL EXTRACT(SECOND FROM data_entrata) SECOND) as k_dataora_lav_ini
     ,now() k_dataora_lav_prev_fin
	  , 0 as k_avg_time_io_minute 
     ,now() k_dataora_lav_prev_fin_min
	  , 0 as k_time_io_minute_min 
     ,now() k_dataora_lav_prev_fin_max
	  , 0 as k_time_io_minute_max 
    FROM pallet left outer join work_table on pallet.Pallet_Code = work_table.barcode  
	where pallet.data_uscita is null or pallet.data_uscita < :k_data
	order by fila, k_dataora_lav_ini

" arguments=(("k_data", date)) )
