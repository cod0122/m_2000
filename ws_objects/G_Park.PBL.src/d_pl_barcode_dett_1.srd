 retrieve="  SELECT barcode.pl_barcode,   
         barcode.pl_barcode_progr,   
         barcode.barcode,   
         barcode.tipo_cicli,   
         barcode.fila_1,   
         barcode.fila_2,   
         barcode.fila_1p,   
         barcode.fila_2p,   
         armo.dose,   
         (armo.peso_kg / armo.colli_2) as armo_peso_kg,   
         armo.larg_2,   
         armo.lung_2,   
         armo.alt_2,   
         armo.pedane,   
         armo.campione,   
         meca.area_mag,    
         meca.contratto,   
         contratti.mc_co,   
         contratti.sc_cf,   
         contratti.descr,   
         armo.cod_sl_pt,
         sl_pt.descr,   
         sl_pt.tipo_cicli,   
         sl_pt.fila_1,   
         sl_pt.fila_2,   
         sl_pt.fila_1p,   
         sl_pt.fila_2p,   
         armo.art,   
         prodotti.des,   
         barcode.num_int,   
         barcode.data_int,   
         meca.clie_3,   
         clienti.rag_soc_10,   
         meca.clie_2,   
         trim(clienti_2.rag_soc_10) || '  ' || meca.clie_2 k_ricevente,   
         barcode.data_sosp,   
         barcode.data_lav_ini,   
         barcode.data_lav_fin,   
         barcode.data_lav_ok,
         barcode.groupage,  
         barcode.x_datins,  
         barcode.x_utente,
         armo.id_armo  
    FROM (((((
         barcode LEFT OUTER JOIN meca ON 
         barcode.num_int = meca.num_int AND barcode.data_int = meca.data_int) 
         LEFT OUTER JOIN armo ON 
         barcode.id_armo = armo.id_armo) 
         LEFT OUTER JOIN contratti ON
         meca.contratto = contratti.codice) 
         LEFT OUTER JOIN sl_pt ON 
         armo.cod_sl_pt = sl_pt.cod_sl_pt) 
         LEFT OUTER JOIN prodotti ON 
         armo.art = prodotti.codice) 
         LEFT OUTER JOIN clienti ON 
         meca.clie_3 = clienti.codice  
         LEFT OUTER JOIN clienti as clienti_2 ON 
         meca.clie_3 = clienti_2.codice  
   WHERE pl_barcode = :k_pl_barcode 
         and (:k_groupage = '%' 
              or (:k_groupage = 'N' and 
                (barcode.barcode_lav is null or barcode.barcode_lav = '') )) 
   order by  barcode.pl_barcode_progr" update="barcode" updatewhere=0 updatekeyinplace=no arguments=(("k_pl_barcode", number),("k_groupage", string)) )
