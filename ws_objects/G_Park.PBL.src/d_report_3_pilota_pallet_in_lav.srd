 retrieve="  SELECT   
         pallet.Pallet_Code,   
         pallet.F1AVP,   
         pallet.F2AVP,   
         pallet.F1APP,   
         pallet.F2APP,   
         pallet.Fase  
		,work_table.nn
        , pallet.N_ordine  
	   , ~" ~" as k_consegna_data
	   , 0 as num_int
	   , date(0) as data_int 
	   , date(0) as data_int 
	   , 0 as id_meca 
	   , 0 as k_clie_2 
	   , ~" ~" as k_rag_soc_10 
	   , ~" ~" as k_colli
    FROM pallet left outer join work_table on pallet.Pallet_Code = work_table.barcode  
	where pallet.data_uscita is null or pallet.data_uscita < :k_data
	order by pallet.Pallet_Code
" arguments=(("k_data", date)) )
