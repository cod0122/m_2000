 retrieve="  SELECT   clienti.codice ,
			clienti.stato,
			clienti.id_clie_settore,
			clienti.id_clie_classe,
             clienti.rag_soc_10 as  rag_soc_10,
             clienti.rag_soc_11 as  rag_soc_11,
             clienti.indi_1 as      indi_1,
           clienti.cap_1 ,
             clienti.loc_1 as       loc_1,
           clienti.prov_1 ,
           clienti.zona ,
           clienti.p_iva ,
           clienti.cf,
             clienti.fono as        fono,
             clienti.fax as         fax, 
           clienti.id_nazione_1 ,
           nazioni.nome ,
           nazioni.area ,
		clie_settori.descr,
		clie_classi.descr,
           clienti.x_datins,
           clienti.x_utente
        FROM ((clienti  left outer join nazioni on   clienti.id_nazione_1 = nazioni.id_nazione )    
                             left outer join pagam on   clienti.cod_pag = pagam.codice )
                             left outer join iva on   clienti.iva = iva.codice 
                             left outer join clie_settori on   clienti.id_clie_settore = clie_settori.id_clie_settore 
                             left outer join clie_classi on   clienti.id_clie_classe = clie_classi.id_clie_classe 
        WHERE ( clienti.codice = :k_id_cliente )   
" arguments=(("k_id_cliente", number)) )
