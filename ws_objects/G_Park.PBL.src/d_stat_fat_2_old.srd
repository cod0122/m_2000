 retrieve="  SELECT s_arfa.clie_3,   
         clienti.rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         s_arfa.gruppo,   
         gru.des,   
         s_arfa.dose,   
         sum(s_arfa.prezzo_t)/1000,   
         sum(s_arfa.colli),   
         sum(s_arfa.cub_tot) as k_mc_out  
FROM ( s_arfa LEFT outer JOIN gru 
	ON s_arfa.gruppo = gru.codice ) 
	 LEFT outer JOIN clienti 
	ON s_arfa.clie_3 = clienti.codice 
		group by 
  						 s_arfa.clie_3,   
			          clienti.rag_soc_10,   
			          clienti.loc_1,   
			          clienti.prov_1,   
			          s_arfa.gruppo,   
			          gru.des,   
			          s_arfa.dose 
		ORDER BY clienti.rag_soc_10 ASC, 
					s_arfa.gruppo asc,  
					s_arfa.dose asc 
" arguments=(("k_id_gruppo", number),("k_id_cliente", number),("k_dose", number),("k_data_da", date),("k_data_a", date)) )
