 retrieve="  SELECT
		CASE
			WHEN  memo_utenti.stato < 6
				THEN 0
			WHEN  memo_utenti.stato > 5
				THEN 1
			ELSE
				0
		END as stato,
         memo.id_memo,   
         memo_utenti.id_memo_utente,  
         clienti.codice as id_cliente,   
         nvl(clienti.rag_soc_10,'') || ' ' || nvl(clienti.rag_soc_11,'') as rag_soc,   
         memo.titolo,   
         memo.note,   
         memo.tipo_sv,   
         memo.x_datins,   
         memo.x_utente  
    FROM clienti LEFT OUTER JOIN clienti_memo on clienti.codice = clienti_memo.id_cliente 
          LEFT OUTER join memo on clienti_memo.id_memo = memo.id_memo 
          LEFT OUTER join memo_utenti  on  clienti_memo.id_memo = memo_utenti.id_memo
   WHERE  clienti.codice = :k_id_cliente and memo_utenti.id_sr_utente = :k_id_sr_utente
ORDER BY memo_utenti.stato ASC,   
         memo_utenti.id_memo ASC   
" arguments=(("k_id_cliente", number),("k_id_sr_utente", number)) )
