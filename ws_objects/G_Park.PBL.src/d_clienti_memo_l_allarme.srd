 column=(type=char(61) updatewhereclause=yes name=rag_soc dbname="rag_soc" )
 column=(type=char(40) updatewhereclause=yes name=memo_titolo dbname="memo.titolo" )
 column=(type=char(120) updatewhereclause=yes name=memo_note dbname="memo.note" )
 column=(type=char(3) updatewhereclause=yes name=tipo_sv dbname="memo.tipo_sv" )
 column=(type=datetime updatewhereclause=yes name=x_datins dbname="memo.x_datins" )
