 retrieve="  SELECT i.clie_3 as clie_3,   
         clienti.rag_soc_10, 
         (NVL( i.colli_trattati,0)) as colli_2,   
         (NVL(s_artr.m_cubi,0))      as m_cubi,   
         (NVL(s_artr.giri_f1_pl,0))  as giri_f1_pl,   
         (NVL(s_artr.giri_f1_lav,0)) as giri_f1_lav,   
         (NVL(s_artr.giri_f2_pl,0))  as giri_f2_pl,   
         (NVL(s_artr.giri_f2_lav,0)) as giri_f2_lav,   
         (NVL(s_artr.giri_f1_pl_gp,0))  as giri_f1_pl_gp,   
         (NVL(s_artr.giri_f1_lav_gp,0)) as giri_f1_lav_gp,   
         (NVL(s_artr.giri_f2_pl_gp,0))  as giri_f2_pl_gp,   
         (NVL(s_artr.giri_f2_lav_gp,0)) as giri_f2_lav_gp,   
         (NVL(s_artr.id_armo_contati,0)) as id_armo_contati,   
         (NVL(s_artr.pedane_trattate,0))  as pedane_trattate,
         (i.pedane )  as pedane,
         (i.imp_trattati )  as imp_periodo,
         (i.imp_trattati - i.imp_armo_fatt)        as imp_da_fatt, 
         (i.colli_fatt)       as colli_fatt,
         (i.colli_da_fatt)  as colli_da_fatt
       , (i.imp_x_pedana)  as imp_x_pedana
	   ,s_artr.somma_giri     as somma_giri
	   ,0 as dettaglio
	  ,i.imp_fatturati as imp_fatt
    FROM vx_MAST3_stat_artr as s_artr 
                INNER JOIN vx_MAST3_stat_imp2 as i ON 
         s_artr.clie_3 = i.clie_3   
                LEFT OUTER JOIN clienti ON  
         s_artr.clie_3 = clienti.codice    
"  sort="rag_soc_10 A " )
