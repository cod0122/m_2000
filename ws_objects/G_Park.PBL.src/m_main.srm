event clicked;//
kuf_pilota_cmd kuf1_pilota_cmd

		
//--- apre la connessione al DB PILOTA
try
	kuf1_pilota_cmd = create kuf_pilota_cmd

	if messagebox ("Genera File Piano di Lavorazione", &
						"L'operazione non e' distruttiva~n~r" &
						+ "Il file di Pianificazione sara' prodotto solo se:~n~r" &
						+"     1-l'ultima Richiesta e' gia' stata esitata positivamente~n~r" &
						+"     2-almeno un Piano di Lavoro nello stato di 'Chiuso' (ovvero ancora da Inviare)~n~r" &
						+" ~n~r" &
						+ "Vuoi proseguire con l'operazione?" &
						 ,question!, YesNo!, 2) = 1 then

		if kuf1_pilota_cmd.job_pianificazione_lavorazioni() then
			kuf1_pilota_cmd.get_pilota_cfg()
			messagebox ("Genera File Piano di Lavorazione", &
					"L'operazione e' conclusa correttamente.~n~r" &
					+ "E' stato generato il file con il Piano di Lavoro nella cartella:~n~r" &
					+ kuf1_pilota_cmd.kist_tab_pilota_cfg.path_file_pl_barcode &
					+ kuf1_pilota_cmd.kist_tab_pilota_cfg.ultimo_nome_file_pl_barcode &
					 ,Information!) 
		else
			messagebox ("Genera File Piano di Lavorazione", &
					"Operazione conclusa.~n~rNessun Piano Inviato al Pilota, per uno dei seguenti motivi:~n~r" &
						+ "   1-Nessun Piano nello stato di Chiuso~n~r" &
						+ "   2-Piano non ancora esitato dal Pilota e ancora valido (non scaduto)~n~r" &
						+" ~n~r" &
					 ,Information!) 
		end if
	
	end if
	
catch (uo_exception kuo_exception)
	kuo_exception.messaggio_utente()

finally
	destroy kuf1_pilota_cmd

end try

end event

