event clicked;//
int k_importati=0
kuf_pl_barcode kuf1_pl_barcode

		
//--- apre la connessione al DB PILOTA
try
	kuf1_pl_barcode = create kuf_pl_barcode

	if messagebox ("Importa Barcode Trattati", &
						"L'operazione AGGIORNA gli Archivi!!~n~r" &
						+ "Saranno importati i dati di lavorazione dal NUOVO Pilota." &
						+" ~n~r" &
						+ "Vuoi proseguire con l'operazione?" &
						 ,question!, YesNo!, 2) = 1 then

		k_importati = kuf1_pl_barcode.importa_trattati_pilota("0") 
		if k_importati > 0 then
			messagebox ("Importa Barcode Trattati", &
					"L'operazione e' conclusa correttamente.~n~r" &
					+ "Sono stati importati dal Pilota i dati per "  &
					+ string(k_importati) &
					+ " Barcode. " &
					 ,Information!) 
		else
			messagebox ("Importa Barcode Trattati", &
					"Operazione conclusa.~n~rNessun dato importato dal Pilota.~n~r" &
						+ "Nessun Barcode ha concluso il Trattamento~n~r" &
						+" ~n~r" &
					 ,Information!) 
		end if
	
	end if
	
catch (uo_exception kuo_exception)
	kuo_exception.messaggio_utente()

finally
	destroy kuf1_pl_barcode

end try

end event

