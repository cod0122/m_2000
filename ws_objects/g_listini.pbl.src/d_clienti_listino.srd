 retrieve="  SELECT listino.cod_cli,
         (select  clienti.rag_soc_10 from clienti where  listino.cod_cli = clienti.codice) as rag_soc_10,   
         listino.cod_art,   
         (select prodotti.des_mkt from prodotti where  listino.cod_art = prodotti.codice) as prodotti_des,   
         (select prodotti.gruppo from prodotti where  listino.cod_art = prodotti.codice) as prodotti_gruppo,   
         listino.contratto,   
         contratti.mc_co,   
         contratti.sc_cf,   
         contratti.sl_pt,   
         listino.dose,   
         listino.magazzino,   
         listino.mis_x,   
         listino.mis_y,   
         listino.mis_z,   
         listino.occup_ped,   
         listino.peso_kg,   
         listino.campione,   
         listino.tipo,   
         listino.prezzo,  
         listino.attivo
    FROM (listino  
          left outer join contratti on 
          listino.contratto = contratti.codice) 
	 where (listino.cod_cli = :k_id_cliente or :k_id_cliente = 0)
	 	    and (listino.cod_art = :k_cod_art or :k_cod_art = '*') 
             and listino.attivo = 'S'
    order by  listino.cod_art, listino.cod_cli, listino.contratto
 
" arguments=(("k_id_cliente", number),("k_cod_art", string)) )
