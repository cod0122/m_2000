 retrieve="  SELECT listino.cod_cli,   
         trim(listino.cod_art) as  cod_art,   
         listino.magazzino,   
         listino.contratto,   
         listino.campione,   
         listino.dose,   
         listino.mis_x,   
         listino.mis_y,   
         listino.mis_z,   
         listino.m_cubi_f,   
         listino.occup_ped,   
         listino.travaso,   
         listino.peso_kg,   
         listino.tipo,   
         listino.prezzo,   
         listino.attivo  
         ,listino.x_datins  
         ,listino.x_utente  
         ,listino.id
		,listino.prezzo_2	
		,listino.prezzo_3	
		,listino.id_cond_fatt_1    	
		,listino.id_cond_fatt_2	   	
		,listino.id_cond_fatt_3   	
		,cf1.descr as cf1_descr
		,cf2.descr as cf2_descr
		,cf3.descr as cf3_descr
		,prodotti.des_mkt as cod_art_des
        ,listino.attiva_listino_pregruppi
         ,listino.id_parent
    FROM listino 
         left outer join prodotti on listino.cod_art = prodotti.codice 
         left outer join cond_fatt as cf1 on listino.id_cond_fatt_1 = cf1.id  
         left outer join cond_fatt as cf2 on listino.id_cond_fatt_2 = cf2.id  
         left outer join cond_fatt as cf3 on listino.id_cond_fatt_3 = cf3.id  
	WHERE ( listino.id = :k_id )
            
          
" update="listino" updatewhere=0 updatekeyinplace=no arguments=(("k_id", number)) )
