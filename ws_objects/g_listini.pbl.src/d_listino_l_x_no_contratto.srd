 retrieve="select
		 LISTINO.MAGAZZINO,
		 LISTINO.COD_ART,
		 LISTINO.COD_CLI,
		 PRODOTTI.DES,
		 LISTINO.DOSE,
		 LISTINO.MIS_Z as larg,
		 LISTINO.MIS_X  as lung,
		 LISTINO.MIS_Y as alt,
		 LISTINO.OCCUP_PED,
		 LISTINO.CAMPIONE,
		 LISTINO.PESO_KG,
		 LISTINO.ID
	from  LISTINO inner join PRODOTTI on LISTINO.COD_ART = PRODOTTI.CODICE
	where  LISTINO.ATTIVO = ~"S~" and (LISTINO.CONTRATTO = 0 or LISTINO.CONTRATTO is null)   
			and (LISTINO.COD_CLI = :K_CLIE_3 OR LISTINO.COD_CLI = 0)
	order by LISTINO.MAGAZZINO, LISTINO.COD_ART

" arguments=(("k_clie_3", number)) )
