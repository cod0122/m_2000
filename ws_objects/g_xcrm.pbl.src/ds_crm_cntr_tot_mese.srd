 retrieve="  SELECT   
         listino.cod_cli as id_cliente,   
         listino.contratto as id_confermaordine, 
         month(s_armo.data_int) mese,   
         year(s_armo.data_int) anno,   
         sum(s_armo.colli_2) as colli,   
         sum(s_armo.pedane) as pedane,   
         sum(s_armo.imp_fatt) as importo_fatturato,   
         sum(s_armo.imp_da_fatt) as importo_da_fatturare 
    FROM s_armo inner join armo on s_armo.id_armo = armo.id_armo    
              inner join listino on   armo.id_listino = listino.id
   WHERE s_armo.id_armo > 111811
	group by 1, 2, 3, 4


" )
