table(column=(type=long updatewhereclause=yes identity=yes name=id_listino dbname="listino.id" dbalias=".id" )
 column=(type=long updatewhereclause=yes name=id_cliente dbname="listino.cod_cli" dbalias=".cod_cli" )
 column=(type=char(12) updatewhereclause=yes name=cod_art dbname="listino.cod_art" dbalias=".cod_art" )
 column=(type=decimal(1) updatewhereclause=yes name=dose dbname="listino.dose" dbalias=".dose" )
 column=(type=decimal(2) updatewhereclause=yes name=prezzo dbname="v_crm_listino_prezzi.prezzo" dbalias=".prezzo" )
 column=(type=long updatewhereclause=yes name=id_listino_voce dbname="v_crm_listino_prezzi.id_listino_voce" dbalias=".id_listino_voce" )
 column=(type=char(80) updatewhereclause=yes name=voce_decriz dbname="v_crm_listino_prezzi.voce_decriz" dbalias=".voce_decriz" )
 column=(type=long updatewhereclause=yes name=id_cond_fatt dbname="v_crm_listino_prezzi.id_cond_fatt" dbalias=".id_cond_fatt" )
 column=(type=char(80) updatewhereclause=yes name=cond_fatt_descriz dbname="v_crm_listino_prezzi.cond_fatt_descriz" dbalias=".cond_fatt_descriz" )
 column=(type=char(1) updatewhereclause=yes name=campione dbname="listino.campione" dbalias=".campione" )
 column=(type=char(126) updatewhereclause=yes name=misure dbname="misure" )
 column=(type=long updatewhereclause=yes name=occup_ped dbname="listino.occup_ped" dbalias=".occup_ped" )
 column=(type=long updatewhereclause=yes name=magazzino dbname="listino.magazzino" dbalias=".magazzino" )
 column=(type=long updatewhereclause=yes name=id_confermaordine dbname="listino.id_confermaordine" dbalias=".id_confermaordine" )
 retrieve="  SELECT distinct listino.id,   
         listino.cod_cli,   
         listino.cod_art,   
         listino.dose,   
         v_crm_listino_prezzi.prezzo,   
         v_crm_listino_prezzi.id_listino_voce,   
         v_crm_listino_prezzi.voce_decriz,   
         v_crm_listino_prezzi.id_cond_fatt,   
         v_crm_listino_prezzi.cond_fatt_descriz,   
         listino.campione
         ,	case    
				when listino.mis_x > 0 then   trim(listino.mis_x + ' x ' + listino.mis_y + ' x ' + listino.mis_z)
				else  ' '
			end  as misure,
         listino.occup_ped,   
         listino.magazzino,   
         listino.contratto  as id_confermaordine
    FROM listino,   
         v_crm_listino_prezzi  
   WHERE  listino.id = v_crm_listino_prezzi.id_listino  and  
           listino.attivo = 'S'  and listino.cod_cli > 0
   order by  listino.id" )
