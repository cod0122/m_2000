 column=(type=char(12) updatewhereclause=yes name=art dbname="contratti_co.art" dbalias=".art" )
 column=(type=decimal(0) updatewhereclause=yes name=cod_gruppo dbname="contratti_co.cod_gruppo" dbalias=".cod_gruppo" )
 column=(type=char(80) updatewhereclause=yes name=nome_contatto dbname="contratti_co.nome_contatto" dbalias=".nome_contatto" )
 column=(type=number updatewhereclause=yes name=prezzo dbname="prezzo" )
 column=(type=char(512) updatewhereclause=yes name=prezzi dbname="prezzi" )
 column=(type=char(32) updatewhereclause=yes name=unita_misura dbname="contratti_co.unita_misura" dbalias=".unita_misura" )
 column=(type=char(255) updatewhereclause=yes name=misure_varie dbname="misure_varie" )
 column=(type=char(512) updatewhereclause=yes name=note dbname="note" )
 column=(type=char(80) updatewhereclause=yes name=contratti_des dbname="contratti_co.contratti_des" dbalias=".contratti_des" )
 column=(type=char(16) updatewhereclause=yes name=sk_dose_map_codice dbname="contratti_co.sk_dose_map_codice" dbalias=".sk_dose_map_codice" )
 column=(type=char(30) updatewhereclause=yes name=sk_dose_map_des dbname="contratti_co.sk_dose_map_des" dbalias=".sk_dose_map_des" )
 column=(type=char(255) updatewhereclause=yes name=altre_condizioni dbname="contratti_co.altre_condizioni" dbalias=".altre_condizioni" )
 column=(type=char(80) updatewhereclause=yes name=consegna_des dbname="contratti_co.consegna_des" dbalias=".consegna_des" )
 column=(type=char(80) updatewhereclause=yes name=ritiro_des dbname="contratti_co.ritiro_des" dbalias=".ritiro_des" )
 column=(type=char(255) updatewhereclause=yes name=note_interne dbname="contratti_co.note_interne" dbalias=".note_interne" )
 retrieve=" SELECT 
         contratti_co.id_cliente ,   
         contratti_co.id_contratto_co as id_contratto,   
         'CO' as tipo_contratto,
         contratti_co.id_contratto_co + 700000 as id_tipocontratto,   
         contratti_co.anno,   
         contratti_co.attivo,   
         contratti_co.magazzino,   
         contratti_co.art,   
         contratti_co.offerta_data,   
         contratti_co.stato,   
         contratti_co.data_stampa,   
         contratti_co.data_inizio,   
         contratti_co.data_fine,   
         contratti_co.oggetto,   
         contratti_co.id_clie_settore,   
         contratti_co.art,   
         contratti_co.gruppo as cod_gruppo,   
         contratti_co.nome_contatto, 
 		COALESCE(contratti_co.prezzo_1,0) as prezzo,  
		'prezzo 1 € ' + trim(COALESCE(contratti_co.prezzo_1,'0')) + '; prezzo 2 € ' + trim(COALESCE(contratti_co.prezzo_2,'0')) as prezzi,
          contratti_co.unita_misura,
         'dose ' + COALESCE( contratti_co.dose, '') + ' misure principali ' + trim(COALESCE(contratti_co.mis_x_1,'0')) + 'x' + trim(COALESCE(contratti_co.mis_y_1,'0')) + 'x' + trim(COALESCE(contratti_co.mis_z_1,'0')) + ' qta= ' +  trim(COALESCE(contratti_co.qtax_1,'0'))  + ' peso max kg= ' +  trim(COALESCE(contratti_co.peso_max_kg,'0'))  + ' seconda misura ' + trim(COALESCE(contratti_co.mis_x_2,'0')) + 'x' + trim(COALESCE(contratti_co.mis_y_2,'0')) + 'x' + trim(COALESCE(contratti_co.mis_z_2,'0')) + ' qta= ' +  trim(COALESCE(contratti_co.qtax_2,'0'))  + ' peso max kg= ' +  trim(COALESCE(contratti_co.peso_max_kg_2, '0')) as misure_varie,   
         contratti_co.note_qtax + ~" ~" + COALESCE(contratti_co.note_fatt,'')  + ~" ~" + COALESCE(contratti_co.note,'') as note,   
          contratti_co.contratti_des,
          contratti_co.sk_dose_map_codice,
          contratti_co.sk_dose_map_des,
          contratti_co.altre_condizioni,
          contratti_co.consegna_des,
          contratti_co.ritiro_des,
         contratti_co.note_interne   
    FROM contratti_co
union all
   select
         contratti_dp.id_cliente ,   
         contratti_dp.id_contratto_dp,
         'DP',
         contratti_dp.id_contratto_dp + 800000,   
         contratti_dp.anno,   
         contratti_dp.attivo,   
         contratti_dp.magazzino,   
         contratti_dp.art,   
         contratti_dp.offerta_data,   
         contratti_dp.stato,   
         contratti_dp.data_stampa,   
         contratti_dp.data_inizio,   
         contratti_dp.data_fine,   
         contratti_dp.oggetto,   
         contratti_dp.id_clie_settore,   
         contratti_dp.art,   
         contratti_dp.gruppo,   
         contratti_dp.nome_contatto,   
		COALESCE(contratti_dp.totale_offerta,0),
         'voci prezzi: ' + trim(descr_1) + ' € ' + trim(COALESCE(contratti_dp.voce_prezzo_1,'0')) + '; ' + trim(COALESCE(descr_2,'')) + ' € ' + trim(COALESCE(contratti_dp.voce_prezzo_2,'0')) + '; ' + trim(COALESCE(descr_3,'')) + ' € ' + trim(COALESCE(contratti_dp.voce_prezzo_3,'0')) + ' ; ' + trim(COALESCE(descr_4,'')) + ' € ' + trim(COALESCE(contratti_dp.voce_prezzo_4,'0')) + '; '+ trim(COALESCE(descr_5,'')) + ' € ' + trim(COALESCE(contratti_dp.voce_prezzo_5,'0')) + '...',   
          contratti_dp.unita_misura,
         trim(COALESCE(contratti_dp.mis_x_1,'0')) + 'x' + trim(COALESCE(contratti_dp.mis_y_1,'0')) + 'x' + trim(COALESCE(contratti_dp.mis_z_1,'0')) + ' qta= ' + trim(COALESCE(contratti_dp.qta_anno,'0')),   
         contratti_dp.note,   
          '',
          '',
          '',
          contratti_dp.altre_condizioni,
          contratti_dp.consegna_des,
          '',
         contratti_dp.note_interne   
    FROM contratti_dp   
union all
   select
         contratti_rd.id_cliente ,   
		contratti_rd.id_contratto_rd,
         'SD',
         contratti_rd.id_contratto_rd + 900000,   
         contratti_rd.anno,   
         'S',   
         contratti_rd.magazzino,   
         contratti_rd.art,   
         contratti_rd.offerta_data,   
         contratti_rd.stato,   
         contratti_rd.data_stampa,   
         contratti_rd.data_inizio,   
         contratti_rd.data_fine,   
         contratti_rd.oggetto,   
         contratti_rd.id_clie_settore,   
         contratti_rd.art,   
         contratti_rd.gruppo,   
         contratti_rd.nome_contatto,   
    	  COALESCE(contratti_rd.totale_contratto,0),
         'voci prezzi: ' + trim(COALESCE(contratti_rd.descr_1,'')) + ' € ' + trim(COALESCE(contratti_rd.voce_prezzo_1,'0')) + '; ' + trim(COALESCE(contratti_rd.descr_2,'')) + ' € ' + trim(COALESCE(contratti_rd.voce_prezzo_2,'0')) + '; ' + trim(COALESCE(contratti_rd.descr_3,'')) + ' € ' + trim(COALESCE(contratti_rd.voce_prezzo_3,'0')) + ' ; ' + trim(COALESCE(contratti_rd.descr_4,'')) + ' € ' + trim(COALESCE(contratti_rd.voce_prezzo_4,'0')) + '; '+ trim(COALESCE(contratti_rd.descr_5,'')) + ' € ' + trim(COALESCE(contratti_rd.voce_prezzo_5,'0')) + '...',   
          '',
          '',   
		  contratti_rd.note,   
          '',
          '',
          '',
          contratti_rd.altre_condizioni,
          '',
          '',
         contratti_rd.note_audit   
    FROM contratti_rd   
"  sort="id_cliente A id_contratto A " )
