 column=(type=long updatewhereclause=yes name=anno dbname="contratti_co.anno" dbalias=".anno" )
 column=(type=char(1) updatewhereclause=yes name=attivo dbname="contratti_co.attivo" dbalias=".attivo" )
 column=(type=long updatewhereclause=yes name=magazzino dbname="contratti_co.magazzino" dbalias=".magazzino" )
