 retrieve="  SELECT menu_window.descr,   
         menu_window.id,   
         menu_window.titolo,   
         menu_window.funzioni,   
         COALESCE(menu_window.attivo_data, convert(date,'01.01.1999')) as attivo_data ,   
         COALESCE(menu_window.attivo_su_commessa, '-') as attivo_su_commessa  
        ,(select COALESCE(count(*),0)  from sr_funzioni where menu_window.id = sr_funzioni.id_programma) kn_associazioni
    FROM menu_window  
 group by menu_window.descr,   
         menu_window.id,   
         menu_window.titolo,   
         menu_window.funzioni
       ,menu_window.attivo_data
       ,menu_window.attivo_su_commessa
" update="menu_window" updatewhere=0 updatekeyinplace=no  sort="attivo_data D attivo_su_commessa A id A funzioni A " )
