 retrieve="  SELECT sr_prof_funz.id_funzioni,   
         sr_utenti.id,   
         sr_utenti.codice,   
         sr_utenti.nome,   
         sr_utenti.x_datins,   
         sr_profili.id,   
         sr_profili.nome,   
         sr_funzioni.nome,   
         sr_funzioni.id_programma,  
         trim(sr_funzioni.descrizione) as sr_funzioni_descrizione 
    FROM sr_prof_utenti LEFT OUTER JOIN sr_profili ON sr_prof_utenti.id_profili = sr_profili.id,   
         sr_utenti,   
         sr_prof_funz,   
         sr_funzioni  
   WHERE ( sr_prof_utenti.id_profili = sr_prof_funz.id_profili ) and  
         ( sr_prof_utenti.id_utenti = sr_utenti.id ) and  
         ( sr_funzioni.id = sr_prof_funz.id_funzioni ) and  
         ( ( sr_prof_funz.id_funzioni = :k_id_funzione ) or (:k_id_funzione = 0 and sr_utenti.id = :k_id_utente )   )
 
" arguments=(("k_id_funzione", number),("k_id_utente", number))  sort="sr_funzioni_descrizione A sr_profili_id A " )
