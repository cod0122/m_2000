 retrieve="  SELECT sr_utenti.codice,   
         sr_profili.id,   
         sr_profili.nome,   
         sr_profili.descrizione,   
         sr_profili.stato,   
         sr_prof_utenti.x_datins,   
         sr_prof_utenti.x_utente,  
         sr_prof_utenti.id 
    FROM sr_prof_utenti LEFT OUTER JOIN sr_profili ON 
         sr_prof_utenti.id_profili = sr_profili.id 
                        LEFT OUTER JOIN sr_utenti ON 
         sr_prof_utenti.id_utenti = sr_utenti.id
   WHERE (sr_prof_utenti.id_utenti = :id_utenti OR  
         :id_utenti = 0)   
   order by sr_utenti.codice,
            sr_profili.nome
" arguments=(("id_utenti", number)) )
