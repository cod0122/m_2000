table(column=(type=number updatewhereclause=yes name=wadoco dbname="f4801_adt.wadoco" dbalias="f4801.wadoco" )
 column=(type=number updatewhereclause=yes name=f4211_e1rorn dbname="f4211_e1rorn" )
 retrieve=" select f4801.wadoco as wadoco
-- 	   ,0 as F4211_e1rorn
 	   ,to_number(nvl(f4801.warorn,'0')) as F4211_e1rorn
-- 	   ,f4801.warorn as F4211_e1rorn
from F4801_ADT  f4801  inner join F3111_ADT  f3111 on 
          					f4801.wadoco = f3111.wmdoco 
											inner join F4108_adt  f4108 on
          					f3111.wmlotn = f4108.iolotn 
where 
   (:k_apid = '' or f4801.waapid = :k_apid)
  and f3111.wmlnty = 'S' 
  and f4801.wasrst in ('08', '20')
  and f4801.wamcu = :k_mcu
  and f4108.iomcu = :k_mcu
  and f3111.wmcpil > ' '
group by f4801.wadoco
 	   ,f4801.warorn
union 
  select 0 as wadoco,
       sddoco as E1_SO_Number     -- numero del sales order
from F4211 inner join F4101 on 
                 sdlitm = imlitm 
             and imprp7 = 'DEPOT'
where sdnxtr = '532'
  and sdlttr != '980'
  and sdlnty = 'S'
  and sdmcu = :k_mcu
  and (:k_apid = '' or F4211.sddoco = :k_apid)
group by  0 ,
 	   F4211.sddoco
" arguments=(("k_apid", string),("k_mcu", string)) )
