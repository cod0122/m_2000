table(column=(type=number updatewhereclause=yes name=f4801_adt_wadoco dbname="f4801_adt.wadoco" dbalias="f4801.wadoco" )
 column=(type=number updatewhereclause=yes name=f4211_e1rorn dbname="f4211_e1rorn" )
 column=(type=number updatewhereclause=yes name=f4801_adt_waan8 dbname="f4801_adt.waan8" dbalias="f4801.waan8" )
 column=(type=decimal(0) updatewhereclause=yes name=f4801_adt_wadrqj dbname="f4801_adt.wadrqj" dbalias="f4801.wadrqj" )
 column=(type=char(25) updatewhereclause=yes name=f3111_adt_wmcpil dbname="f3111_adt.wmcpil" dbalias="f3111.wmcpil" )
 column=(type=char(30) updatewhereclause=yes name=f3111_adt_wmlotn dbname="f3111_adt.wmlotn" dbalias="f3111.wmlotn" )
 column=(type=char(30) updatewhereclause=yes name=f4108_adt_iolot1 dbname="f4108_adt.iolot1" dbalias="f4108.iolot1" )
 column=(type=char(30) updatewhereclause=yes name=f4108_adt_iolot2 dbname="f4108_adt.iolot2" dbalias="f4108.iolot2" )
 column=(type=char(30) updatewhereclause=yes name=f4801_adt_waapid dbname="f4801_adt.waapid" dbalias="f4801.waapid" )
 retrieve=" select f4801.wadoco  wadoco,
 	   to_number(nvl(f4801.warorn,'0'))  F4211_e1rorn,
       f4801.waan8  waan8,
       f4801.wadrqj  wadrqj,
       f3111.wmcpil  wmcpil,
       f3111.wmlotn  wmlotn,
       f4108.iolot1   iolot1,
       f4108.iolot2   iolot2,
       f4801.waapid  waapid
from PRODDTA.F4801_ADT  f4801  inner join PRODDTA.F3111_ADT  f3111 on 
          					f4801.wadoco = f3111.wmdoco 
											inner join PRODDTA.F4108_adt  f4108 on
          					f3111.wmlotn = f4108.iolotn 
where 
   (:k_apid = '' or f4801.waapid = :k_apid)
-- and f3111.WMDCTO = 'WO'
  and f3111.wmlnty = 'S' 
  and f4801.wasrst in ('08', '20')
  and f4801.wamcu = :k_mcu
  and f4108.iomcu = :k_mcu
  and f3111.wmcpil > ' '   
-- and f4108.iolot2 > ' '
group by  f4801.wadoco ,
 	    f4801.warorn,
       f4801.waan8 ,
       f4801.wadrqj ,
       f3111.wmcpil ,
       f3111.wmlotn ,
       f4108.iolot1  ,
       f4108.iolot2  ,
       f4801.waapid 
union 
  select 0 as wadoco,
       sddoco ,   -- numero del sales order
       sdan8  ,   -- codice cliente
       sddrqj ,   -- data della richiesta
       sdlitm ,   -- item -> si aggancia al listino
       sdlotn ,   -- barcode E1
       sdir02 ,   -- numero packing list
       sdir01 ,   -- barcode pallet sulla packing list
       sdir05     -- riferimento M2000
from PRODDTA.F4211 inner join PRODDTA.F4101 on 
                 sdlitm = imlitm 
             and imprp7 = 'DEPOT'
where sdnxtr = '532'
  and sdlttr != '980'
  and sdlnty = 'S'
  and sdmcu = :k_mcu
  and (:k_apid = '' or F4211.sdir05 = :k_apid)
 -- and sdir01 > ' '
group by  0 ,
 	   F4211.sddoco,
 	   F4211.sdan8,
 	   F4211.sddrqj,
 	   F4211.sdlitm,
 	   F4211.sdlotn,
 	   F4211.sdir02,
 	   F4211.sdir01,
 	   F4211.sdir05
" arguments=(("k_apid", string),("k_mcu", string))  sort="f3111_adt_wmlotn A " )
