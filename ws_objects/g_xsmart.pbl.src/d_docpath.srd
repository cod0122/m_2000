 retrieve="  SELECT docpath.id_docpath,   
         docpath.duplica,   
         trim(docpath.descr) as descr,   
         trim(docpath.path) as path,   
         docpath.x_datins,   
         docpath.x_utente,   
         docpath.id_doctipo,   
         doctipo.descr  
        , '. ' as k_path_root_doc
        , '. ' as k_path_root_doc_1
        , '. ' as k_path_root_doc_2
        ,trim(docpath.path_x_documentale) as path_x_documentale
    FROM docpath LEFT OUTER JOIN doctipo ON docpath.id_doctipo = doctipo.id_doctipo  
   WHERE docpath.id_docpath = :k_id_docpath    
" update="docpath" updatewhere=0 updatekeyinplace=no arguments=(("k_id_docpath", number)) )
