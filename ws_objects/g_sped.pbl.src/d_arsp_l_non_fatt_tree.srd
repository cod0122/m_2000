 retrieve="select 
		ARSP.NUM_BOLLA_OUT  as num_bolla_out,
		ARSP.DATA_BOLLA_OUT as data_bolla_out,
         arsp.id_arsp,
         arsp.id_armo,
		MECA.CLIE_1,
		MECA.CLIE_2,
		MECA.CLIE_3,
		c1.rag_soc_10 as mandante,
		c2.rag_soc_10 as ricevente,
		MECA.NUM_INT as num_int,
		MECA.DATA_INT as data_int,
		meca.id,
		armo.art,
		armo.dose,
         arsp.colli
	from ARSP inner join ARMO on
		ARSP.ID_ARMO   = ARMO.ID_ARMO
                     inner join MECA on
		armo.id_meca   = MECA.ID
                    inner join CLIENTI as c1 on
		MECA.CLIE_1  = c1.CODICE 
                     inner join CLIENTI as c2 on
		MECA.CLIE_2  = c2.CODICE 
	where
         ARSP.DATA_BOLLA_OUT > :k_data
         and meca.clie_3 = :k_clie_3
         and arsp.stampa = 'S'
         and (meca.aperto is null or meca.aperto <> 'N' )
	order by 
		ARSP.DATA_BOLLA_OUT
		,ARSP.NUM_BOLLA_OUT
" arguments=(("k_clie_3", number),("k_data", date)) )
