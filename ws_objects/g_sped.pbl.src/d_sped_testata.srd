 retrieve="  SELECT sped.num_bolla_out,   
         sped.data_bolla_out,   
         sped.clie_2,   
         sped.clie_2 as meca_clie_2,   
         sped.clie_3,   
         clienti.codice,   
         clienti.rag_soc_10,   
         clienti.rag_soc_10 as meca_rag_soc_10,     
         clienti.p_iva,   
         clienti.cf,   
         clienti.loc_1,   
         clienti.prov_1,   
         sped.id_nazione,   
         nazioni.nome,   
         trim(sped.rag_soc_1) as rag_soc_1,   
         trim(sped.rag_soc_2) as rag_soc_2,  
         trim(sped.indi) as indi,   
         trim(sped.loc) as loc,   
         sped.cap,   
         sped.prov,   
         sped.stampa,   
         sped.id_sped   
         ,(COALESCE(trim(clie3.rag_soc_10), '') + ' ' + COALESCE(trim(clie3.rag_soc_11), ' ')) as clie3_rag_soc 
         ,sped.data_rit
         ,sped.ora_rit
        ,trim(sped.note_1) as note_1
 	    ,sped.x_datins
       ,sped.x_utente
       ,getdate() as k_competenza_dal 
		,clienti.iva
           ,sped.cura_trasp
           ,sped.mezzo
           ,sped.caus_codice
           ,sped.causale
           ,sped.aspetto
           ,sped.porto
           ,sped.colli
           , 0 as id_vettore
			,trim(SPED.VETT_1) as vett_1
			,trim(SPED.VETT_2) as vett_2
          ,0 as k_tot_colli_out
          ,0 as k_nr_ind_ddt
          ,sped.sv_call_vettore as sv_call_vettore
	 , (select  top 1 id_memo from  meca_memo mm 
               where mm.id_meca in (select armo.id_meca from armo inner join arsp on armo.id_armo = arsp.id_armo 
                                                                              where arsp.id_sped = sped.id_sped) 
                                    and mm.allarme in ('A', 'T')
		      union all 
		     select  top 1 id_memo  from  clienti_memo cm  
       	        where (sped.clie_2 = cm.id_cliente or sped.clie_3 = cm.id_cliente) 
    		                      and cm.allarme in ('A', 'T')) as id_memo
    FROM sped 
						LEFT OUTER JOIN clienti ON sped.clie_2 = clienti.codice 
	                   LEFT OUTER JOIN nazioni ON sped.id_nazione = nazioni.id_nazione 
						LEFT OUTER JOIN clienti as clie3 ON sped.clie_3 = clie3.codice   
    where sped.id_sped = :k_id

" arguments=(("k_id", number)) )
