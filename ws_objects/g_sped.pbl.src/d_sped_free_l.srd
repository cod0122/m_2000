 column=(type=char(3) updatewhereclause=yes name=id_nazione_1 dbname="clienti.id_nazione_1" )
 column=(type=char(4000) updatewhereclause=yes name=causale dbname="v_sped_free.causale" )
 column=(type=char(1) updatewhereclause=yes name=stampa dbname="stampa" values="	F/	N/	S" )
 column=(type=datetime updatewhereclause=yes name=x_datins dbname="v_sped_free.x_datins" )
 column=(type=char(12) updatewhereclause=yes name=x_utente dbname="v_sped_free.x_utente" )
 column=(type=char(4000) updatewhereclause=yes name=trasporto dbname="v_sped_free.trasporto" values="	V/	0" )
 column=(type=char(4000) updatewhereclause=yes name=data_ora_rit dbname="data_ora_rit" )
 retrieve="  SELECT
         sped.num_bolla_out ,   
         sped.data_bolla_out,   
         sped.id_sped_free,   
         sped.clie_3 ,   
         sped.clie_2 ,   
         sped.intestazione,   
         sped.intestazione_ind1,   
         clienti.prov_1,   
         clienti.id_nazione_1,   
         sped.causale  
        ,COALESCE(stampa, 'N') as stampa
        ,sped.x_datins as x_datins
        ,sped.x_utente as x_utente
        ,sped.trasporto 
        ,(COALESCE(sped.data_ora_rit,'')) as data_ora_rit
  FROM v_sped_free as sped LEFT OUTER JOIN clienti ON sped.clie_2 = clienti.codice
  where sped.data_bolla_out between :k_data_ini and :k_data_fin
        and (:k_cliente = 0 or sped.clie_2 = :k_cliente)



" arguments=(("k_data_ini", date),("k_data_fin", date),("k_cliente", number))  sort="sped_free_data_bolla_out D id_sped_free D " )
