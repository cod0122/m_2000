 column=(type=long updatewhereclause=yes name=clie_3 dbname="v_sped_free.clie_3" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="v_sped_free.clie_2" )
 column=(type=char(4000) updatewhereclause=yes name=intestazione dbname="v_sped_free.intestazione" )
 column=(type=char(4000) updatewhereclause=yes name=intestazione_ind1 dbname="v_sped_free.intestazione_ind1" )
