 retrieve="  SELECT armo.art,   
         prodotti.des,   
         trim(arsp.note_1) as note_1,   
         trim(arsp.note_2) as note_2,   
         trim(arsp.note_3) as note_3,   
         arsp.colli,   
         arsp.colli as colli_orig,   
         0 as colli_da_sped_max,   
         arsp.peso_kg_out,   
         arsp.colli_out,   
         arsp.id_arsp,   
         armo.id_armo,
         arsp.stampa,
		ARMO.MAGAZZINO,
		ARMO.DOSE,
		ARMO.COLLI_2,
       ARMO.PESO_KG as peso_kg_in,
       sum(v_armo_colli_inout.colli_sped) as colli_spediti  
       ,sum(v_armo_colli_inout.colli_trattati) as colli_trattati
       ,sum(v_armo_colli_inout.colli_danontrattare) as colli_danontrattare  
       ,sum(v_armo_colli_inout.colli_fatt) as colli_fatturati  
		,0 as k_progressivo
       ,armo.id_meca
       ,armo.num_int
       ,armo.data_int
	   ,armo.alt_2
	  ,trim(ARMO_NT.NOTE_1) as ARMO_NT_NOTE_1
	  ,trim(ARMO_NT.NOTE_2) as ARMO_NT_NOTE_2
	  ,trim(ARMO_NT.NOTE_3) as ARMO_NT_NOTE_3
	  ,trim(ARMO_NT.NOTE_4) as ARMO_NT_NOTE_4
	  ,trim(ARMO_NT.NOTE_5) as ARMO_NT_NOTE_5
	  ,trim(ARMO_NT.NOTE_6) as ARMO_NT_NOTE_6
	  ,trim(ARMO_NT.NOTE_7) as ARMO_NT_NOTE_7
	  ,trim(ARMO_NT.NOTE_8) as ARMO_NT_NOTE_8
	  ,trim(ARMO_NT.NOTE_9) as ARMO_NT_NOTE_9
	  ,trim(ARMO_NT.NOTE_10) as ARMO_NT_NOTE_10
	   ,meca.e1doco
	   ,meca.e1rorn
		,COALESCE(meca.e1srst,'NC') as e1srst
       ,meca.data_ent
       FROM armo  INNER JOIN arsp ON armo.id_armo = arsp.id_armo 
                  INNER JOIN meca ON meca.id = armo.id_meca 
                  LEFT OUTER JOIN prodotti ON armo.art = prodotti.codice 
                  left outer join ARMO_NT on armo.id_armo = armo_nt.id_armo 
					inner join v_armo_colli_inout on armo.id_armo = v_armo_colli_inout.id_armo 
   WHERE (arsp.id_arsp = :k_id_arsp and armo.id_armo = :k_id_armo)   
 group by 
  	   armo.art,   
       prodotti.des,   
       arsp.note_1 ,
       arsp.note_2 , 
       arsp.note_3 ,
       arsp.colli,   
       arsp.peso_kg_out,   
       arsp.colli_out,   
         arsp.id_arsp,   
       armo.id_armo,
       arsp.stampa,
		ARMO.MAGAZZINO
		,ARMO.DOSE
		,ARMO.COLLI_2
       ,ARMO.PESO_KG 
       ,armo.id_meca
       ,armo.num_int
       ,armo.data_int
	   ,armo.alt_2
	  ,ARMO_NT.NOTE_1
	  ,ARMO_NT.NOTE_2
	  ,ARMO_NT.NOTE_3
	  ,ARMO_NT.NOTE_4
	  ,ARMO_NT.NOTE_5
	  ,ARMO_NT.NOTE_6
	  ,ARMO_NT.NOTE_7
	  ,ARMO_NT.NOTE_8
	  ,ARMO_NT.NOTE_9
	  ,ARMO_NT.NOTE_10
	   ,meca.e1doco
	   ,meca.e1rorn
		,meca.e1srst
      ,meca.data_ent
" update="arsp" updatewhere=0 updatekeyinplace=no arguments=(("k_id_arsp", number),("k_id_armo", number)) )
