 retrieve="  SELECT  
         arsp.num_bolla_out,   
         arsp.data_bolla_out,   
         sped.clie_2,   
         sped.clie_3,   
         clienti.rag_soc_10,   
         c3.rag_soc_10 as fatturato,   
         sped.data_rit,   
         sped.ora_rit,   
		sped.stampa,
		sped.rag_soc_1,
		sped.rag_soc_2,
		sped.indi,
		sped.cap,
		sped.loc,
		sped.id_nazione,
         sum(armo.colli_2) as colli_2,   
         sum(arsp.colli) as colli,     
         sum(arsp.colli_out) as colli_out
    FROM sped    inner JOIN arsp  
           ON sped.id_sped = arsp.id_sped
             	  inner JOIN armo 
           ON arsp.id_armo = armo.id_armo
                 LEFT OUTER JOIN clienti 
           ON sped.clie_2 = clienti.codice
                 LEFT OUTER JOIN clienti as c3 
           ON sped.clie_3 = c3.codice
   WHERE 
		 sped.id_sped = :k_id_sped
group by 
         arsp.num_bolla_out,   
         arsp.data_bolla_out,   
         sped.clie_2,   
         sped.clie_3,   
         clienti.rag_soc_10,   
         sped.data_rit,   
         sped.ora_rit,   
		c3.rag_soc_10,
		sped.stampa,
		sped.rag_soc_1,
		sped.rag_soc_2,
		sped.indi,
		sped.cap,
		sped.loc,
		sped.id_nazione

;
" arguments=(("k_id_sped", number)) )
