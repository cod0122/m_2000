table(column=(type=long updatewhereclause=yes name=num_bolla_out dbname="sped.num_bolla_out" dbalias=".num_bolla_out" )
 column=(type=date updatewhereclause=yes name=data_bolla_out dbname="sped.data_bolla_out" dbalias=".data_bolla_out" )
 column=(type=long updatewhereclause=yes identity=yes name=id_sped dbname="sped.id_sped" dbalias=".id_sped" )
 column=(type=long updatewhereclause=yes name=id_docprod_file dbname="sped.id_docprod" dbalias=".id_docprod" )
 column=(type=char(1) updatewhereclause=yes name=mezzo dbname="sped.mezzo" dbalias=".mezzo" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="sped.clie_3" dbalias=".clie_3" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="sped.clie_2" dbalias=".clie_2" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10 dbname="clienti.rag_soc_10" dbalias=".rag_soc_10" )
 column=(type=char(30) updatewhereclause=yes name=clienti_loc_1 dbname="clienti.loc_1" dbalias=".loc_1" )
 column=(type=char(2) updatewhereclause=yes name=clienti_prov_1 dbname="clienti.prov_1" dbalias=".prov_1" )
 column=(type=char(3) updatewhereclause=yes name=id_nazione_1 dbname="clienti.id_nazione_1" dbalias=".id_nazione_1" )
 column=(type=char(40) updatewhereclause=yes name=causale dbname="sped.causale" dbalias=".causale" )
 column=(type=char(1) updatewhereclause=yes name=stampa dbname="stampa" values="	F/	N/	S" )
 column=(type=datetime updatewhereclause=yes name=x_datins dbname="sped.x_datins" dbalias=".x_datins" )
 column=(type=char(12) updatewhereclause=yes name=x_utente dbname="sped.x_utente" dbalias=".x_utente" )
 column=(type=char(1) updatewhereclause=yes name=cura_trasp dbname="sped.cura_trasp" dbalias=".cura_trasp" values="	V/	0" )
 column=(type=date updatewhereclause=yes name=k_ritiro_data dbname="sped.k_ritiro_data" dbalias=".k_ritiro_data" )
 column=(type=char(8) updatewhereclause=yes name=k_ritiro_ora dbname="k_ritiro_ora" )
 column=(type=long updatewhereclause=yes name=id_memo dbname="id_memo" )
 retrieve="  SELECT
         sped.num_bolla_out ,   
         sped.data_bolla_out,   
         sped.id_sped,   
         sped.id_docprod,   
         sped.mezzo ,   
         sped.clie_3 ,   
         sped.clie_2 ,   
         clienti.rag_soc_10,   
         clienti.loc_1,   
         clienti.prov_1,   
         clienti.id_nazione_1,   
         sped.causale  
        ,COALESCE(stampa, ~"N~") as stampa
        ,sped.x_datins as x_datins
        ,sped.x_utente as x_utente
        ,sped.cura_trasp 
        ,sped.data_rit as k_ritiro_data
        ,(COALESCE(sped.ora_rit,'')) as k_ritiro_ora
	 , (select  top 1 coalesce(id_memo, 0) from  meca_memo mm 
               where mm.id_meca in (select armo.id_meca from armo inner join arsp on armo.id_armo = arsp.id_armo 
                                                                              where arsp.id_sped = sped.id_sped) 
                                    and mm.allarme in ('A', 'T')
		      union all 
		 select  top 1 coalesce(id_memo,0)  from  clienti_memo cm  
       	        where (sped.clie_2 = cm.id_cliente or sped.clie_3 = cm.id_cliente) 
    		                      and cm.allarme in ('A', 'T'))
            as id_memo
  FROM sped LEFT OUTER JOIN clienti ON sped.clie_2 = clienti.codice
  where sped.data_bolla_out between :k_data_ini and :k_data_fin
        and (:k_cliente = 0 or sped.clie_2 = :k_cliente)



" arguments=(("k_data_ini", date),("k_data_fin", date),("k_cliente", number))  sort="data_bolla_out D num_bolla_out D " )
