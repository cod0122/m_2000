 retrieve="  SELECT distinct 
			armo.art,
         arsp.num_bolla_out,   
         arsp.data_bolla_out,   
         sped.clie_2,   
         sped.clie_3,   
         clienti.rag_soc_10,   
         sped.id_sped,
         sped.data_rit,   
         sped.ora_rit,   
         arsp.colli,    
         arsp.colli_out,   
         trim (COALESCE(arsp.note_1, '') + ' ' + COALESCE(arsp.note_2, '') + ' ' + COALESCE(arsp.note_3,'')) as note, 
         armo.cod_sl_pt,   
         armo.colli_2   
    FROM ((armo INNER JOIN  arsp on
           armo.id_armo = arsp.id_armo)
            LEFT OUTER JOIN sped  
           ON arsp.num_bolla_out = sped.num_bolla_out and arsp.data_bolla_out = sped.data_bolla_out)
            LEFT OUTER JOIN clienti 
           ON sped.clie_2 = clienti.codice    
   WHERE
          armo.id_meca = :k_id 
order by armo.art" arguments=(("k_id", number)) )
