 retrieve="  SELECT armo.art,   
         prodotti.des,   
         trim(arfa.des) as des,   
         arfa.colli,   
         arfa.prezzo_u,   
         arfa.iva,   
         arfa.prezzo_t,   
         iva.des,
		arfa.tipo_riga,  
         arfa.colli_out,   
         arfa.tipo_l,
         arfa.id_armo,
         arfa.id_armo_prezzo,
		0.00 as k_imp_iva,
		0.00 as k_imp_tot,
		0 as k_progressivo
       FROM arfa  LEFT OUTER JOIN armo ON arfa.id_armo = armo.id_armo LEFT OUTER JOIN prodotti ON armo.art = prodotti.codice LEFT OUTER JOIN iva ON arfa.iva = iva.codice  
   WHERE arfa.id_arfa = :kid_riga    
" arguments=(("kid_riga", number)) )
