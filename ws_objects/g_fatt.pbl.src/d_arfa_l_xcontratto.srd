table(column=(type=long updatewhereclause=yes name=id_fattura dbname="prodotti.id_fattura" dbalias=".id_fattura" )
 column=(type=char(12) updatewhereclause=yes name=art dbname="prodotti.art" dbalias=".art" )
 column=(type=char(40) updatewhereclause=yes name=des dbname="prodotti.des" dbalias=".des" )
 column=(type=date updatewhereclause=yes name=data_fatt dbname="prodotti.data_fatt" dbalias=".data_fatt" )
 column=(type=long updatewhereclause=yes name=num_fatt dbname="prodotti.num_fatt" dbalias=".num_fatt" )
 column=(type=decimal(0) updatewhereclause=yes name=arfa_colli dbname="prodotti.colli" dbalias=".colli" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_out dbname="prodotti.colli_out" dbalias=".colli_out" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="prodotti.clie_3" dbalias=".clie_3" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10 dbname="clienti.rag_soc_10" dbalias=".rag_soc_10" )
 retrieve="  SELECT id_fattura,
         armo.art,
         prodotti.des,
		 arfa.data_fatt,   
         arfa.num_fatt,   
         arfa.colli,
         arfa.colli_out,
         arfa.clie_3,   
         clienti.rag_soc_10   
    FROM (armo INNER JOIN  arfa on
           armo.id_armo = arfa.id_armo)
                LEFT OUTER JOIN prodotti ON 
           armo.art = prodotti.codice
                LEFT OUTER JOIN clienti ON 
           arfa.clie_3 = clienti.codice   
   WHERE 
          armo.id_listino in
			(select id
               from listino
               where listino.contratto = :k_contratto)
union all
  SELECT  id_fattura,
         '' as art,
         arfa_v.comm,
		  arfa_v.data_fatt,   
         arfa_v.num_fatt,   
         arfa_v.colli,
         arfa_v.colli as colli_out,
         arfa_v.clie_3,   
         clienti.rag_soc_10   
    FROM arfa_v LEFT OUTER JOIN clienti ON 
           arfa_v.clie_3 = clienti.codice   
   WHERE
         arfa_v.contratto = :k_contratto
order by 3, 4
" arguments=(("k_contratto", number)) )
