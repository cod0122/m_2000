table(column=(type=long update=yes updatewhereclause=yes key=yes name=num_fatt dbname="prof.num_fatt" )
 column=(type=date update=yes updatewhereclause=yes key=yes name=data_fatt dbname="prof.data_fatt" )
 column=(type=char(1) update=yes updatewhereclause=yes key=yes name=flag dbname="prof.flag" values="partita (conto cliente)	A/contropartita (conto ricavi)	C/" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=conto dbname="prof.conto" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=s_conto dbname="prof.s_conto" )
 column=(type=decimal(3) update=yes updatewhereclause=yes name=importo dbname="prof.importo" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=iva dbname="prof.iva" )
 column=(type=char(2) update=yes updatewhereclause=yes name=tipo_doc dbname="prof.tipo_doc" values="Fattura	FT/Nota di Credito	NC/Autofattura	AF/Proforma	PF/Altro	/" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=cod_pag dbname="prof.cod_pag" )
 column=(type=char(1) update=yes updatewhereclause=yes name=profis_1 dbname="prof.profis" values="Non Trasferito	N/Trasferito	S/	/" )
 column=(type=char(11) update=yes updatewhereclause=yes name=p_iva dbname="prof.p_iva" )
 column=(type=long updatewhereclause=yes identity=yes name=id_cliente dbname="clienti.codice" )
 column=(type=long updatewhereclause=yes name=id_fattura dbname="prof.id_fattura" )
 retrieve="  SELECT prof.num_fatt,   
         prof.data_fatt,   
         prof.flag,   
         prof.conto,   
         prof.s_conto,   
         prof.importo,   
         prof.iva,   
         prof.tipo_doc,   
         prof.cod_pag,   
         prof.profis,   
         prof.p_iva,
         clienti.codice,
         prof.id_fattura   
    FROM prof   
				  LEFT OUTER JOIN clienti ON 
                    prof.p_iva <> ' ' and prof.p_iva = clienti.p_iva 
where data_fatt = :k_data_fatt
and num_fatt = :k_num_fatt 
and importo = :k_importo
and conto = :k_conto
and s_conto = :k_s_conto" update="prof" updatewhere=0 updatekeyinplace=no arguments=(("k_num_fatt", number),("k_data_fatt", date),("k_importo", number),("k_conto", number),("k_s_conto", number)) )
