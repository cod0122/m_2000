 retrieve="SELECT 
		ric.tipo, 
		ric.dist, 
		ric.scad, 
		ric.data_fatt, 
		ric.num_fatt, 
		ric.importo, 
		ric.rata, 
		ric.clie, 
		clienti.rag_soc_10, 
		clienti.indi_1, 
		clienti.loc_1, 
		clienti.prov_1, 
		clienti.cap_1,
		clienti.p_iva,
		clienti.banca,
		clienti.abi as cli_abi,
		clienti.cab as cli_cab,
      clienti.tipo_banca,
      banche.descrizione,
      banche.abi,
      banche.cab,
      banche.conto, 
		ric.flag_st,
		ric.data_st 
	FROM (ric LEFT JOIN clienti ON ric.clie = clienti.codice)
	      LEFT JOIN banche ON ric.tipo = banche.codice
	order by 
			ric.tipo, 
			ric.dist, 
			ric.scad, 
			clienti.rag_soc_10, 
			ric.data_fatt, 
			ric.num_fatt"  sort="tipo A tipo A scad A clie A " )
