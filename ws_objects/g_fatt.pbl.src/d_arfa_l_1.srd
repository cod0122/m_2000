 retrieve="  SELECT armo.art,
			arfa.data_fatt,   
         arfa.num_fatt,   
         arfa.colli,
         arfa.colli_out,
         arfa.clie_3,   
         clienti.rag_soc_10,
         id_fattura   
    FROM (armo INNER JOIN  arfa on
           armo.id_armo = arfa.id_armo)
                LEFT OUTER JOIN clienti ON 
           arfa.clie_3 = clienti.codice   
   WHERE
          armo.id_meca = :k_id 
			order by armo.art	 
" arguments=(("k_id", number)) )
