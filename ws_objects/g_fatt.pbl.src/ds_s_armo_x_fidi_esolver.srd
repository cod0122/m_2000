table(column=(type=long updatewhereclause=yes name=num_int dbname="s_armo.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="s_armo.data_int" dbalias=".data_int" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="s_armo.clie_3" dbalias=".clie_3" )
 column=(type=decimal(0) updatewhereclause=yes name=s_armo_colli_2 dbname="s_armo.colli_2" dbalias=".colli_2" )
 column=(type=decimal(2) updatewhereclause=yes name=imp_fatt dbname="imp_fatt" )
 column=(type=decimal(2) updatewhereclause=yes name=imp_da_fatt dbname="imp_da_fatt" )
 column=(type=decimal(0) updatewhereclause=yes name=iva dbname="iva" )
 column=(type=long updatewhereclause=yes name=id_armo dbname="s_armo.id_armo" dbalias=".id_armo" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="s_armo.id_meca" dbalias=".id_meca" )
 column=(type=decimal(0) updatewhereclause=yes name=gru_conto dbname="gru.conto" dbalias=".conto" )
 column=(type=decimal(0) updatewhereclause=yes name=gru_s_conto dbname="gru.s_conto" dbalias=".s_conto" )
 retrieve=" SELECT distinct 
         s_armo.num_int,   
         s_armo.data_int,   
         s_armo.clie_3,   
         s_armo.colli_2,
         COALESCE(s_armo.imp_fatt,0) as imp_fatt,
         COALESCE(s_armo.imp_da_fatt,0) as imp_da_fatt,
         COALESCE(clienti.iva,0) as iva,
	     s_armo.id_armo,
	     s_armo.id_meca,
--         prodotti.iva,
         gru.conto,
		gru.s_conto
    FROM 
         s_armo  
              inner JOIN clienti ON s_armo.clie_3 = clienti.codice
              inner JOIN gru ON s_armo.gruppo = gru.codice
 --             inner JOIN armo ON s_armo.id_armo = armo.id_armo
              inner JOIN meca ON s_armo.id_meca = meca.id
--              inner JOIN prodotti ON armo.art = prodotti.codice
     WHERE 
        s_armo.data_int >= :k_data_da
         and meca.clie_3 > 0 
         and (meca.clie_3 = :k_clie_3  or :k_clie_3 = 0)
		and (s_armo.clie_3 = :k_clie_3  or :k_clie_3 = 0)
         and s_armo.imp_da_fatt > 0 
         and (s_armo.aperto <> 'N' or s_armo.aperto is null) 
         and s_armo.imp_da_fatt > 0
--         and (armo.colli_2 > armo.colli_fatt or armo.colli_fatt is null)

" arguments=(("k_clie_3", number),("k_data_da", date))  sort="data_int A num_int A id_armo A " )
