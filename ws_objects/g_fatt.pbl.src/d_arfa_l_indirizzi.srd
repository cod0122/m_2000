 retrieve="  SELECT DISTINCT arfa_testa.id_cliente,   
         arfa_testa.rag_soc_1,   
         arfa_testa.rag_soc_2,   
         arfa_testa.indi,   
         arfa_testa.loc,   
         arfa_testa.cap,   
         arfa_testa.prov,   
         arfa_testa.id_nazione,   
         nazioni.nome  
    FROM arfa_testa LEFT OUTER JOIN nazioni ON arfa_testa.id_nazione = nazioni.id_nazione  
   WHERE arfa_testa.id_cliente = :k_id and arfa_testa.data_fatt > :k_data    
