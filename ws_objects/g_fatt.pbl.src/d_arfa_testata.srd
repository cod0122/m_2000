 retrieve="  SELECT arfa_testa.num_fatt,   
         arfa_testa.data_fatt,   
         arfa_testa.id_cliente,   
         clienti.codice,   
         clienti.rag_soc_10,   
         clienti.p_iva,   
         clienti.cf,   
         clienti.loc_1,   
         clienti.prov_1,   
         arfa_testa.id_nazione,   
         nazioni.nome,   
         trim(arfa_testa.rag_soc_1) as rag_soc_1,   
         trim(arfa_testa.rag_soc_2) as rag_soc_2,  
         trim(arfa_testa.indi) as indi,   
         trim(arfa_testa.loc) as loc,   
         arfa_testa.cap,   
         arfa_testa.prov,   
         arfa_testa.cod_pag,   
         arfa_testa.tipo_doc,   
         arfa_testa.stampa,   
         arfa_testa.data_stampa,   
         arfa_testa.id_fattura,   
         pagam.des 
         ,arfa_testa.stampa_tradotta
        ,arfa_testa.note_1
        ,arfa_testa.note_2
 	    ,arfa_testa.x_datins
       ,arfa_testa.x_utente
       ,getdate() as k_competenza_dal 
        ,clienti_fatt.fattura_da
		,clienti.iva
		,clienti.cadenza_fattura
		,0 as k_iva_esente
		,fat1.note_1
		,fat1.note_2
		,fat1.note_3
		,fat1.note_4
		,fat1.note_5
		,fat1.note_normative
         ,'' as k_clienti_fattura 
           ,arfa_testa.modo_stampa
           ,arfa_testa.modo_email
           ,arfa_testa.email_invio
           ,arfa_testa.file_prodotto
           ,arfa_testa.id_email_invio

    FROM arfa_testa 
						LEFT OUTER JOIN fat1 ON arfa_testa.id_fattura = fat1.id_fattura 
						LEFT OUTER JOIN clienti ON arfa_testa.id_cliente = clienti.codice 
	                      LEFT OUTER JOIN clienti_fatt ON arfa_testa.id_cliente = clienti_fatt.id_cliente 
	                      LEFT OUTER JOIN nazioni ON arfa_testa.id_nazione = nazioni.id_nazione 
						LEFT OUTER JOIN pagam ON arfa_testa.cod_pag = pagam.codice   
    where arfa_testa.id_fattura = :k_id

" update="arfa_testa" updatewhere=0 updatekeyinplace=no arguments=(("k_id", number)) )
