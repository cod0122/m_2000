 retrieve="   select      
              MECA.ID
              ,MECA.NUM_INT
              ,MECA.DATA_INT
              ,COALESCE(MECA.STATO, 0) as stato
	          ,trim(meca_stato.descrizione) + '(' + convert(varchar,COALESCE(MECA.STATO, 0)) + ')' as meca_stato_descrizione
              ,MECA_BLK.ID_MECA_CAUSALE
              ,trim(MECA_BLK.DESCRIZIONE) as MECA_BLK_DESCRIZIONE
              ,MECA.CONTRATTO
              ,MECA.AREA_MAG
              ,MECA.CONSEGNA_DATA
              ,CONTRATTI.MC_CO
              ,CONTRATTI.SC_CF
              ,trim(MECA.NUM_BOLLA_IN) as num_bolla_in
              ,MECA.DATA_BOLLA_IN
              ,MECA.CLIE_1
              ,MECA.CLIE_2
              ,MECA.CLIE_3
              ,MECA.id_wm_pklist
              ,MECA.aperto
              ,MECA.stato_in_attenzione
              ,MECA.x_datins
              ,MECA.x_utente
              ,MECA.DATA_LAV_FIN
             ,trim(c1.RAG_SOC_10) as c1_rag_soc_10
             ,trim(c1.RAG_SOC_11) as c1_rag_soc_11
             ,c1.P_IVA
             ,c1.INDI_1
             ,c1.LOC_1
             ,c1.PROV_1
             ,c1.CAP_1
             ,c1.id_nazione_1
             ,trim(c2.RAG_SOC_10) as c2_rag_soc_10
             ,trim(c2.RAG_SOC_11) as c2_rag_soc_11
             ,c2.P_IVA
             ,c2.INDI_1
             ,c2.LOC_1
             ,c2.PROV_1
             ,c2.CAP_1
             ,c2.id_nazione_1
             ,trim(c3.RAG_SOC_10) as c3_rag_soc_10
             ,trim(c3.RAG_SOC_11) as c3_rag_soc_11
             ,c3.P_IVA
             ,c3.INDI_1
             ,c3.LOC_1
             ,c3.PROV_1
             ,c3.CAP_1
             ,c3.id_nazione_1
             ,wm_pklist.idpkl
             ,0 as colli
             ,'avvertenze' as k_avvertenze
              ,MECA.e1doco as e1doco
              ,MECA.e1rorn as e1rorn
              ,MECA.DATA_ENT
              ,COALESCE(meca.e1srst,'NC') as e1srst
              ,(select COALESCE(trim(meca_causali.descrizione),'') from meca_causali where meca_causali.id_meca_causale = MECA_BLK.ID_MECA_CAUSALE) meca_causali_descrizione
        from  MECA left outer join CONTRATTI on MECA.CONTRATTO = CONTRATTI.CODICE 
                    left outer join MECA_BLK on MECA.ID = MECA_BLK.ID_MECA 
                    left outer join clienti c1 on meca.clie_1 = c1.codice
                    left outer join clienti c2 on meca.clie_2 = c2.codice
                    left outer join clienti c3 on meca.clie_3 = c3.codice
                    left outer join wm_pklist on meca.id_wm_pklist = wm_pklist.id_wm_pklist
		             left outer join meca_stato on meca.stato = meca_stato.codice 
          where MECA.id  = :K_id
                 

" update="MECA,meca_blk" updatewhere=0 updatekeyinplace=yes arguments=(("k_id", number)) )
