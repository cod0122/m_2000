table(column=(type=char(5) updatewhereclause=yes name=stato dbname="stato" )
 column=(type=long updatewhereclause=yes name=n_ordine dbname="pallet.N_ordine" )
 column=(type=char(12) updatewhereclause=yes name=pallet_code dbname="pallet.Pallet_Code" )
 column=(type=char(4) updatewhereclause=yes name=posizione dbname="pallet.Posizione" )
 column=(type=datetime updatewhereclause=yes name=k_dataora_lav_ini dbname="k_dataora_lav_ini" )
 column=(type=decimal(0) updatewhereclause=yes name=fase dbname="pallet.Fase" )
 column=(type=char(2) updatewhereclause=yes name=nn dbname="nn" )
 column=(type=decimal(0) updatewhereclause=yes name=f1avp dbname="f1avp" )
 column=(type=decimal(0) updatewhereclause=yes name=f2avp dbname="F2AVP" )
 column=(type=decimal(0) updatewhereclause=yes name=f1app dbname="F1APP" )
 column=(type=decimal(0) updatewhereclause=yes name=f2app dbname="F2APP" )
 column=(type=decimal(0) updatewhereclause=yes name=fila dbname="fila" )
 column=(type=char(12) updatewhereclause=yes name=barcode_figlio dbname="barcode_figlio" )
 column=(type=number updatewhereclause=yes name=ordine_figlio dbname="ordine_figlio" )
 column=(type=char(45) updatewhereclause=yes name=lotto_figlio dbname="lotto_figlio" )
 retrieve="SELECT  'WORK' stato
        , pallet.N_ordine  
        , pallet.Pallet_Code   
        , pallet.Posizione 
        , (data_entrata - INTERVAL EXTRACT(SECOND FROM data_entrata) SECOND) as k_dataora_lav_ini
       , pallet.Fase  
		, coalesce(work_table.nn, 0) nn
        , cast(pallet.F1AVP as decimal) f1avp
        , cast(pallet.F2AVP as decimal) F2AVP   
        , cast(pallet.F1APP as decimal) F1APP   
        , cast(pallet.F2APP as decimal) F2APP   
        , case
         	 when F1APP > '00' and F2APP > '00' then 1
         	 when F1APP > '00' then 1 
         	 when F2APP > '00' then 2
          	 else  0
		  end fila 
 	    , coalesce(groupage.pallet_figlio, '') as barcode_figlio
        , coalesce(groupage.ordine_figlio, 0) as ordine_figlio
        , coalesce(groupage.lotto, '0') as lotto_figlio
    FROM pallet left outer join work_table on pallet.Pallet_Code = work_table.barcode  
                left outer join groupage on pallet.Pallet_Code = groupage.padre    
	where pallet.data_uscita is null or pallet.data_uscita < '01.01.1990'
union all
SELECT  'QUEUE'
          , queue_table.Ordine 
          , queue_table.Barcode 
          , queue_table.Posizione 
		  , STR_TO_DATE('1900-01-01T00:00:00.000','%Y-%m-%dT%H:%i:%s.%f') as k_dataora_lav_ini
          , 0 fase  
		  , 0 nn
          , cast(queue_table.CicliFila1 as decimal)  f1avp
          , cast(queue_table.CicliFila2 as decimal)  f2avp
          , cast(queue_table.CicliFila1p as decimal) f1app
          , cast(queue_table.CicliFila2p as decimal) f2app
        , case
         	 when CicliFila1 > '00' and CicliFila2 > '00' then 1
         	 when CicliFila1 > '00' then 1 
         	 when CicliFila2 > '00' then 2
          	 else  0
		  end fila 
	      , coalesce(groupage.pallet_figlio, '') as barcode_figlio
          , coalesce(groupage.ordine_figlio, 0) as ordine_figlio
          , coalesce(groupage.lotto, 0) as lotto_figlio
        FROM queue_table left outer join groupage
                 on queue_table.Barcode = groupage.padre    
	" )
