 retrieve="  SELECT barcode.barcode,   
         meca_dosim.barcode,   
         meca_dosim.barcode_dosimetro,
         COALESCE(barcode.lav_fila_1,0) lav_fila_1
         ,COALESCE(barcode.lav_fila_1p,0) lav_fila_1p
         ,COALESCE(barcode.lav_fila_2,0) lav_fila_2
         ,COALESCE(barcode.lav_fila_2p,0) lav_fila_2p
    FROM barcode LEFT OUTER JOIN meca_dosim ON 
                             barcode.id_meca = meca_dosim.id_meca
                       AND barcode.barcode = meca_dosim.barcode_lav  
   WHERE barcode.id_meca = :k_id_meca   
ORDER BY barcode.barcode ASC   
" arguments=(("k_id_meca", number)) )
