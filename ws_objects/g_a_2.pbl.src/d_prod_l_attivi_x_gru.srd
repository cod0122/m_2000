         prodotti.des_mkt as des,   
         prodotti.gruppo,
         gru.des as gru_des,   
         prodotti.magazzino,   
         prodotti.attivo  
        FROM (prodotti left outer join gru on
             prodotti.gruppo = gru.codice) 
