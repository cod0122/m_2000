 retrieve="  SELECT certif.id,   
         certif.data_stampa,   
         certif.num_certif,   
         certif.data,   
         meca.id,   
         meca.num_int,   
         meca.data_int,   
         meca.clie_1,   
         clienti_a.rag_soc_10,   
         meca.num_bolla_in,   
         meca.data_bolla_in,   
         certif.colli,   
         certif.clie_2,   
         clienti_b.rag_soc_10,   
         meca.clie_3,   
         clienti_c.rag_soc_10,   
         contratti.mc_co,   
         contratti.sl_pt,   
         certif.note,   
         certif.note_1,   
         certif.note_2,   
         contratti.sc_cf,   
         sc_cf.data,   
         sc_cf.descr,   
         certif.dose,   
         certif.dose_min,   
         certif.dose_max,   
         certif.lav_data_ini,   
         certif.lav_data_fin,   
         certif.st_dose_min,
         certif.st_dose_max,
         certif.st_data_ini,
         certif.st_data_fin,
         certif.x_utente,   
         certif.x_datins,   
         meca.contratto,
         'N' k_forza_dose_min,   
         'N' k_forza_dose_max,
          0.00 k_dose_min,
          0.00 k_dose_max,
         meca.data_ent, 
         certif.ora_stampa   
    FROM certif LEFT OUTER JOIN clienti clienti_b ON 
              certif.clie_2 = clienti_b.codice 
              LEFT OUTER JOIN meca ON 
              certif.id_meca = meca.id
              LEFT OUTER JOIN clienti clienti_a ON 
              meca.clie_1 = clienti_a.codice 
              LEFT OUTER JOIN clienti clienti_c ON 
              meca.clie_3 = clienti_c.codice 
              LEFT OUTER JOIN contratti ON 
              meca.contratto = contratti.codice 
              LEFT OUTER JOIN sc_cf ON 
              contratti.sc_cf = sc_cf.codice  
   WHERE 
         certif.num_certif = :k_num_certif 

" update="certif" updatewhere=0 updatekeyinplace=no arguments=(("k_id", number),("k_num_certif", number)) )
