 column=(type=long updatewhereclause=yes name=armo_magazzino dbname="armo.magazzino" )
 retrieve="               select
                          ARFA.ID_ARMO,
                          ARFA.NUM_FATT,
                          ARFA.DATA_FATT,
                          ARFA.NUM_BOLLA_OUT,
                          ARFA.DATA_BOLLA_OUT,
                          ARMO.NUM_INT,
                          ARMO.DATA_INT,
                          ARFA.TIPO_RIGA,
                          ARMO.ART,
                          ARFA.IVA,
                          ARMO.DOSE,
                          ARFA.COLLI,
                          ARFA.COLLI_OUT,
                          ARFA.PESO_KG_OUT,
                          ARFA.PREZZO_U,
                          ARFA.PREZZO_T,
                          ARFA.CLIE_3,
                          ARFA.TIPO_DOC,
                          ARFA.STAMPA,
                          ARFA.TIPO_L,
                          ARFA.COD_PAG,
                          PRODOTTI.DES,
                          PRODOTTI.GRUPPO,
                          ARMO.MAGAZZINO
                     from  ARFA inner join ARMO on
                               ARMO.ID_ARMO         = ARFA.ID_ARMO
						left outer join PRODOTTI on
                               PRODOTTI.CODICE      = ARMO.ART
                     where ARFA.NUM_FATT   between
                           :k_NUM_FATT_ini and :k_NUM_FATT_fin   
                           and ARFA.DATA_FATT between
                           :k_DATA_FATT_ini and :k_DATA_FATT_fin   
                  union all
               select     0,
                          ARFA_V.NUM_FATT,
                          ARFA_V.DATA_FATT,
                          0,
                          convert(date, '01.01.1899'),
                          0,
                          convert(date, '01.01.1899'),
                          'V',
                          ' ',
                          ARFA_V.IVA,
                          0,
                          ARFA_V.COLLI,
                          ARFA_V.COLLI,
                          0,
                          ARFA_V.PREZZO_U,
                          ARFA_V.PREZZO_T,
                          ARFA_V.CLIE_3,
                          ARFA_V.TIPO_DOC,
                          ARFA_V.STAMPA,
                          'C',
                          ARFA_V.COD_PAG,
                          ARFA_V.COMM,
                          0,
                          0
                     from  ARFA_V
                     where ARFA_V.NUM_FATT   between
                           :k_NUM_FATT_ini and :k_NUM_FATT_fin   
                           and ARFA_V.DATA_FATT between
                           :k_DATA_FATT_ini and :k_DATA_FATT_fin   
			       order by 
					   NUM_FATT, 
					   DATA_FATT,
                          TIPO_RIGA,
                          NUM_BOLLA_OUT,
					   DATA_BOLLA_OUT,
                         ART,
                         DOSE,
                         PREZZO_U
" arguments=(("k_NUM_FATT_ini", number),("k_NUM_FATT_fin", number),("k_DATA_FATT_ini", date),("k_DATA_FATT_fin", date)) )
