 column=(type=datetime updatewhereclause=yes name=x_datins dbname="certif.x_datins" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="certif.id_meca" )
 retrieve="  SELECT certif.num_certif,   
         certif.data,   
         meca.num_int,   
         meca.data_int,   
         meca.data_bolla_in,   
         meca.num_bolla_in,   
         certif.data_stampa,   
         certif.clie_2,   
         clienti.rag_soc_10,   
         certif.colli,   
         certif.dose,   
         certif.note,
		certif.form_di_stampa , 
		certif.id  
		,certif.id_docprod  
		,certif.x_datins
		,certif.id_meca
    FROM certif LEFT OUTER JOIN meca ON 
         certif.id_meca = meca.id 
                LEFT OUTER JOIN clienti ON 
         certif.clie_2 = clienti.codice  
   WHERE ( certif.data between :k_data_da and :k_data_a)  AND  
         ( :k_num_certif = 0  OR  
           certif.num_certif = :k_num_certif ) AND  
         ( :k_clie_2 = 0  OR  
           certif.clie_2 = :k_clie_2 ) AND  
         ( :k_id_meca = 0  OR  
           certif.id_meca = :k_id_meca )   
           
ORDER BY certif.num_certif DESC   
" arguments=(("k_data_da", date),("k_data_a", date),("k_num_certif", number),("k_clie_2", number),("k_id_meca", number)) )
