 retrieve="	SELECT 
         barcode.barcode,
         barcode.barcode_lav,
         barcode.groupage,
         barcode.causale,
         barcode.flg_dosimetro,
			barcode.data,
			barcode.pl_barcode,
         barcode.num_int,   
         barcode.data_int,   
         barcode.id_meca,   
         barcode.data_stampa,
			barcode.data_lav_ini,
			barcode.data_lav_fin,
			barcode.data_lav_ok,
			barcode.data_sosp,
			barcode.id_armo,
			barcode.fila_1,
			barcode.fila_2,
			barcode.fila_1p,
			barcode.fila_2p,
			barcode.posizione,
			barcode.bilancella,
			barcode.x_datins,
			barcode.x_utente,
         barcode.err_lav_fin,   
         barcode.note_lav_fin,   
         meca.err_lav_ok,   
         meca.note_lav_ok,  
			barcode.upd_data_fin,   
			barcode.upd_utente_fin,   
			barcode.upd_data_ok,   
			barcode.upd_utente_ok,  
         min(meca_dosim.dosim_dose) dosim_dose_min,   
         max(meca_dosim.dosim_dose) dosim_dose_max,   
         barcode.ora_lav_ini,   
         barcode.ora_lav_fin,   
         COALESCE(barcode.lav_fila_1,0) barcode_lav_fila_1,   
         COALESCE(barcode.lav_fila_2,0) barcode_lav_fila_2,   
         COALESCE(barcode.lav_fila_1p,0) barcode_lav_fila_1p,   
         COALESCE(barcode.lav_fila_2p,0) barcode_lav_fila_2p,   
         meca.clie_1, 
         meca.clie_3, 
			meca.num_bolla_in,
			meca.data_bolla_in,
			meca.area_mag,
			meca.contratto,
			meca.consegna_data,
			contratti.mc_co,
			contratti.sc_cf,
			contratti.descr,
         c1.rag_soc_10,
         c3.rag_soc_10,
         armo.art,
			armo.dose,
			armo.larg_2,
			armo.lung_2,
			armo.alt_2,
			armo.peso_kg,
			armo.colli_2,
			armo.pedane,
			armo.id_listino,
			armo.magazzino,
			sl_pt.cod_sl_pt,
			sl_pt.descr,
			sl_pt.densita,
			pl_barcode.data,
			pl_barcode.note_1,
			pl_barcode.note_2,
			pl_barcode.data_sosp,
			pl_barcode.data_chiuso,
			prodotti.des,
			arsp.num_bolla_out,
			arsp.data_bolla_out,
			arfa.num_fatt,
			arfa.data_fatt,
         certif.num_certif,
         certif.data,
         certif.data_stampa,
         0 as barcode_figli
         ,barcode.modgiri_data
         ,barcode.modgiri_utente
         ,barcode.imptime_second
         ,min(meca_dosimbozza.dosim_data) meca_dosimbozza_dosim_data
         ,max(meca_dosimbozza.dosim_dose) meca_dosimbozza_dosim_dose
    FROM (((((((((((((barcode
			 LEFT OUTER JOIN arsp ON 
	       barcode.id_armo = arsp.id_armo)
			 LEFT OUTER JOIN arfa ON 
	       barcode.id_armo = arfa.id_armo)
          LEFT OUTER JOIN meca ON 
	       barcode.num_int = meca.num_int and barcode.data_int = meca.data_int)
			 LEFT OUTER JOIN clienti c1 ON 
			 meca.clie_1 = c1.codice)
			 LEFT OUTER JOIN clienti c3 ON 
			 meca.clie_3 = c3.codice)
			 LEFT OUTER JOIN contratti ON 
			 meca.contratto = contratti.codice)
          LEFT OUTER JOIN certif ON 
	       meca.id = certif.id_meca)
          LEFT OUTER JOIN meca_dosim ON 
	       meca.id = meca_dosim.id_meca)
          LEFT OUTER JOIN meca_dosimbozza ON 
	       barcode.barcode = meca_dosimbozza.barcode_lav)
			 LEFT OUTER JOIN armo ON 
			 barcode.id_armo = armo.id_armo)
			 LEFT OUTER JOIN pl_barcode ON
			 barcode.pl_barcode = pl_barcode.codice)
			 LEFT OUTER JOIN sl_pt ON 
			 armo.cod_sl_pt = sl_pt.cod_sl_pt)
			 LEFT OUTER JOIN prodotti ON 
			 armo.art = prodotti.codice)
			 
    WHERE 
	       barcode.barcode = :k_barcode
    group by
         barcode.barcode,
         barcode.barcode_lav,
         barcode.groupage,
         barcode.causale,
         barcode.flg_dosimetro,
			barcode.data,
			barcode.pl_barcode,
         barcode.num_int,   
         barcode.data_int,   
         barcode.id_meca,   
         barcode.data_stampa,
			barcode.data_lav_ini,
			barcode.data_lav_fin,
			barcode.data_lav_ok,
			barcode.data_sosp,
			barcode.id_armo,
			barcode.fila_1,
			barcode.fila_2,
			barcode.fila_1p,
			barcode.fila_2p,
			barcode.posizione,
			barcode.bilancella,
			barcode.x_datins,
			barcode.x_utente,
         barcode.err_lav_fin,   
         barcode.note_lav_fin,   
         meca.err_lav_ok,   
         meca.note_lav_ok,  
			barcode.upd_data_fin,   
			barcode.upd_utente_fin,   
			barcode.upd_data_ok,   
			barcode.upd_utente_ok,  
         barcode.ora_lav_ini,   
         barcode.ora_lav_fin,   
         barcode.lav_fila_1,   
         barcode.lav_fila_2,   
         barcode.lav_fila_1p,   
         barcode.lav_fila_2p,   
         meca.clie_1, 
         meca.clie_3, 
			meca.num_bolla_in,
			meca.data_bolla_in,
			meca.area_mag,
			meca.contratto,
			meca.consegna_data,
			contratti.mc_co,
			contratti.sc_cf,
			contratti.descr,
         c1.rag_soc_10,
         c3.rag_soc_10,
         armo.art,
			armo.dose,
			armo.larg_2,
			armo.lung_2,
			armo.alt_2,
			armo.peso_kg,
			armo.colli_2,
			armo.pedane,
			armo.id_listino,
			armo.magazzino,
			sl_pt.cod_sl_pt,
			sl_pt.descr,
			sl_pt.densita,
			pl_barcode.data,
			pl_barcode.note_1,
			pl_barcode.note_2,
			pl_barcode.data_sosp,
			pl_barcode.data_chiuso,
			prodotti.des,
			arsp.num_bolla_out,
			arsp.data_bolla_out,
			arfa.num_fatt,
			arfa.data_fatt,
         certif.num_certif,
         certif.data,
         certif.data_stampa
         ,barcode.modgiri_data
         ,barcode.modgiri_utente
         ,barcode.imptime_second
" update="barcode" updatewhere=0 updatekeyinplace=yes arguments=(("k_barcode", string)) )
