 retrieve="  SELECT distinct certif.id,   
         certif.data_stampa,   
         certif.num_certif,   
         certif.data,   
         concat(meca.num_int, '/', year(meca.data_int)) as meca_num_int,   
         meca.clie_1,   
         concat(trim(clienti_a.rag_soc_10), ' (', meca.clie_1, ')') as rag_soc_10,   
         trim(meca.num_bolla_in) meca_num_bolla_in,   
         meca.data_bolla_in,   
         certif.colli,   
         certif.clie_2,   
         concat(trim(clienti_b.rag_soc_10), ' (', meca.clie_2, ')') as rag_soc_10_1,   
         meca.clie_3,   
         concat(trim(clienti_c.rag_soc_10), ' (', meca.clie_3, ')') as rag_soc_10_2,   
         contratti.mc_co,   
         certif.note,
			certif.note_1,
         certif.note_2, 
         contratti.sc_cf,   
         sc_cf.data,   
         sc_cf.descr,   
         certif.dose,   
         certif.dose_min,   
         certif.dose_max,   
         certif.lav_data_ini,   
         certif.lav_data_fin,   
         certif.st_dose_min,
         certif.st_dose_max,
         certif.st_data_ini,
         certif.st_data_fin,
         certif.st_dati_bolla_in,
         certif.x_utente,   
         certif.x_datins,   
         meca.contratto,
         (select max(meca_dosim.dosim_lotto_dosim) from  meca_dosim where meca_dosim.id_meca = meca.id) as meca_dosim_lotto_dosim,
         meca.id_wm_pklist,
         'note' as k_note,
         '1' as k_test 
--        ,'0' as k_mat_farmaceutico 
        ,COALESCE((select distinct 1 from armo inner join sl_pt on armo.cod_sl_pt = sl_pt.cod_sl_pt where armo.id_meca = meca.id and sl_pt.tipo = '2'),0) as k_mat_farmaceutico 
		,certif.id_meca
        ,dosimetrie.note
        ,wm_pklist.idpkl
        ,'0' as k_flg_img_bn
        ,'1' as k_ragsoc_base_1
        ,'2' as k_ragsoc_base_2
        ,COALESCE(meca.e1doco, 0) as e1doco
        ,COALESCE(meca.e1rorn, 0) as e1rorn
		 , (select  top 1 id_memo from  meca_memo mm 
               where meca.id = mm.id_meca and mm.allarme = 'A'  
		      union all 
		     select  top 1 id_memo  from  clienti_memo cm  
       	        where (meca.clie_1 = cm.id_cliente or meca.clie_2 = cm.id_cliente or meca.clie_3 = cm.id_cliente) 
    		                      and cm.allarme = 'A') as id_memo 
    FROM (((((((((certif LEFT OUTER JOIN clienti clienti_b ON 
             certif.clie_2 = clienti_b.codice) 
         		    LEFT OUTER JOIN meca ON 
                         certif.id_meca = meca.id) 
                      LEFT OUTER JOIN clienti clienti_a ON 
                         meca.clie_1 = clienti_a.codice) 
                      LEFT OUTER JOIN clienti clienti_c ON 
                        meca.clie_3 = clienti_c.codice) 
              LEFT OUTER JOIN contratti ON 
                        meca.contratto = contratti.codice) 
              LEFT OUTER JOIN sc_cf ON 
                        contratti.sc_cf = sc_cf.codice)  
              LEFT OUTER JOIN meca_dosim ON 
                        meca_dosim.id_meca = meca.id) 
              LEFT OUTER JOIN dosimetrie ON 
                        meca_dosim.dosim_lotto_dosim = dosimetrie.lotto_dosim) 
              LEFT OUTER JOIN wm_pklist on 
                        meca.id_wm_pklist = wm_pklist.id_wm_pklist)
	WHERE  certif.num_certif = :k_num_certif_ini 
	order by 
         certif.data_stampa,   
         certif.num_certif
" arguments=(("k_num_certif_ini", number),("k_num_certif_fin", number)) )
