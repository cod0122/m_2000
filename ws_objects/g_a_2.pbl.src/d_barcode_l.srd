 retrieve="  SELECT barcode.barcode,   
         barcode.tipo_cicli,   
         barcode.fila_1,   
         barcode.fila_2,   
         barcode.fila_1p,   
         barcode.fila_2p,   
         armo.id_armo,   
         armo.dose,   
         armo.peso_kg,   
         armo.larg_2,   
         armo.lung_2,   
         armo.alt_2,   
         armo.pedane,   
         armo.campione,   
         meca.area_mag,    
         meca.contratto,   
         contratti.mc_co,   
         contratti.sc_cf,   
         contratti.descr,   
         armo.cod_sl_pt,   
         sl_pt.descr,   
         sl_pt.tipo_cicli,   
         sl_pt.fila_1,   
         sl_pt.fila_2,   
         sl_pt.fila_1p,   
         sl_pt.fila_2p,   
         armo.art,   
         prodotti.des_mkt as prodotti_des,   
         barcode.num_int,   
         barcode.data_int,   
         meca.clie_3,   
         clienti.rag_soc_10,   
         meca.clie_2,   
         trim(clienti_2.rag_soc_10) + '  ' +  convert(varchar, meca.clie_2) k_ricevente,   
         barcode.data_sosp,   
         barcode.data_lav_ini,   
         barcode.data_lav_fin,   
         barcode.data_lav_ok,
         barcode.x_datins,  
         barcode.x_utente  
         ,barcode.flg_dosimetro
    FROM (((((
         meca inner JOIN barcode ON 
         meca.id = barcode.id_meca) 
                  inner JOIN armo ON 
         barcode.id_armo = armo.id_armo) 
                 LEFT OUTER JOIN contratti ON
         meca.contratto = contratti.codice) 
                 LEFT OUTER JOIN sl_pt ON 
         armo.cod_sl_pt = sl_pt.cod_sl_pt) 
                 LEFT OUTER JOIN prodotti ON 
         armo.art = prodotti.codice) 
                 inner JOIN clienti ON 
         meca.clie_3 = clienti.codice  
                 inner JOIN clienti as clienti_2 ON 
         meca.clie_2 = clienti_2.codice  

   WHERE (:k_num_int = 0 or (meca.num_int = :k_num_int AND meca.data_int = :k_data_int)) 
         and 
         (:k_operazione = ~"tutti~" 
          or 
			 (:k_operazione = ~"no_pl~" 
			  and (barcode.causale <> 'T' ) 
			  and ((barcode.pl_barcode = 0 or barcode.pl_barcode is null) 
			      or (barcode.pl_barcode = :k_pl_barcode and  :k_pl_barcode > 0)) 
			  and (barcode.barcode_lav is null or barcode.barcode_lav = ~"~" )
			  and barcode.data_stampa > convert(date, '01.01.1899') 
			  and (barcode.data_sosp = convert(date, '01.01.1899') or barcode.data_sosp is null)  
               ) 
          or
             (:k_operazione = ~"gia_pl~" and
                                             barcode.pl_barcode > 0) 
          or
             (:k_operazione = ~"in_lav~" and
                                             barcode.data_lav_ini > convert(date, '01.01.1899')) 
          or
             (:k_operazione = ~"gia_lav~" and
                                              barcode.data_lav_fin > convert(date, '01.01.1899')) 
          or
             (:k_operazione = ~"gia_ok~" and
                                               barcode.data_lav_ok > convert(date, '01.01.1899')) 
         )
         and ( :k_fila_1 = 999 or barcode.fila_1 = :k_fila_1 )
         and ( :k_fila_2 = 999  or barcode.fila_2 = :k_fila_2)
         and ( :k_fila_1p = 999 or barcode.fila_1p = :k_fila_1p)
         and ( :k_fila_2p = 999 or barcode.fila_2p = :k_fila_2p)
         and ( :k_dose = 0 or armo.dose = :k_dose)

     order by barcode.barcode

" arguments=(("k_num_int", number),("k_data_int", date),("k_operazione", string),("k_fila_1", number),("k_fila_2", number),("k_fila_1p", number),("k_fila_2p", number),("k_dose", number),("k_pl_barcode", number)) )
