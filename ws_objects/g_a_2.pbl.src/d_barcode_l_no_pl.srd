 retrieve="  SELECT barcode.barcode,   
         barcode.tipo_cicli,   
         barcode.fila_1,   
         barcode.fila_2,   
         barcode.fila_1p,   
         barcode.fila_2p,   
         armo.id_armo,   
         armo.dose,   
         armo.peso_kg,   
         armo.larg_2,   
         armo.lung_2,   
         armo.alt_2,   
         armo.pedane,   
         armo.campione,   
         meca.area_mag,    
         meca.contratto,   
         armo.cod_sl_pt,   
         armo.art,   
         barcode.num_int,   
         barcode.data_int,   
         meca.clie_2,   
         barcode.groupage,  
         barcode.id_meca 
         ,barcode.flg_dosimetro
         ,clienti.e1ancodrs
    FROM 
         barcode inner JOIN meca ON 
         		barcode.id_meca = meca.id 
         inner JOIN armo ON 
         		barcode.id_armo = armo.id_armo 
         inner JOIN clienti ON 
              meca.clie_3 = clienti.codice
   WHERE barcode.id_meca = :k_id_meca
         and (barcode.causale <> 'T' or barcode.causale is null)
		   and ((barcode.pl_barcode = 0 or barcode.pl_barcode is null) 
			    or barcode.pl_barcode = :k_pl_barcode) 
			and barcode.data_stampa > DATEADD(day,-365,GETDATE()) 
			and (barcode.data_sosp = convert(date, '01.01.1899') or barcode.data_sosp is null)  
         and (barcode_lav = '' or barcode_lav is null) 
     order by barcode.barcode
" arguments=(("k_id_meca", number),("k_pl_barcode", number)) )
