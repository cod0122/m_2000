 retrieve="  SELECT barcode.pl_barcode,   
         barcode.pl_barcode_progr,   
         barcode.barcode,   
         barcode.tipo_cicli,   
         barcode.fila_1,   
         barcode.fila_2,   
         barcode.fila_1p,   
         barcode.fila_2p,   
         armo.dose,   
         armo.peso_kg,   
         armo.larg_2,   
         armo.lung_2,   
         armo.alt_2,   
         armo.campione,   
         meca.area_mag,    
         armo.cod_sl_pt,
--         sl_pt.descr,   
--         sl_pt.tipo_cicli,   
--         sl_pt.fila_1,   
--         sl_pt.fila_2,   
--         sl_pt.fila_1p,   
--         sl_pt.fila_2p,   
         armo.art,   
         barcode.num_int,   
         barcode.data_int,   
--         meca.clie_3,   
--         meca.clie_2,   
         barcode.data_sosp,   
         barcode.data_lav_ini,   
         barcode.data_lav_fin,   
         barcode.data_lav_ok,
         barcode.groupage,  
         barcode.x_datins,  
         barcode.x_utente,
         armo.id_armo,  
         meca.id as id_meca,  
         COALESCE(clienti.e1ancodrs, '') e1ancodrs
    FROM ((
         barcode INNER  JOIN meca ON 
         barcode.id_meca = meca.id) 
         INNER  JOIN armo ON 
         barcode.id_armo = armo.id_armo) 
        inner JOIN clienti ON 
         meca.clie_3 = clienti.codice
   WHERE meca.id = :k_id_meca
   order by  barcode.barcode" arguments=(("k_id_meca", number))  sort="barcode A " )
