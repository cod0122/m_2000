 column=(type=char(1) updatewhereclause=yes name=stato dbname="pl_barcode.stato" )
 column=(type=char(1) updatewhereclause=yes name=priorita dbname="pl_barcode.priorita" values=" Urgente	1/ Alta	4/ Normale	6/ Bassa	8/ Prima del Barcode	2/" )
 column=(type=char(13) updatewhereclause=yes name=prima_del_barcode dbname="pl_barcode.prima_del_barcode" )
 column=(type=char(4) updatewhereclause=yes name=k_path_file_pilota dbname="pl_barcode.k_path_file_pilota" )
 column=(type=long updatewhereclause=yes name=pilota_cmd_tipo dbname="pilota_cmd.tipo" values=" Piano di Trattamento	1/ Piano di Trattamento	11/ Modifica Programm. del Pilota	3/ Modifica Programm. del Pilota	13/ Piano // Modifica Trattamento	/" )
 column=(type=long updatewhereclause=yes name=pilota_cmd_num_rich dbname="pl_barcode.pilota_cmd_num_rich" )
 retrieve="  SELECT pl_barcode.codice,   
         pl_barcode.data,   
         pl_barcode.data_sosp,   
         pl_barcode.data_chiuso,   
         sr_utenti.nome,   
         pl_barcode.note_1,   
         pl_barcode.note_2,   
         pl_barcode.path_file_pilota,   
         pl_barcode.x_datins,   
         pl_barcode.x_utente  
         ,pl_barcode.stato  
         ,pl_barcode.priorita  
         ,pl_barcode.prima_del_barcode  
         ,~"Apri~" as k_path_file_pilota
		,pilota_cmd.tipo 
        ,pl_barcode.pilota_cmd_num_rich
    FROM pl_barcode  left outer join sr_utenti on
              pl_barcode.upd_utente_chiuso = sr_utenti.id
			left outer join pilota_cmd on pl_barcode.pilota_cmd_num_rich = pilota_cmd.num_rich
   WHERE  pl_barcode.data >= :k_data  AND 
          ( :k_operazione = 'tutti'  or   
            (pl_barcode.stato = 'A' AND  :k_operazione = 'aperti' ) OR  
            ( pl_barcode.stato = 'C'  AND  :k_operazione = 'chiusi' ) OR  
            ( pl_barcode.stato  = 'S' AND  :k_operazione = 'sospesi' )  )           
" arguments=(("k_data", date),("k_operazione", string))  sort="data D codice D priorita A " )
