table(column=(type=char(13) updatewhereclause=yes name=barcode_padre dbname="barcode" )
 column=(type=char(21) update=yes updatewhereclause=yes name=fila_1 dbname="fila_1" )
 column=(type=char(21) update=yes updatewhereclause=yes name=fila_2 dbname="fila_2" )
 column=(type=long updatewhereclause=yes identity=yes name=id_armo dbname="id_armo" )
 column=(type=decimal(1) updatewhereclause=yes name=dose dbname="dose" )
 column=(type=decimal(2) updatewhereclause=yes name=peso_kg dbname="peso_kg" )
 column=(type=char(85) updatewhereclause=yes name=larg_2 dbname="larg_2" )
 column=(type=decimal(0) updatewhereclause=yes name=alt_2 dbname="alt_2" )
 column=(type=char(1) updatewhereclause=yes name=campione dbname="campione" values="	S/	0" )
 column=(type=char(12) updatewhereclause=yes name=art dbname="art" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="id_meca" )
 column=(type=long updatewhereclause=yes name=num_int dbname="num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="data_int" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="clie_3" )
 column=(type=char(30) updatewhereclause=yes name=clienti_rag_soc_10 dbname="rag_soc_10" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="clie_2" )
 column=(type=date updatewhereclause=yes name=consegna_data dbname="consegna_data" )
 column=(type=date updatewhereclause=yes name=data_chiuso dbname="data_chiuso" )
 retrieve="  SELECT distinct
         barcode.barcode,   
         CONCAT(barcode.fila_1, ' + ', barcode.fila_1p) as fila_1,   
         CONCAT(barcode.fila_2, ' + ', barcode.fila_2p) as fila_2,   
         armo.id_armo,   
         armo.dose,   
         armo.peso_kg,   
         CONCAT(armo.larg_2, ' x ', armo.lung_2) as larg_2,   
         armo.alt_2,   
         armo.campione,   
         armo.art,   
         armo.id_meca,   
         armo.num_int,   
         armo.data_int,   
         meca.clie_3,   
         clienti.rag_soc_10,   
         meca.clie_2,   
         meca.consegna_data,   
         pl_barcode.data_chiuso 
    FROM ((((

         barcode LEFT OUTER JOIN meca ON 
         barcode.num_int = meca.num_int AND barcode.data_int = meca.data_int) 
         LEFT OUTER JOIN pl_barcode ON 
         barcode.pl_barcode = pl_barcode.codice) 
         LEFT OUTER JOIN armo ON 
         barcode.id_armo = armo.id_armo) 
         LEFT OUTER JOIN clienti ON 
         meca.clie_3 = clienti.codice)  

   WHERE
		(barcode.data_lav_ini is null or barcode.data_lav_ini <= convert(date, '01.01.1899'))
		and (barcode.barcode_lav is null or barcode.barcode_lav = '')
		and barcode.data_stampa > convert(date, '01.01.1899') 
		and (barcode.data_sosp = convert(date, '01.01.1899') or barcode.data_sosp is null)  
		and (barcode.causale <> 'T' or barcode.causale is null) 
	    and (pl_barcode.data_chiuso <= convert(date, '01.01.1899') or  pl_barcode.data_chiuso is null)
		and (barcode.barcode <> :k_barcode)
         and (barcode.fila_1 = :k_fila_1)
         and (barcode.fila_1p = :k_fila_1p)
         and (barcode.fila_2 = :k_fila_2) 
         and (barcode.fila_2p = :k_fila_2p) 

     order by armo.data_int, armo.num_int, barcode.barcode

" arguments=(("k_barcode", string),("k_fila_1", number),("k_fila_2", number),("k_fila_1p", number),("k_fila_2p", number)) )
