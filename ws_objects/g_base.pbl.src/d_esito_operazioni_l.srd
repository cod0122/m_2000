 retrieve="  SELECT esito_operazioni.tipo_operazione,   
         esito_operazioni.ts_operazione,   
         esito_operazioni.id_esito_operazioni,   
         esito_operazioni.ts_esito,   
         esito_operazioni.esito_ko,   
         esito_operazioni.esito,   
         esito_operazioni.x_datins,   
         esito_operazioni.x_utente  
    FROM esito_operazioni  
   WHERE (:tipo_operazione = '*' or esito_operazioni.tipo_operazione = :tipo_operazione)    
       and ( (:dalla_data = 'S' and convert(date,esito_operazioni.ts_operazione) >= convert(date,:kts_operazione) ) 
