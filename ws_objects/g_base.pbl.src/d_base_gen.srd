 retrieve="  SELECT base.id,   
         trim(base.rag_soc_1) rag_soc_1,   
         trim(base.rag_soc_2) rag_soc_2,   
         trim(base.indi) indi,   
         base.cap,   
         trim(base.loc) loc,   
         base.prov,   
--         base.inc,   
         base.mis_x,   
         base.mis_y,   
         base.mis_z,    
         base.peso_max,   
         trim(base.stamp_bo) stamp_bo,   
         trim(base.stamp_cert) stamp_cert,   
         trim(base.stamp_ft) stamp_ft,   
         trim(base.stamp_cbarre) stamp_cbarre,   
         trim(base.stamp_pdf) stamp_pdf,   
         base.op_sys,   
         base.mc_co_sc_cf,   
         base.data_stat,   
         base.ora_stat,   
         trim(base.titolo_procedura) as titolo_procedura,   
         base.anno,   
         base.p_iva,   
         base.cf,   
         base.telefono,   
         base.fax,   
         base.e_mail,   
         base.ind_web,   
         trim(base.fgrp_out_path) fgrp_out_path,   
         base.fpilota_out_dt_dal,   
         base.update_last_vers,   
         base.last_version,   
         trim(base.path_centrale) path_centrale,   
         trim(base.path_pgm_upd) path_pgm_upd,   
         base.valid_t_dose_min,   
         base.barcode_modulo  
        ,base_fatt.bolli_lim_stampa
	   ,base_fatt.bolli_note
       , trim(base_fatt.banca) banca   
        ,base_fatt.impon_minimo
        ,base_fatt.id_email_lettera_fattura
	   ,base_fatt.comunicazione
	   ,base_fatt.comunicazione_attiva
//	   ,base_fatt.esolver_fidi_id_meca_causale
        ,email1.codice
        ,base_fatt.id_email_lettera_avviso
        ,email2.codice
        ,trim(smtp_server) as smtp_server
        ,trim(smtp_port) as smtp_port
       , smtp_autorizz_rich 
       , trim(smtp_logfile) as  smtp_logfile
       , trim(smtp_user) as smtp_user
       , trim(smtp_pwd) as  smtp_pwd
       , trim(base_dir.dir_fatt) dir_fatt
       , trim(base_dir.doc_root) doc_root
//       , trim(base_dir.esolver_expanag_dir) esolver_expanag_dir
//       , trim(base_dir.esolver_expanag_nome) esolver_expanag_nome
//       , trim(base_dir.esolver_fidi_dir) esolver_fidi_dir
//       , trim(base_dir.esolver_expfidi_nome) esolver_expfidi_nome
//       , trim(base_dir.esolver_inpfidi_nome) esolver_inpfidi_nome
       , trim(base.dosimetro_ult_barcode) dosimetro_ult_barcode
       , base.dosimetro_id_armo   
       , trim(base.dosimetro_barcode_mask)  dosimetro_barcode_mask
	  , trim(base_fatt.ddt_qtna_note) ddt_qtna_note
	  , trim(base_fatt.ddt_blk_lotti_senza_attestato) ddt_blk_lotti_senza_attestato
      ,sv_call_vettore_id_listino
      ,listino.cod_art
      ,listino.prezzo
      , trim(COALESCE(base.rea_ufficio,'')) as rea_ufficio
      , trim(COALESCE(base.rea_numero,'')) as rea_numero
      , COALESCE(base.capitale_sociale,0) as capitale_sociale
      , trim(COALESCE(base.socio_unico,'')) as socio_unico
      , trim(COALESCE(base.stato_liquidazione,'')) as stato_liquidazione
      , COALESCE((select max(data_int) from s_armo_p), convert(date,'01.01.1900')) as data_max_armo_p  
      , trim(COALESCE(base.e1_enabled,'')) as e1_enabled
      , COALESCE(base_dir.e1dtlav_allineagg,0) as e1dtlav_allineagg
      , COALESCE(base_gmt.utc_fuso, 0) utc_fuso
      , COALESCE(base_gmt.oralegale_on, 'N') oralegale_on
      , base_gmt.oralegale_start oralegale_start
      , base_gmt.oralegale_end oralegale_end
      , COALESCE(trim(base_dir.dir_report_pilota),'') as dir_report_pilota
      , COALESCE(trim(base.e1mcu),'') as e1mcu
     , COALESCE(base_fatt.e1_id_listino_dsm_tof554701,0) as e1_id_listino_dsm_tof554701
     , COALESCE(base_fatt.e1_id_listino_dsm_tof554701_f,'N') as e1_id_listino_dsm_tof554701_f
     , concat(COALESCE(trim(l1.cod_art), ''), ' - ', COALESCE(trim(prodotti.des), '')) as prodotti_des  
     , COALESCE(trim(base_dir.report_export_dir),'') as report_export_dir
     , COALESCE(trim(base_dir.aco_exp_regcdp_dir),'') as aco_exp_regcdp_dir
     , COALESCE(trim(base_dir.e1_certif_saved_dir),'') as e1_certif_saved_dir
    FROM base left outer join base_fatt on 
              base.id = base_fatt.id_base   
					left outer join base_smtp on 
			base.id = base_smtp.id_base
					left outer join base_dir on 
			base.id = base_dir.id_base
					left outer join email email1 on 
			base_fatt.id_email_lettera_fattura= email1.id_email
					left outer join email email2 on 
			base_fatt.id_email_lettera_avviso= email2.id_email
					left outer join listino on 
			base_fatt.sv_call_vettore_id_listino = listino.id
		               left outer join base_gmt on 
         base.id = base_gmt.id_base   
					left outer join listino  l1 on 
			base_fatt.e1_id_listino_dsm_tof554701 = l1.id
                      left outer join prodotti on
             l1.cod_art = prodotti.codice
" update="base" updatewhere=0 updatekeyinplace=yes )
