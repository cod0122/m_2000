 retrieve="  SELECT sc_cf.codice,
         sc_cf.data,   
         sc_cf.data_scad,   
         sc_cf.cod_cli, 
         sc_cf.m_r_f, 
         clienti.rag_soc_10,        
         sc_cf.descr,
         sc_cf.attivo,   
         sc_cf.sc_cv,   
         sc_cf.sd_md,   
         sc_cf.sl_pt,   
         sc_cf.magazzino,   
         sc_cf.x_datins,
         sc_cf.x_utente,
         sl_pt.descr
    FROM sc_cf left outer join clienti on
         sc_cf.cod_cli = clienti.codice   
         left outer join sl_pt on 
         sc_cf.sl_pt = sl_pt.cod_sl_pt
    where sc_cf.codice = :k_codice
" update="sc_cf" updatewhere=0 updatekeyinplace=no arguments=(("k_codice", string)) )
