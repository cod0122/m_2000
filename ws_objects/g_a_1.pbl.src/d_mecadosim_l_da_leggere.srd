 column=(type=decimal(0) updatewhereclause=yes name=colli_groupage dbname="colli_groupage" )
 column=(type=long updatewhereclause=yes name=n_dosim dbname="n_dosim" )
 column=(type=long updatewhereclause=yes name=n_dosim_letti dbname="n_dosim_letti" )
 column=(type=long updatewhereclause=yes name=n_dosim_letti dbname="n_dosim_letti" )
 column=(type=long updatewhereclause=yes name=id_meca_memo dbname="id_meca_memo" )
 column=(type=date updatewhereclause=yes name=data_lav_fin dbname="meca_dosim.data_lav_fin" dbalias=".data_lav_fin" )
 column=(type=long updatewhereclause=yes name=e1doco dbname="meca_dosim.e1doco" dbalias=".e1doco" )
 column=(type=long updatewhereclause=yes name=collidanontrattare dbname="collidanontrattare" )
 column=(type=date updatewhereclause=yes name=data_ent dbname="meca_dosim.data_ent" dbalias=".data_ent" )
 retrieve="    SELECT distinct
         meca.data_int,   
         meca.num_int,   
         meca.contratto,   
         meca.clie_1,   
         meca.clie_3,   
         clienti_b.rag_soc_10,   
         meca.num_bolla_in,   
         meca.data_bolla_in,
			CASE
         	WHEN :k_dosimbozza_si = 1 THEN COALESCE(meca.err_lav_ok,' ')
         	ELSE '_'
       	END as err_lav_ok,
         meca.x_datins,   
         meca.x_utente,
         meca.id,
         armo.dose,
         armo.colli_2,
        -- (select COALESCE(sum(artr.colli_trattati), 0) from artr where armo.id_armo = artr.id_armo) as colli_trattati,
         (select COALESCE(count(*), 0) from barcode where barcode.id_meca = meca.id and barcode.data_lav_fin  > CONVERT(date,'01.01.1999')) as colli_trattati,
         (select COALESCE(sum(artr.colli_groupage), 0) from artr where armo.id_armo = artr.id_armo) as colli_groupage,
         (select COALESCE(count(*),0) from meca_dosim where meca_dosim.id_meca = meca.id) as n_dosim
			,CASE
         	WHEN :k_dosimbozza_si = 1 THEN (select COALESCE(count(*),0) from meca_dosimbozza where meca_dosimbozza.id_meca = meca.id)
         	ELSE (select COALESCE(count(*),0) from meca_dosimbozza where meca_dosimbozza.id_meca = meca.id)
       	END as n_dosim_letti
        , (select COALESCE(count(*),0) from meca_dosimbozza where meca_dosimbozza.id_meca = meca.id) as n_dosim_letti
        , (select COALESCE(max(meca_memo.id_meca_memo),0) from meca_memo where meca_memo.id_meca = meca.id) as id_meca_memo
        , meca.data_lav_fin
        , meca.e1doco
        ,(select COALESCE(count(*), 0) from barcode where barcode.id_meca = meca.id and barcode.causale = 'T') as collidanontrattare
        ,meca.data_int as data_ent  
    FROM  ((meca 
			 INNER JOIN armo 
          ON meca.id = armo.id_meca) 
          inner JOIN clienti as clienti_b
          ON meca.clie_3 = clienti_b.codice) 
          inner JOIN meca_dosim 
          ON meca.id = meca_dosim.id_meca
    WHERE meca.data_int >= :k_data_int 
	      and (meca_dosim.dosim_dose = 0 or meca_dosim.dosim_dose is null) 
         and meca.data_lav_fin > CONVERT(date,'01.01.2017')  
         and ( (:k_id_cliente = 0 and :k_id_meca = 0)
              or ((:k_id_meca > 0 and meca.id = :k_id_meca)
                  or (:k_id_cliente > 0 and meca.clie_3 = :k_id_cliente))
             ) 
         and ( (:k_dosimbozza_si = 1 and exists (select id_meca from meca_dosimbozza 
                          where meca_dosimbozza.id_meca = meca_dosim.id_meca and meca_dosimbozza.barcode = meca_dosim.barcode))
              or 
               (:k_dosimbozza_si = 0 and not exists (select id_meca from meca_dosimbozza 
                          where meca_dosimbozza.id_meca = meca_dosim.id_meca and meca_dosimbozza.barcode = meca_dosim.barcode))
              )
ORDER BY meca.data_int DESC,   
         meca.num_int DESC   
  
" arguments=(("k_data_int", date),("k_id_cliente", number),("k_id_meca", number),("k_dosimbozza_si", number)) )
