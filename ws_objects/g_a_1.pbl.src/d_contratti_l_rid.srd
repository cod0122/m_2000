 retrieve="  SELECT 
         contratti.codice,
         contratti.mc_co,   
         contratti.data_scad,   
         contratti.cod_cli,   
         contratti.sc_cf,   
         contratti.sl_pt,   
         contratti.descr,
         (select COALESCE(rag_soc_10,'') from clienti where codice = contratti.cod_cli) as rag_soc_10,
         contratti.x_datins
    from contratti left outer join clienti on 
         contratti.cod_cli = clienti.codice 
    where (contratti.cod_cli = :k_id_clie or :k_id_clie = 0)
    order by  contratti.mc_co desc
      ,contratti.data_scad desc

          " arguments=(("k_id_clie", number)) )
