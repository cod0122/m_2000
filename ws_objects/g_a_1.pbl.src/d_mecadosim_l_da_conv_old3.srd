 retrieve="   SELECT distinct
         meca.data_int,   
         meca.num_int,   
         meca.contratto,   
         meca.clie_1,   
         meca.clie_3,   
         clienti_b.rag_soc_10,   
         meca.err_lav_ok,   
         meca.x_datins,   
         meca.x_utente,
         meca.id,
 			COALESCE(armo.cod_sl_pt, '') as cod_sl_pt,
         armo.dose,
         armo.colli_2,
         (select COALESCE(sum(artr.colli_trattati), 0) from artr where armo.id_armo = artr.id_armo) as colli_trattati,
			(select COALESCE(count(*),0) from meca_dosim where meca_dosim.id_meca = meca.id) as n_dosim
        , COALESCE((select max(meca_memo.id_meca_memo) from meca_memo where meca_memo.id_meca = meca.id), 0) as id_meca_memo
        , meca.data_lav_fin
			,(select max(dosim_data) from meca_dosim where meca_dosim.id_meca = meca.id) as dosim_data_max
			,CASE
         	WHEN :k_dosim_si = 1 then 1
         	ELSE 0 
       	END as k_esegui, 
			COALESCE(meca_dosim.barcode_lav, '') as barcode_lav,   
         COALESCE(meca_dosim.barcode, '') as barcode,   
         meca_dosim.dosim_data as dosim_data,   
         COALESCE(meca_dosim.dosim_dose, 0.0) as dosim_dose,   
         COALESCE(meca_dosim.dosim_lotto_dosim, '') as dosim_lotto_dosim,   
			COALESCE(meca_dosim.dosim_flg_tipo_dose,'') as dosim_flg_tipo_dose,
   		COALESCE((barcode.fila_1) + '+' + (barcode.fila_1p), '') as previsti_fila_1,
   		COALESCE((barcode.fila_2) + '+' + (barcode.fila_2p), '') as previsti_fila_2,
   		COALESCE((barcode.lav_fila_1) + '+' + (barcode.lav_fila_1p), '') as lav_fila_1,
   		COALESCE((barcode.lav_fila_2) + '+' + (barcode.lav_fila_2p), '') as lav_fila_2,
			COALESCE(sl_pt.dose_min, 0.00) as dose_min,
			COALESCE(sl_pt.dose_max, 0.00) as dose_max 
    FROM  meca 
			 INNER JOIN armo 
          ON meca.id = armo.id_meca
			 INNER JOIN sl_pt 
          ON sl_pt.cod_sl_pt = armo.cod_sl_pt
          inner JOIN clienti as clienti_b
          ON meca.clie_3 = clienti_b.codice 
          inner JOIN meca_dosim 
          ON meca.id = meca_dosim.id_meca
			 left outer JOIN barcode
          ON meca_dosim.barcode_lav = barcode.barcode 
   WHERE  meca.data_int >= :k_data_int 
         and meca.data_lav_fin > date('01.01.2000')  
         and ( (:k_id_cliente = 0 and :k_id_meca = 0)
              or ((:k_id_meca = 0 or meca.id = :k_id_meca)
                  or (:k_id_cliente > 0 and meca.clie_3 = :k_id_cliente))
             ) 
         and ( (:k_dosim_si = 1 
                       and (meca.err_lav_ok = '2' or meca.err_lav_ok = 'B' or meca.err_lav_ok = '1')
                ))
              or 
               (:k_dosim_si = 0 and (
                  meca.err_lav_ok = 'A' or meca.err_lav_ok = 'L' or meca.err_lav_ok = 'K'
                   
               ))
ORDER BY meca.data_int DESC,   
         meca.num_int DESC,
			barcode   
  
" arguments=(("k_data_int", date),("k_id_cliente", number),("k_id_meca", number),("k_dosim_si", string)) )
