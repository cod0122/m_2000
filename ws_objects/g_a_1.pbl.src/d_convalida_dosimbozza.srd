 retrieve=" SELECT 
         COALESCE(meca_dosimbozza.id_meca_dosimbozza,0) as id_meca_dosimbozza,   
         meca_dosim.id_meca as id_meca ,
         COALESCE(meca_dosim.barcode_lav, '') as barcode_lav,   
         COALESCE(meca_dosim.barcode, '') as barcode,   
         COALESCE(meca_dosim.barcode_dosimetro, '') as barcode_dosimetro,   
         (meca_dosimbozza.dosim_data) as dosim_data,   
         COALESCE(meca_dosimbozza.dosim_lotto_dosim,'') as dosim_lotto_dosim,   
         COALESCE(meca_dosimbozza.dosim_temperatura,0) as dosim_temperatura,   
         COALESCE(meca_dosimbozza.dosim_umidita,0) as dosim_umidita,   
         COALESCE(meca_dosimbozza.dosim_assorb,0) as dosim_assorb,   
         COALESCE(meca_dosimbozza.dosim_spessore,0) as dosim_spessore,   
         COALESCE(meca_dosimbozza.dosim_rapp_a_s,0.000) as dosim_rapp_a_s,   
			COALESCE(meca_dosimbozza.dosim_flg_tipo_dose,'M') as dosim_flg_tipo_dose,
         COALESCE(meca_dosimbozza.dosim_dose,0.0) as dosim_dose,   
         (meca_dosimbozza.x_data_dosim_lettura) as x_data_dosim_lettura,   
         COALESCE(meca_dosimbozza.x_utente_dosim_lettura,'') as x_utente_dosim_lettura,
         (meca_dosimbozza.x_datins) as x_datins,   
         COALESCE(meca_dosimbozza.x_utente,'') as x_utente,
		   meca.contratto,
			contratti.sc_cf,
         meca.err_lav_ok,   
         meca.note_lav_ok,   
         meca.data_int,   
         meca.num_int  
        ,COALESCE(dosimpos.codice, 'ND') as dosimpos_codice
        ,COALESCE(meca_dosim.id_dosimpos, 0) as id_dosimpos
    FROM meca inner join meca_dosim on
	      meca.id = meca_dosim.id_meca
    			  inner join meca_dosimbozza on
	      meca.id = meca_dosimbozza.id_meca
              left outer join contratti on
         meca.contratto = contratti.codice
              left outer join dosimpos on 
         dosimpos.id_dosimpos = meca_dosim.id_dosimpos
	where meca_dosim.id_meca = :k_id_meca
" update="meca_dosimbozza" updatewhere=0 updatekeyinplace=no arguments=(("k_id_meca", number)) )
