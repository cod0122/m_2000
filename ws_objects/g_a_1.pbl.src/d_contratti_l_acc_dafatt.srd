table(column=(type=long update=yes updatewhereclause=yes key=yes identity=yes name=codice dbname="contratti.codice" dbalias=".codice" )
 column=(type=date update=yes updatewhereclause=yes name=data dbname="contratti.data" dbalias=".data" )
 column=(type=char(12) update=yes updatewhereclause=yes name=mc_co dbname="mc_co" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=cod_cli dbname="cod_cli" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_10 dbname="rag_soc_10" )
 column=(type=char(50) update=yes updatewhereclause=yes name=descr dbname="descr" )
 column=(type=char(1) update=yes updatewhereclause=yes name=flg_acconto dbname="flg_acconto" values="	S/	0" )
 column=(type=char(1) update=yes updatewhereclause=yes name=flg_fatt_dopo_valid dbname="flg_fatt_dopo_valid" values="	S/	0" )
 column=(type=decimal(2) updatewhereclause=yes name=acconto_imp dbname="acconto_imp" )
 column=(type=long updatewhereclause=yes name=acconto_perc dbname="acconto_perc" )
 column=(type=decimal(0) updatewhereclause=yes name=acconto_cod_pag dbname="acconto_cod_pag" )
 retrieve="  SELECT contratti.codice,   
         contratti.data,   
         COALESCE(contratti.mc_co, '') as mc_co,   
         COALESCE(contratti.cod_cli, 0) as cod_cli,   
         (select COALESCE(rag_soc_10, '') from clienti where codice = cod_cli) as rag_soc_10,
         COALESCE(contratti.descr, '') as descr,   
         COALESCE(contratti.flg_acconto, '') as flg_acconto,   
         COALESCE(contratti.flg_fatt_dopo_valid, '') as flg_fatt_dopo_valid
        , case
              when id_contratto_co > 0 then 0.00
              when id_contratto_dp > 0 then 0.00
              when id_contratto_rd > 0 then (select COALESCE(acconto_imp, 0.00) from contratti_rd where contratti_rd.id_contratto_rd = contratti.id_contratto_rd)
        end as acconto_imp
        , case
              when id_contratto_co > 0 then 0
              when id_contratto_dp > 0 then 0
              when id_contratto_rd > 0 then (select COALESCE(acconto_perc, 0) from contratti_rd where contratti_rd.id_contratto_rd = contratti.id_contratto_rd)
        end as acconto_perc
        , case
              when id_contratto_co > 0 then 0
              when id_contratto_dp > 0 then 0
              when id_contratto_rd > 0 then (select COALESCE(acconto_cod_pag, 0) from contratti_rd where contratti_rd.id_contratto_rd = contratti.id_contratto_rd)
        end as acconto_cod_pag
    FROM contratti
   WHERE 
         ( ( contratti.flg_acconto = 'S' ) 
         and ( contratti.data > :k_data ) )   
         and ( :k_clie = 0 or contratti.cod_cli = :k_clie )   
--	and contratti.codice not in 
-- (  SELECT  distinct COALESCE(meca.contratto, 0)   
--    FROM meca
 --  WHERE  meca.data_int > :k_data 
 --)
  and contratti.codice not in
 (  SELECT  distinct COALESCE(arfa_v.contratto, 0)  
    FROM  arfa_v  
   WHERE arfa_v.data_fatt > :k_data 
 )

ORDER BY contratti.data ASC   
" update="contratti" updatewhere=1 updatekeyinplace=no arguments=(("k_clie", number),("k_data", date)) )
