 column=(type=datetime updatewhereclause=yes name=data_ent dbname="armo.data_ent" dbalias=".data_ent" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="armo.clie_3" dbalias=".clie_3" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="armo.clie_2" dbalias=".clie_2" )
