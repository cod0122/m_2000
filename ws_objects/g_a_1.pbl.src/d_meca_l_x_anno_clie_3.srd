 retrieve="  SELECT meca.data_int,   
         meca.num_int,   
         meca.id,
         meca.contratto,   
         meca.clie_1,   
         clienti_b.rag_soc_10,   
         meca.num_bolla_in,   
         meca.data_bolla_in,
         sum(armo.colli_2) as colli_2,
		sum(armo.pedane) as pedane     
      ,meca.data_ent 
      ,meca.clie_3   
      ,meca.clie_2
      ,meca.id_wm_pklist  
    FROM (meca 
          LEFT OUTER JOIN clienti 
          clienti_b ON meca.clie_1 = clienti_b.codice) 
          LEFT OUTER JOIN armo 
          ON meca.id = armo.id_meca
   WHERE  ( meca.clie_1 = :k_clie  OR  
              :k_clie = 0 )   
       and year(meca.data_ent) >= :k_anno
group by  meca.data_int,   
         meca.num_int,   
         meca.id,
         meca.contratto,   
         meca.clie_1,   
         clienti_b.rag_soc_10,   
         meca.num_bolla_in,   
         meca.data_bolla_in
      ,meca.data_ent 
      ,meca.clie_3   
      ,meca.clie_2
      ,meca.id_wm_pklist  
ORDER BY meca.data_ent DESC,   
         meca.num_int DESC   
" arguments=(("k_anno", number),("k_clie", number)) )
