 retrieve="  SELECT meca.id ,
         meca.data_int,   
         meca.num_int,   
         meca.contratto,   
         contratti.mc_co,   
         contratti.sc_cf,   
         contratti.sl_pt,   
         contratti.descr,   
         meca.clie_1,   
         clienti_a.rag_soc_10,   
         meca.clie_2,   
         clienti_b.rag_soc_10,   
         meca.clie_3,   
         clienti_c.rag_soc_10,   
         meca.id_wm_pklist,   
         meca.num_bolla_in,   
         meca.data_bolla_in,   
         meca.consegna_data,   
         meca.area_mag,   
         meca_dosim.barcode,   
         meca_dosim.barcode_dosimetro,   
         meca_dosim.dosim_dose,   
         meca_dosim.dosim_data,   
         meca.err_lav_ok,   
         meca.note_lav_ok,   
         meca.err_lav_fin,   
         meca.data_lav_fin,   
         meca_dosim.dosim_assorb,   
         meca_dosim.dosim_spessore,   
         meca_dosim.dosim_lotto_dosim,   
         meca_dosim.dosim_rapp_a_s,   
         meca_dosim.dosim_temperatura,   
         meca_dosim.dosim_umidita,   
         trim(meca_dosim.x_utente_dosim_verifica) as x_utente_dosim_verifica,
         meca_dosim.x_data_dosim_verifica,   
         trim(meca_dosim.x_utente_dosim_sblocco_ko ) as x_utente_dosim_sblocco_ko,   
         meca_dosim.x_data_dosim_sblocco_ko,   
         meca.cert_forza_stampa,   
         meca.x_data_cert_f_st,   
         meca.x_utente_cert_f_st,   
         meca.cert_farma_st_ok,   
         meca.x_data_cert_farma,   
         meca.x_utente_cert_farm,    
         meca.cert_aliment_st_ok,   
         meca.x_data_cert_alim,   
         meca.x_utente_cert_alim,   
         meca.x_datins,   
         meca.x_utente,  
         meca.stato,  
         meca_stato.descrizione,  
         meca_stato.rgb_r,  
         meca_stato.rgb_g,  
         meca_stato.rgb_b,  
         meca_blk.id_meca_causale,
		meca_blk.descrizione,   
         meca_blk.x_datins_blk,   
         meca_blk.x_utente_blk,  
         meca_blk.x_datins_sblk,   
         meca_blk.x_utente_sblk,
		meca_causali.descrizione  ,
        wm_pklist.idpkl
    FROM meca LEFT OUTER JOIN clienti clienti_a on
          	     meca.clie_1 = clienti_a.codice 
          		  LEFT OUTER JOIN clienti clienti_b on
          	     meca.clie_2 = clienti_b.codice 
          		  LEFT OUTER JOIN clienti clienti_c on
                 meca.clie_3 = clienti_c.codice 
                 LEFT OUTER JOIN meca_dosim on
                 meca.id = meca_dosim.id_meca 
                 LEFT OUTER JOIN meca_blk on
                 meca.id = meca_blk.id_meca 
                 LEFT OUTER JOIN contratti on
                 meca.contratto = contratti.codice 
                 LEFT OUTER JOIN meca_stato on
                 meca.stato = meca_stato.codice 
                 LEFT OUTER JOIN wm_pklist on
                 meca.id_wm_pklist = wm_pklist.id_wm_pklist 
                 LEFT OUTER JOIN meca_causali on
                 meca_blk.id_meca_causale = meca_causali.id_meca_causale 
   WHERE  meca.id = :k_id
" retrieve.asneeded=yes data.storage=disk arguments=(("k_id", number)) )
