table(column=(type=long updatewhereclause=yes name=num_int dbname="armo.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="armo.data_int" dbalias=".data_int" )
 column=(type=long updatewhereclause=yes name=clie_1 dbname="meca.clie_1" dbalias=".clie_1" )
 column=(type=char(30) updatewhereclause=yes name=rag_soc_1 dbname="clienti.rag_soc_1" dbalias="clienti_a.rag_soc_1" )
 column=(type=char(12) updatewhereclause=yes name=art dbname="armo.art" dbalias=".art" )
 column=(type=char(40) updatewhereclause=yes name=des dbname="prodotti.des" dbalias=".des" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_2 dbname="armo.colli_2" dbalias=".colli_2" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_fatt dbname="colli_fatt" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_sped dbname="colli_sped" )
 column=(type=char(24) updatewhereclause=yes name=meca_num_bolla_in dbname="meca.num_bolla_in" dbalias=".num_bolla_in" )
 column=(type=date updatewhereclause=yes name=meca_data_bolla_in dbname="meca.data_bolla_in" dbalias=".data_bolla_in" )
 column=(type=long updatewhereclause=yes identity=yes name=id_armo dbname="armo.id_armo" dbalias=".id_armo" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="armo.id_meca" dbalias=".id_meca" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_da_fatt dbname="colli_da_fatt" )
 retrieve=" SELECT distinct 
         armo.num_int,   
         armo.data_int,   
         meca.clie_1,   
         clienti_a.rag_soc_10 as rag_soc_1,   
         armo.art,   
         prodotti.des_mkt as des,
         armo.colli_2,
         COALESCE(armo.colli_fatt,0) as colli_fatt,
--	    case
--   			 WHEN (count(armo_prezzi.id_armo)) is null or count(armo_prezzi.id_armo) = 0	THEN sum(COALESCE(arsp.colli,0)) 
--               ELSE sum(COALESCE(arsp.colli,0)) / count(armo_prezzi.id_armo)     
--         end  as colli_sped,
         sum(COALESCE(arsp.colli,0))  as colli_sped,
         meca.num_bolla_in,
         meca.data_bolla_in,
	    armo.id_armo,
	    armo.id_meca,
	    armo.colli_2 - COALESCE(armo.colli_fatt,0) as colli_da_fatt
--	    case
--  			 WHEN (sum(armo_prezzi.item_dafatt)) is null THEN (armo.colli_2 - COALESCE(armo.colli_fatt,0))
--               ELSE armo.colli_2 - COALESCE(armo.colli_fatt,0) + sum(armo_prezzi.item_dafatt)    
--         end  as colli_da_fatt
    FROM 
         armo  
         	inner JOIN prodotti 
         ON armo.art = prodotti.codice
              inner JOIN meca
         ON armo.id_meca = meca.id
              inner JOIN clienti as clienti_a 
         ON meca.clie_1 = clienti_a.codice
			 LEFT OUTER JOIN arsp 
         ON armo.id_armo = arsp.id_armo
			 left outer join armo_prezzi on 
		armo.id_armo = armo_prezzi.id_armo and armo_prezzi.stato = '6' 
   WHERE 
        armo.data_int >= :k_data_da
         and armo.magazzino <> 2  
		and (meca.clie_3 = :k_clie_3  or meca.clie_3 = 0)
		and (armo_prezzi.id_armo is null and  (ARMO.COLLI_2 > ARMO.COLLI_FATT or ARMO.COLLI_FATT is NULL) or armo_prezzi.id_armo > 0)
         and (meca.aperto <> 'N' or meca.aperto is null) 
group by
         armo.num_int,   
         armo.data_int,   
         meca.clie_1,   
         clienti_a.rag_soc_10 ,   
         armo.art,   
         prodotti.des_mkt,
         armo.colli_2,
         armo.colli_fatt,
         meca.num_bolla_in,
         meca.data_bolla_in,
	    armo.id_armo,
	    armo.id_meca

order by armo.data_int desc, armo.num_int desc

