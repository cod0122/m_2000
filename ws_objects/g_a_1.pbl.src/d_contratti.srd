 retrieve="  SELECT
         contratti.mc_co,
         contratti.codice,
         contratti.sc_cf,         
         contratti.sl_pt,         
         contratti.data,   
         contratti.data_scad,   
         contratti.cod_cli, 
         clienti.rag_soc_10,        
         trim(contratti.descr)   descr,   
         sl_pt.descr,
         sc_cf.descr,
         contratti.cert_st_dati_bolla_in,
         contratti.et_bcode_st_dt_rif,
         trim(contratti.et_bcode_note) et_bcode_note,
         contratti.flag_bolla_in_dett,
         contratti.id_meca_causale,
         contratti.x_datins,
         contratti.x_utente,
		meca_causali.descrizione  
         ,contratti.tipo
         ,contratti.flg_deperibile
         ,contratti.contratto_co_data_ins
         ,contratti.id_contratto_co
         ,contratti.id_contratto_dp
         ,contratti.id_contratto_rd
         ,contratti.et_dosimetro
         ,sc_cf.data
         ,sc_cf.data_scad
    FROM ((contratti left outer join clienti on
         contratti.cod_cli = clienti.codice)
         left outer join sl_pt on 
         contratti.sl_pt = sl_pt.cod_sl_pt)
         left outer join sc_cf on 
         contratti.sc_cf = sc_cf.codice
         left outer join meca_causali on 
         contratti.id_meca_causale = meca_causali.id_meca_causale
    where contratti.codice = :k_codice
" update="contratti" updatewhere=0 updatekeyinplace=no arguments=(("k_codice", number)) )
