 retrieve="  SELECT armo.id_armo,   
         armo.id_listino,   
         armo.art,   
         prodotti.des,   
         armo.colli_2,   
         armo.pedane,   
         armo.campione,   
         armo.dose,   
         armo.cod_sl_pt,   
         armo.larg_2,   
         armo.lung_2,   
         armo.alt_2,   
         armo.magazzino,   
         trim(armo.note_1) + ' ' + trim(armo.note_2) + ' ' + trim(armo.note_3) as note
		,COALESCE(listino.contratto,0) as contratto
		,COALESCE(listino.e1litm,'') as e1litm
    FROM 
        armo 
              LEFT OUTER JOIN prodotti 
              ON armo.art = prodotti.codice
              LEFT OUTER JOIN listino 
              ON armo.id_listino = listino.id
   WHERE id_meca = :k_id   
ORDER BY armo.id_armo ASC   
" arguments=(("k_id", number)) )
