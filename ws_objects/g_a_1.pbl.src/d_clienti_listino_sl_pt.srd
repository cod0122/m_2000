 retrieve="  SELECT  
         trim(clienti.rag_soc_10) as rag_soc_10, 
         listino.cod_cli, 
         listino.cod_art,   
		listino.id,
         trim(prodotti.des_mkt) as prodotti_des,   
         prodotti.gruppo,
         listino.contratto,   
         contratti.sl_pt,   
         listino.dose,   
         listino.magazzino,   
         listino.mis_x,   
         listino.mis_y,   
         listino.mis_z,   
         listino.occup_ped,   
         listino.peso_kg,   
         listino.campione,   
         listino.tipo,   
         listino.prezzo,  
         listino.attivo,
         contratti.mc_co,
         contratti.sc_cf,
         contratti.data_scad,
         0   as k_pwd,
         listino.x_datins,
		gru.des,
         listino.prezzo_2,  
         listino.prezzo_3  
    FROM (((listino  
                            inner JOIN clienti ON  listino.cod_cli = clienti.codice)  
                            LEFT OUTER JOIN contratti ON listino.contratto = contratti.codice)  
						 INNER JOIN prodotti ON listino.cod_art = prodotti.codice)  
  		                   inner join gru on prodotti.gruppo = gru.codice
	 where (:k_sl_pt = '*' or contratti.sl_pt = :k_sl_pt) and
               (:k_id_cliente = 0 or listino.cod_cli = :k_id_cliente) and
	 	      (:k_contratto = 0 or listino.contratto = :k_contratto) and 
			 (contratti.data_scad >= :k_data_scad or contratti.data_scad is null) 
    order by   clienti.rag_soc_10, listino.attivo desc, listino.cod_art 
 
" arguments=(("k_id_cliente", number),("k_data_scad", date),("k_sl_pt", string),("k_contratto", number)) )
