 column=(type=long updatewhereclause=yes name=meca_num_int dbname="meca.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=meca_data_int dbname="meca.data_int" dbalias=".data_int" )
 column=(type=datetime update=yes updatewhereclause=yes name=x_datins dbname="meca.x_datins" dbalias=".x_datins" )
 column=(type=char(12) update=yes updatewhereclause=yes name=x_utente dbname="meca.x_utente" dbalias=".x_utente" )
 column=(type=decimal(0) updatewhereclause=yes name=v_arfa_x_id_armo_colli dbname="v_arfa_x_id_armo.colli" dbalias="v_arfa.colli" )
 column=(type=decimal(0) updatewhereclause=yes name=armo_colli_2 dbname="colli_2" )
 retrieve="  SELECT  	meca.aperto,
				meca.id,   
				meca.num_int,   
				meca.data_int
				,meca.x_datins
				,meca.x_utente
                  ,v_arfa.colli
				, sum(armo.colli_2) as colli_2
		 FROM meca inner join armo on   meca.id = armo.id_meca  
                           inner join v_arfa_x_id_armo v_arfa on  armo.id_armo = v_arfa.id_armo    
		WHERE (:k_id_meca = 0 or meca.id = :k_id_meca) 
  and meca.data_int < :k_data_int
  and (meca.aperto <> 'N' or meca.aperto is null) and armo.magazzino <> 1    
	 group by 	meca.aperto,
				meca.id,   
				meca.num_int,   
				meca.data_int
				,meca.x_datins
				,meca.x_utente
                  ,v_arfa.colli
	having sum(armo.colli_2) <= v_arfa.colli ;
" arguments=(("k_id_meca", number),("k_data_int", date)) )
