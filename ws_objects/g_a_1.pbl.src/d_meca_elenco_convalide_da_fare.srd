 retrieve="    SELECT distinct
         meca.data_int,   
         meca.num_int,   
         meca.contratto,   
         meca.clie_1,   
         meca.clie_3,   
         clienti_b.rag_soc_10,   
         meca.num_bolla_in,   
         meca.data_bolla_in,
         meca.err_lav_ok,   
         meca.x_datins,   
         meca.x_utente,
         meca.id,
         armo.dose,
         armo.colli_2,
         sum(colli_trattati) as colli_trattati,
         sum(colli_groupage) as colli_groupage 
    FROM  (((meca 
			 INNER JOIN armo 
          ON meca.id = armo.id_meca) 
			 INNER JOIN artr 
          ON armo.id_armo = artr.id_armo) 
          LEFT OUTER JOIN clienti as clienti_b
          ON meca.clie_3 = clienti_b.codice) 
   WHERE  meca.data_int >= :k_data_int 
	      and (meca.err_lav_ok is null or meca.err_lav_ok = ' ')
         and artr.data_fin > CONVERT(date,'01.01.1900')   
         and meca.aperto <> 'N' 
group by
         meca.data_int,   
         meca.num_int,   
         meca.contratto,   
         meca.clie_1,   
         meca.clie_3,   
         clienti_b.rag_soc_10,   
         meca.num_bolla_in,   
         meca.data_bolla_in,
         meca.err_lav_ok,   
         meca.x_datins,   
         meca.x_utente,
         meca.id,
         armo.dose,
         armo.colli_2
          
ORDER BY meca.data_int DESC,   
         meca.num_int DESC   
  
" arguments=(("k_data_int", date)) )
