table(column=(type=char(1) update=yes updatewhereclause=yes name=meca_aperto dbname="meca.aperto" dbalias=".aperto" )
 column=(type=long updatewhereclause=yes identity=yes name=meca_id dbname="meca.id" dbalias=".id" )
 column=(type=long updatewhereclause=yes name=meca_num_int dbname="meca.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=meca_data_int dbname="meca.data_int" dbalias=".data_int" )
 column=(type=long updatewhereclause=yes identity=yes name=id_armo dbname="armo.id_armo" dbalias=".id_armo" )
 column=(type=decimal(0) updatewhereclause=yes name=colli_entrati dbname="armo.colli_entrati" dbalias=".colli_entrati" )
 retrieve="  SELECT  	meca.aperto,
				meca.id,   
				meca.num_int,   
				meca.data_int
				, armo.id_armo
				, armo.colli_2 as colli_entrati
		 FROM meca inner join armo on   meca.id = armo.id_meca  
						inner join armo_prezzi  on  armo.id_armo = armo_prezzi.id_armo   
		WHERE (:k_id_armo = 0 or armo.id_meca = :k_id_armo) 
                    and meca.data_int < :k_data_meca 
  and (meca.aperto <> 'N' or meca.aperto is null) and armo.magazzino <> 1    
  and armo_prezzi.tipo_calcolo = 'M' and armo_prezzi.stato in ('4', '8') 
  and (armo_prezzi.nrmese is null or armo_prezzi.nrmese = 0 or armo_prezzi.nrmese = :k_mese)
	 group by meca.aperto,
				meca.id,   
				meca.num_int,   
				meca.data_int
				, armo.id_armo
				, armo.colli_2
;" arguments=(("k_id_armo", number),("k_data_meca", date),("k_mese", number))  sort="meca_id A " )
