 retrieve="  SELECT 	meca.aperto,
				meca.id,   
				meca.num_int,   
				meca.data_int
				, sum(armo.colli_2) as colli_2
				 , sum(arsp.colli) as colli_sped  
		 FROM meca inner join armo on   meca.id = armo.id_meca  
				         left outer join arsp on  armo.id_armo = arsp.id_armo   
		WHERE (:k_id_meca = 0 or meca.id = :k_id_meca) 
  and (meca.aperto <> 'N' or meca.aperto is null) and armo.magazzino <> 1    
	 group by 
	             meca.aperto,
				meca.id,   
				meca.num_int,   
				meca.data_int
	having sum(armo.colli_2) > sum(arsp.colli) or sum(arsp.colli) is null;
" update="meca" updatewhere=0 updatekeyinplace=no arguments=(("k_id_meca", number))  sort="meca_id A " )
