 retrieve="  SELECT 
         meca.id ,
         meca_dosim.dosim_data,   
         meca_dosim.dosim_lotto_dosim,   
         meca_dosim.barcode_dosimetro,   
         meca_dosim.barcode,   
         meca.num_int,   
         meca.data_int,   
         meca_dosim.dosim_assorb,   
         meca_dosim.dosim_spessore,   
         meca_dosim.dosim_rapp_a_s,   
         meca_dosim.dosim_dose,   
         meca.err_lav_ok,   
         meca.note_lav_ok,   
         (meca_dosim.dosim_temperatura) as dosim_temperatura,   
         (meca_dosim.dosim_umidita) as dosim_umidita,   
         (meca_dosim.x_datins) as x_datins,   
         (meca_dosim.x_utente) as x_utente,
         (meca_dosim.x_data_dosim_verifica) as x_data_dosim_verifica,   
         (meca_dosim.x_utente_dosim_verifica) as x_utente_dosim_verifica,
         (meca_dosim.x_data_dosim_sblocco_ko) as x_data_dosim_sblocco_ko,   
         (meca_dosim.x_utente_dosim_sblocco_ko) as x_utente_dosim_sblocco_ko,
         'blocco_rapp'  as blocca_rapporto,
         ' '  as stato_lotto,
         ' '  as stato_lotto_flag,
		   meca.contratto,
			contratti.sc_cf
        ,max(meca_memo.id_meca_memo) as id_meca_memo
    FROM meca inner join meca_dosim on
	      meca.id = meca_dosim.id_meca
              inner join contratti on
         meca.contratto = contratti.codice
              left outer join meca_memo on 
         meca.id = meca_memo.id_meca 
	where meca_dosim.barcode = :k_barcode
   group by
         meca.id ,
         meca_dosim.dosim_data,   
         meca_dosim.dosim_lotto_dosim,   
         meca_dosim.barcode_dosimetro,   
         meca_dosim.barcode,   
         meca.num_int,   
         meca.data_int,   
         meca_dosim.dosim_assorb,   
         meca_dosim.dosim_spessore,   
         meca_dosim.dosim_rapp_a_s,   
         meca_dosim.dosim_dose,   
         meca_dosim.dosim_temperatura,   
         meca_dosim.dosim_umidita,   
         meca_dosim.x_datins,   
         meca_dosim.x_utente,
         meca_dosim.x_data_dosim_verifica,   
         meca_dosim.x_utente_dosim_verifica,
         meca_dosim.x_data_dosim_sblocco_ko,   
         meca_dosim.x_utente_dosim_sblocco_ko,
         meca.err_lav_ok,   
         meca.note_lav_ok,   
        meca.contratto,
		contratti.sc_cf
" arguments=(("k_barcode", string)) )
