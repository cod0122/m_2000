 column=(type=char(30) updatewhereclause=yes name=c1_rag_soc_10 dbname="clienti.rag_soc_10" )
 column=(type=long updatewhereclause=yes name=clie_2 dbname="meca.clie_2" )
 column=(type=char(30) updatewhereclause=yes name=c2_rag_soc_10 dbname="clienti.rag_soc_10" )
 column=(type=long updatewhereclause=yes name=clie_3 dbname="arfa.clie_3" )
 column=(type=long updatewhereclause=yes identity=yes name=id_arfa dbname="arfa.id_arfa" )
 column=(type=long updatewhereclause=yes name=id_fattura dbname="arfa.id_fattura" )
 column=(type=long updatewhereclause=yes name=num_fatt dbname="arfa.num_fatt" )
 column=(type=date updatewhereclause=yes name=data_fatt dbname="arfa.data_fatt" )
 column=(type=char(1) updatewhereclause=yes name=tipo_riga dbname="arfa.tipo_riga" )
 retrieve="select 
		armo.NUM_INT,
		armo.DATA_INT,
		armo.id_meca,
		armo.id_armo,
		armo.id_listino,
		armo.campione,
		armo.art,
		prodotti.des,
		armo.dose,
		armo.colli_2,  
		arfa.colli as colli_fatt,
		MECA.CLIE_1,
		c1.rag_soc_10,
		MECA.CLIE_2,
		c2.rag_soc_10,
		arfa.CLIE_3,
		arfa.id_arfa,
		arfa.id_fattura,
		arfa.num_fatt,
		arfa.data_fatt,
		arfa.tipo_riga
from arfa
				inner join armo on 
		arfa.id_armo = armo.id_armo
				inner join meca on 
		armo.id_meca = meca.id 
				inner join CLIENTI as c1 on 
		MECA.CLIE_1  = c1.CODICE 
				inner join CLIENTI as c2 on 
		MECA.CLIE_2  = c2.CODICE 
				inner join prodotti on 
		armo.art  = prodotti.CODICE 
where
	arfa.data_fatt >= :k_data
	and arfa.tipo_doc = 'FT'
	and (arfa.clie_3 = :k_clie_3 or arfa.clie_3 = 0 )
union all
select 
		0,
		convert(date, '01.01.1899'),
		0,
		0,
		0,
		'N',
		arfa.art,
		arfa.comm,
		0,
		0,  
		COALESCE(arfa.colli,0) as colli_fatt,
		0,
		' ',
		0,
		' ',
		arfa.CLIE_3,
		arfa.id_arfa_v,
		arfa.id_fattura,
		arfa.num_fatt,
		arfa.data_fatt,
		'V'
from arfa_v as arfa
				left outer join prodotti on 
		arfa.art  = prodotti.CODICE 
where
	arfa.data_fatt >= :k_data
	and arfa.tipo_doc = 'FT'
	and (arfa.clie_3 = :k_clie_3 or arfa.clie_3 = 0 )

order by 2, 1
; 
