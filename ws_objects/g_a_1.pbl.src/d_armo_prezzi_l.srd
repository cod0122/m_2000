 retrieve="  SELECT 
         armo_prezzi.id_armo,   
         armo_prezzi.id_armo_prezzo,   
         listino_voci.id_listino_voce,   
         listino_voci.descr,   
         listino_voci.attivo,   
         listino_voci.id_listino_voci_categ,   
         armo_prezzi.tipo_listino,   
         armo_prezzi.tipo_calcolo,   
         armo_prezzi.prezzo,   
         armo_prezzi.item_dafatt,   
         armo_prezzi.item_fatt,   
         armo_prezzi.item_nofatt,   
         armo_prezzi.id_listino_link_pregruppo,
		armo_prezzi.stato  
      ,armo.num_int
      ,armo.data_int
    FROM armo_prezzi left outer join listino_voci on  
                     armo_prezzi.id_listino_voce = listino_voci.id_listino_voce
                     inner join armo on  
                     armo_prezzi.id_armo = armo.id_armo
   WHERE 
         ( armo_prezzi.id_armo = :k_id_armo 
             and (:k_stato = '*' or armo_prezzi.stato = :k_stato) )   
ORDER BY armo_prezzi.id_armo_prezzo ASC   
" arguments=(("k_id_armo", number),("k_stato", string)) )
