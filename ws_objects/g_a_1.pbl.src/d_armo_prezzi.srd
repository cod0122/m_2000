 retrieve="  SELECT armo_prezzi.id_armo,   
         armo_prezzi.id_armo_prezzo,   
         armo_prezzi.id_listino_voce,   
         listino_voci.descr,   
         armo_prezzi.id_listino_link_pregruppo,   
         listino_pregruppo.descr,   
         armo_prezzi.prezzo,   
         armo_prezzi.item_dafatt,   
         armo_prezzi.item_fatt,   
         armo_prezzi.item_nofatt,   
         armo_prezzi.tipo_listino,   
         armo_prezzi.tipo_calcolo,   
         armo_prezzi.stato,   
         armo_prezzi.x_datins,   
         armo_prezzi.x_utente,   
         listino_pregruppo.id_listino_pregruppo,   
         listino_voci.um,   
         listino_voci.descr_xctr,   
         listino_voci.descr_xctr_eng  
    FROM armo_prezzi inner join listino_voci on  armo_prezzi.id_listino_voce = listino_voci.id_listino_voce
                                inner join listino_link_pregruppi on  armo_prezzi.id_listino_link_pregruppo = listino_link_pregruppi.id_listino_link_pregruppo
                                inner join listino_pregruppo on listino_pregruppo.id_listino_pregruppo = listino_link_pregruppi.id_listino_pregruppo
           
   WHERE 
       armo_prezzi.id_armo_prezzo = :k_id_armo_prezzo 
" update="armo_prezzi" updatewhere=0 updatekeyinplace=no arguments=(("k_id_armo_prezzo", number)) )
