 retrieve=" SELECT  
         'barcode',  
         meca.consegna_data,   
         armo.magazzino,   
         armo.num_int,   
         armo.data_int,   
         meca.clie_1,   
         (select clienti.rag_soc_10 from clienti   where  meca.clie_1 = clienti.codice) as rag_soc_1,   
         meca.clie_2,   
         (select clienti.rag_soc_10 from clienti   where  meca.clie_2 = clienti.codice) as rag_soc_2,   
         meca.clie_3,   
         (select clienti.rag_soc_10 from clienti   where  meca.clie_3 = clienti.codice) as rag_soc_3,   
         armo.art,   
         prodotti.des_mkt as des,
         armo.dose,
         armo.colli_2,
		armo.pedane,
         armo.m_cubi,
         armo.peso_kg,
         artr.colli,
         artr.colli_trattati,
         artr.num_certif,
         artr.data_in,
         artr.data_fin,
         meca_dosim.dosim_data,
         meca.num_bolla_in,
         meca.data_bolla_in,
         meca.contratto,
         artr.data_st,
         artr.colli_groupage,
		artr.pl_barcode ,
	     armo.id_armo,
	     armo.id_meca,
         sum(barcode.fila_1 + barcode.fila_2 + barcode.fila_1p + barcode.fila_2p) as giri,
         sum(barcode.lav_fila_1 + barcode.lav_fila_2 + barcode.lav_fila_1p + barcode.lav_fila_2p) as giri_lav
    FROM 
         armo  LEFT OUTER JOIN barcode 
         ON armo.id_armo = barcode.id_armo 
					left outer JOIN artr 
         ON barcode.id_armo = artr.id_armo and barcode.pl_barcode = artr.pl_barcode
               LEFT OUTER JOIN prodotti 
         ON armo.art = prodotti.codice
               INNER JOIN meca
         ON armo.id_meca = meca.id
               LEFT OUTER JOIN meca_dosim 
         ON meca.id = meca_dosim.id_meca
   WHERE 
        armo.data_int between :k_data_int_i and :k_data_int_f 
             
group by
         meca.consegna_data,   
         armo.magazzino,   
         armo.num_int,   
         armo.data_int,   
         meca.clie_1,   
         rag_soc_1,   
         meca.clie_2,   
         rag_soc_2,   
         meca.clie_3,   
         rag_soc_3,   
         armo.art,   
         prodotti.des_mkt,
         armo.dose,
         armo.colli_2,
		armo.pedane,
         armo.m_cubi,
         armo.peso_kg,
         artr.colli,
         artr.colli_trattati,
         artr.num_certif,
         artr.data_in,
         artr.data_fin,
         meca_dosim.dosim_data,
         meca.num_bolla_in,
         meca.data_bolla_in,
         meca.contratto,
         artr.data_st,
         artr.colli_groupage,
		artr.pl_barcode,
	    armo.id_armo,
		armo.id_meca

order by armo.data_int desc, armo.num_int desc
