 retrieve="  SELECT email_invio.id_email_invio,   
         email_invio.id_cliente,   
         clienti.rag_soc_10,   
         email_invio.email,   
         email_invio.data_ins,   
         email_invio.data_inviato,   
         email_invio.ora_inviato,   
         email_invio.note,   
         email_invio.oggetto,   
         email_invio.link_lettera,   
         email_invio.allegati_cartella
        ,email_invio.cod_funzione
    FROM email_invio left outer join  clienti on email_invio.id_cliente = clienti.codice
	where email_invio.id_email_invio > :k_id_mail_invio
           and email_invio.cod_funzione in (:k_cod_funzione_1, :k_cod_funzione_2)
" arguments=(("k_id_mail_invio", number),("k_cod_funzione_1", string),("k_cod_funzione_2", string))  sort="data_ins D " )
