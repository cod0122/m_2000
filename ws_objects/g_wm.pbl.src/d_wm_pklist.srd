 retrieve="  SELECT wm_pklist.id_wm_pklist,   
         trim(wm_pklist.idpkl) as idpkl,   
         wm_pklist.eliminato,   
         wm_pklist.stato,   
         wm_pklist.tpimportazione,   
         wm_pklist.dtimportazione,   
         wm_pklist.idimportazione,   
         wm_pklist.clie_1,   
         wm_pklist.clie_2,   
         wm_pklist.clie_3,   
         trim(wm_pklist.nrord) as nrord,   
         wm_pklist.dtord,   
         trim(wm_pklist.nrddt) as nrddt,   
         wm_pklist.dtddt,   
         trim(wm_pklist.mc_co) as mc_co,   
         trim(wm_pklist.sc_cf) as sc_cf,   
         wm_pklist.colliddt,   
         wm_pklist.collipkl,   
         wm_pklist.note,   
         wm_pklist.x_datins_elim,   
         wm_pklist.x_utente_elim,   
         wm_pklist.x_datins,   
         wm_pklist.x_utente,   
         clienti_a.rag_soc_10,   
         clienti_b.rag_soc_10,   
         clienti_c.rag_soc_10,
         trim(wm_pklist.note_lotto) as note_lotto
         ,'' as k_e1litm_ok
         ,wm_pklist.packinglistcode as packinglistcode
         ,wm_pklist.id_wm_pklist_padre as id_wm_pklist_padre
         ,0 as id_contratto
         ,COALESCE(wm_pklist.customerlot,'') as customerlot
    FROM wm_pklist LEFT OUTER JOIN clienti clienti_a ON wm_pklist.clie_1 = clienti_a.codice 
                            LEFT OUTER JOIN clienti clienti_b ON wm_pklist.clie_2 = clienti_b.codice 
                            LEFT OUTER JOIN clienti clienti_c ON wm_pklist.clie_3 = clienti_c.codice  
   WHERE wm_pklist.id_wm_pklist = :kid_wm_pklist    
" update="wm_pklist" updatewhere=0 updatekeyinplace=no arguments=(("kid_wm_pklist", number)) )
