 retrieve="  SELECT wm_pklist_righe.id_wm_pklist_riga,   
         wm_pklist_righe.stato,   
         trim(wm_pklist_righe.idart_clie) as idart_clie,   
         trim(wm_pklist_righe.idlotto_clie) as idlotto_clie,   
         wm_pklist_righe.qtapezzi_pallet,   
         wm_pklist_righe.idpezzo_clie,   
         trim(wm_pklist_righe.areamag) as areamag,   
         wm_pklist_righe.gruppo,   
         wm_pklist_righe.colli,   
         trim(wm_pklist_righe.wm_barcode) as wm_barcode,   
         trim(wm_pklist_righe.barcode) as barcode,   
         wm_pklist_righe.id_meca,   
         wm_pklist_righe.id_armo,   
         wm_pklist_righe.eliminato,   
         wm_pklist_righe.x_datins_elim,   
         wm_pklist_righe.x_utente_elim,   
         wm_pklist_righe.x_datins,   
         wm_pklist_righe.x_utente,   
         wm_pklist_righe.id_wm_pklist,   
         trim(wm_pklist.idpkl ) as idpkl 
		,0 as k_riga
    FROM wm_pklist_righe,   
         wm_pklist  
   WHERE ( wm_pklist_righe.id_wm_pklist = wm_pklist.id_wm_pklist ) and  
         ( ( wm_pklist_righe.id_wm_pklist_riga = :id_wm_pklist_riga ) )    
