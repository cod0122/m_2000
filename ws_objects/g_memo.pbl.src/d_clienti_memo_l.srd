         clienti_memo.id_cliente,   
         clienti_memo.tipo_sv,   
         clienti.rag_soc_10,   
         memo.titolo,   
         memo.note,   
         clienti_memo.id_memo,   
         memo.tipo_memo,
         memo.dataora_ins,
         memo.utente_ins,
         memo.x_datins,
         memo.x_utente 
