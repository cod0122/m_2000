table(column=(type=long update=yes updatewhereclause=yes identity=yes name=id_memo dbname="memo.id_memo" dbalias=".id_memo" )
 column=(type=datetime update=yes updatewhereclause=yes name=dataora_ins dbname="memo.dataora_ins" dbalias=".dataora_ins" )
 column=(type=char(12) update=yes updatewhereclause=yes name=utente_ins dbname="utente_ins" )
 column=(type=datetime updatewhereclause=yes name=data_ins dbname="data_ins" )
 column=(type=char(40) update=yes updatewhereclause=yes name=titolo dbname="memo.titolo" dbalias=".titolo" )
 column=(type=char(120) update=yes updatewhereclause=yes name=note dbname="memo.note" dbalias=".note" )
 column=(type=char(5) updatewhereclause=yes name=tipo_memo dbname="tipo_memo" )
 column=(type=char(3) update=yes updatewhereclause=yes name=tipo_sv dbname="tipo_sv" )
 column=(type=datetime update=yes updatewhereclause=yes name=x_datins dbname="memo.x_datins" dbalias=".x_datins" )
 column=(type=char(12) update=yes updatewhereclause=yes name=x_utente dbname="x_utente" )
 column=(type=long updatewhereclause=yes name=id_cliente dbname="clienti_memo.id_cliente" dbalias=".id_cliente" )
 column=(type=long updatewhereclause=yes name=id_meca dbname="meca_memo.id_meca" dbalias=".id_meca" )
 column=(type=long updatewhereclause=yes name=num_int dbname="meca.num_int" dbalias=".num_int" )
 column=(type=date updatewhereclause=yes name=data_int dbname="meca.data_int" dbalias=".data_int" )
 column=(type=char(61) updatewhereclause=yes name=rag_soc_10 dbname="rag_soc_10" )
 column=(type=long updatewhereclause=yes name=stato dbname="stato" values="da Leggere	0/Letto	2/Rimosso	8/" )
 column=(type=long updatewhereclause=yes name=memo_permessi dbname="memo.permessi" dbalias=".permessi" values="nessuno	0/solo lettura	2/modificabile	4/completo	9/" )
 column=(type=long updatewhereclause=yes name=id_memo_utente dbname="id_memo_utente" )
 retrieve="  SELECT distinct memo.id_memo,    
 memo.dataora_ins,   
 trim(memo.utente_ins) as utente_ins,   
 (memo.dataora_ins) as data_ins,   
 memo.titolo,   
 memo.note,   
 trim(memo.tipo_memo) as tipo_memo,   
 trim(memo.tipo_sv) as tipo_sv,   
 memo.x_datins, 
 trim(memo.x_utente) as x_utente, clienti_memo.id_cliente, 
 meca_memo.id_meca,  meca.num_int,
 meca.data_int, COALESCE(trim(clienti.rag_soc_10),'') + ' ' + COALESCE(clienti.rag_soc_11, '') as rag_soc_10, 
 COALESCE(memo_utenti.stato, 0) as stato ,  
 memo.permessi , 
 COALESCE(memo_utenti.id_memo_utente, 0) as id_memo_utente 
 FROM memo left outer join memo_utenti on memo.id_memo = memo_utenti.id_memo   
            left outer join clienti_memo on memo.id_memo = clienti_memo.id_memo   
		     left outer join clienti on clienti_memo.id_cliente = clienti.codice 
            left outer join meca_memo on memo.id_memo = meca_memo.id_memo   
			 left outer join meca on meca_memo.id_meca = meca.id 
WHERE 
      (:k_utente_ins = '*' or :k_utente_ins = 'MASTER' or memo.utente_ins = :k_utente_ins or memo.x_utente = :k_utente_ins)  
   and convert(date, memo.x_datins) between convert(date, :k_x_datins) and convert(date, :k_x_datins_fin) 
   and memo_utenti.stato between :k_stato_da and :k_stato_a 
   and (:k_id_utente = 9999 or memo_utenti.id_sr_utente = :k_id_utente) 
   and (:k_id = 0 or (meca_memo.id_meca = :k_id or clienti_memo.id_cliente = :k_id) or (:k_id = 888888 and meca_memo.id_meca > 0) or (:k_id = 999999 and clienti_memo.id_cliente > 0)) 
 order by memo.x_datins " arguments=(("k_id", number),("k_stato_da", number),("k_stato_a", number),("k_utente_ins", string),("k_id_utente", number),("k_x_datins", datetime),("k_x_datins_fin", datetime)) )
