 retrieve="  SELECT arsp.id_sped,   
         memo.titolo,   
         memo.note,   
         memo.id_memo,   
         memo.tipo_memo,
         memo.dataora_ins,
         memo.utente_ins,
         memo.x_datins,
         memo.x_utente
       , arsp.num_bolla_out
      , arsp.data_bolla_out
    FROM arsp inner join armo on arsp.id_armo  = armo.id_armo
	     inner join meca on armo.id_meca = meca.id
		left outer join clienti_memo on (meca.clie_1 = clienti_memo.id_cliente 
                                               or meca.clie_2 = clienti_memo.id_cliente 
                                               or meca.clie_3 = clienti_memo.id_cliente) and clienti_memo.allarme in ('S', 'T')  
		left outer join meca_memo on armo.id_meca = meca_memo.id_meca and meca_memo.allarme in ('S', 'T')  
         left outer join memo on (clienti_memo.id_memo = memo.id_memo or meca_memo.id_memo = memo.id_memo)
   WHERE arsp.id_sped = :k_id_sped
" arguments=(("k_id_sped", number))  sort="x_datins D " )
