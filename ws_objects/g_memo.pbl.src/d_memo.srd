 column=(type=char(1) updatewhereclause=yes name=meca_memo_allarme dbname="meca_memo.allarme" values="nessuno	N/carico lotto	I/d.d.t.	S/fatturazione	F/d.d.t e fatt.	T/" )
 column=(type=long updatewhereclause=yes name=num_int dbname="meca.num_int" )
