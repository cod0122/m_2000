 column=(type=long updatewhereclause=yes name=clie_3 dbname="meca.clie_3" )
 column=(type=char(61) updatewhereclause=yes name=meca_rag_soc_10 dbname="meca_rag_soc_10" )
 column=(type=char(12) updatewhereclause=yes name=cod_sl_pt dbname="sl_pt_memo.cod_sl_pt" )
 column=(type=char(50) updatewhereclause=yes name=sl_pt_descr dbname="sl_pt_descr" )
 column=(type=long updatewhereclause=yes name=k_fascicola_pt dbname="k_fascicola_pt" values="	1/	0" )
 retrieve="  SELECT memo.id_memo,   
         memo.tipo_sv,   
         sr_settori.descr,   
         trim(memo.titolo) as titolo,   
         trim(memo.note) as note,   
         memo.permessi,
         memo.tipo_memo,
         memo.dataora_ins,
         memo.utente_ins,
         memo.x_datins,
         memo.x_utente,
          clienti_memo.id_cliente,
          clienti_memo.allarme,
          trim(clienti.rag_soc_10) + ' ' + trim(clienti.rag_soc_11) as rag_soc_10,
         meca_memo.id_meca,
         meca_memo.allarme,
         meca.num_int,
         meca.data_int,
         meca.clie_3,
         trim(c1.rag_soc_10) + ' ' + trim(c1.rag_soc_11) as meca_rag_soc_10
         ,sl_pt_memo.cod_sl_pt
         ,COALESCE((select sl_pt.descr from sl_pt where sl_pt.cod_sl_pt = sl_pt_memo.cod_sl_pt), '') as sl_pt_descr
			,CASE
         	WHEN trim(sl_pt_memo.cod_sl_pt) > ' ' THEN 1
         	ELSE 0
       	END as k_fascicola_pt
    FROM memo left outer join clienti_memo on
                  memo.id_memo = clienti_memo.id_memo
                       left outer join sr_settori on 
                  memo.tipo_sv = sr_settori.sr_settore
                       left outer join clienti on
                  clienti_memo.id_cliente = clienti.codice
					 left outer join meca_memo on
                  memo.id_memo = meca_memo.id_memo
                       left outer join meca on
                  meca_memo.id_meca = meca.id
                       left outer join clienti as c1 on
                  meca.clie_3 = c1.codice
					 left outer join sl_pt_memo on
                  memo.id_memo = sl_pt_memo.id_memo
   WHERE memo.id_memo = :k_id_memo 
" update="memo" updatewhere=0 updatekeyinplace=no arguments=(("k_id_memo", number)) )
